<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsl="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified">
	<xs:include schemaLocation="OTA_SimpleTypes.xsd"/>
	<xs:simpleType name="AgentPseudoCityType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Pseudo City Code.
Example: AAA2</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="16"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AggregatorRoleType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Aggregator Role enumerated list.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list.
=enumerated values=
A = AGGREGATOR COMMUNICATING WITH AIRLINE
R = AGGREGATOR COMMUNICATING WITH REQUESTOR</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation>A = AGGREGATOR COMMUNICATING WITH AIRLINE</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R">
				<xs:annotation>
					<xs:documentation>R = AGGREGATOR COMMUNICATING WITH REQUESTOR</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AirlineCodeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Airline designator code.
Example: UA

For maximum interoperability, it is recommended that IATA/ATA defined 2-3 character designator encoding be used.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="2"/>
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AirportCityCodeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Airport or city code.
Examples: JFK, NYC</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="IATAAirportCityCodeType"/>
	</xs:simpleType>
	<xs:simpleType name="AirRowNbrType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Aircraft row number.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="01"/>
			<xs:maxInclusive value="99"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AmountCategoryType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Amount category.
Examples:
A (AMOUNT)
P (PERCENTAGE)</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list.
=enumerated values=
A = AMOUNT
P = PERCENTAGE</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Commission is an amount.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Comission is a percentage.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AvailSeatStatusType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Available seats status type.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">Seat status.
Example: SEATS ON REQUEST ONLY TO AIRLINE

For maximum interoperability, Refer to IATA PADIS Code List for data element 9865 - Seat Occupation.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="BagAllowType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Baggage Allowance Type enumerated list.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a BAGGAGE WEIGHT LIMIT string value.
=enumerated values=
PC = PER PIECE
1PC = 1 PEICE
2PC = 2 PIECES</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="PC">
				<xs:annotation>
					<xs:documentation>PC = PER PIECE</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1PC">
				<xs:annotation>
					<xs:documentation>1PC = 1 PEICE</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2PC">
				<xs:annotation>
					<xs:documentation>2PC = 2 PIECES</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BagOptChargeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Catalog of Charges Type enumerated list.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value that is bilaterally agreed upon between trading partners.
=enumerated values=
C = CATALOG ONLY
S = SERVICE CHARGE DETAILS</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation>C = CATALOG ONLY</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="S">
				<xs:annotation>
					<xs:documentation>S = SERVICE CHARGE DETAILS</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ChoiceListType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Predefined Choices enumerated list.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value that is bilaterally agreed upon between trading partners.
=enumerated values=
Y = YES
N = NO</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>N = NO</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Y">
				<xs:annotation>
					<xs:documentation>Y = YES</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ChoiceType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Choice type.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value.
=enumerated values=
Y = YES
N = NO</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="ChoiceListType"/>
	</xs:simpleType>
	<xs:simpleType name="ChoicePrefType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Predefined Preference enumerated list.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list.
=enumerated values=
N = NOT ACCEPTABLE  (only search results that do not include this preference should be returned)
P = PREFERRED (search results that do or do not satisfy this preference should be returned)
R = REQUIRED (only search results that satisfy this preference should be returned)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="P">
				<xs:annotation>
					<xs:documentation>P = PREFERENCE PREFERRED
Search results that do or do not satisfy this preference should be returned.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R">
				<xs:annotation>
					<xs:documentation>R = PREFERENCE REQUIRED
Only search results that satisfy this preference should be returned.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CityNameType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">City name.
Example: PARIS</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ClassOfServiceType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight class of service.
Example: B</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Z]{1}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CountryCodeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Two character country code.
Example: US</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Z]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CurrencyCodeDefType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">ISO 4217 3 alpha character currency code definition.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Z]{3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DataFormatType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Data Format enumerated list.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value that is bilaterally agreed upon between trading partners.
=enumerated values=
A = ALPHANUMERIC (DEFAULT)
D = ISO DATE FORMAT
N = NUMERIC</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation>A = ALPHANUMERIC (DEFAULT)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D">
				<xs:annotation>
					<xs:documentation>D = ISO DATE FORMAT</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>N = NUMERIC</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DateType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Date.
Example: 2013-12-25</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:date"/>
	</xs:simpleType>
	<xs:simpleType name="DisclosureCodeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">US Department of Transportation (DOT) Disclosure enumerated list.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value that is bilaterally agreed upon between trading partners.
=enumerated values=
N = BAGGAGE IS NOT SUBJECT TO US DOT RESERVATION OR DISCLOSURE
Y = BAGGAGE IS SUBJECT TO US DOT RESERVATION AND DISCLOSURE
D = BAGGAGE IS SUBJECT TO DISCLOSURE BUT NOT TO US DOT RESERVATION</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="D">
				<xs:annotation>
					<xs:documentation>D = BAGGAGE IS SUBJECT TO DISCLOSURE, BUT NOT TO US DOT RESERVATION</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>N = BAGGAGE IS NOT SUBJECT TO US DOT RESERVATION OR DISCLOSURE</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Y">
				<xs:annotation>
					<xs:documentation>Y = BAGGAGE IS SUBJECT TO BOTH US DOT DISCLOSURE AND RESERVATION</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DistanceUnitType">
