				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="PackageProductType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:group ref="SPNRCommonElements"/>
			<xs:element name="PaymentDetails" type="PaymentDetailsType">
				<xs:annotation>
					<xs:documentation>Holds all details relating to payment for this product</xs:documentation>
				</xs:annotation>
