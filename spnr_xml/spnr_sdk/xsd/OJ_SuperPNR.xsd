				<xs:restriction base="xs:string">
					<xs:enumeration value="pendingPayment"/>
					<xs:enumeration value="pendingTicket"/>
					<xs:enumeration value="ticketSuccess"/>
					<xs:enumeration value="cancelled"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="ProcessingStatus" type="ProcessingStatusType" use="optional">
			<xs:annotation>
				<xs:documentation>Used to indicate more detailed status</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SuperPNR_ID" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The unique id of this product (the Super PNR id)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="CommonProductAttributes"/>
		<xs:attribute name="TKCancelType" type="xs:string">
			<xs:annotation>
				<xs:documentation>This attribute indicates the ticket cancellation type, its value can be TicketVoid/TicketRefund</xs:documentation>
			</xs:annotation>
