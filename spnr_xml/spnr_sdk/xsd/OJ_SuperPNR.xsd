				<xs:annotation>
					<xs:documentation>Holds contact details about the user who made the booking</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BackendAudits" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BackendAudit" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="FileDate" type="xs:string"/>
								<xs:attribute name="AuditID" type="xs:string"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Attachments" minOccurs="0">
				<xs:annotation>
					<xs:documentation>To store information about any file attachments stored against the SuperPNR</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Attachment" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="AttachmentsType">
										<xs:sequence>
											<xs:element name="Agent" type="AgentDetailsType" minOccurs="0"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Errors" type="SPNR_ErrorsType" minOccurs="0"/>
			<xs:element name="CrossRefs" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Holds references to other SuperPNRs or booking references that may exist outside of the SuperPNR Database.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="UniqueID" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="UniqueID_Type">
										<xs:attribute name="SeqNo" type="xs:int">
											<xs:annotation>
												<xs:documentation>Number identifying position of crossRef item in DB</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="Historic">
											<xs:annotation>
												<xs:documentation>CrossRef is made Historic when deleted. It needs this attribute to delete.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AssociatedBookings" type="AssociatedBookingsType" minOccurs="0"/>
			<xs:element name="Emails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Email" type="SPNR_Email_Type" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
