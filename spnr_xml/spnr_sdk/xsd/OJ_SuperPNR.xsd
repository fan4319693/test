					<xs:attributeGroup ref="TSAncillaryProductGroup"/>
					<xs:attribute name="VoucherRequired" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Indicates if voucher issuance is required for this ancillary.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Tag" type="xs:string">
						<xs:annotation>
							<xs:documentation>Product tag.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Coupons" type="CouponsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Contains a booking request for a single coupon product. A coupon product can consist of one or more individual coupons.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Membership" type="MembershipReservationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Contains a booking request for a single membership product. A membership product can consist of one or more individual benefits.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ItineraryReceipt" minOccurs="0" type="ItineraryReceiptProductType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Contains a booking request for a single Itinerary Receipt product. This product is a receipt for an existing Air or other itinerary.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
		<xs:attribute name="PolicyStatus" type="xs:string">
			<xs:annotation>
				<xs:documentation>Attribute displaying the applied corporate filter.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="CommonProductAttributes"/>
		<xs:attribute name="BookedWith" type="ListOfRPH">
			<xs:annotation>
				<xs:documentation>ProductNumber(s) of associated Product(s). For example, a Hotel with multiple rooms - each room contains the ProductNumber(s) of associated rooms</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ReplacedBy" type="ListOfRPH">
			<xs:annotation>
				<xs:documentation>ProductNumber of the Product that replaces this product after a date change or upgrade.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RecoveredFrom" type="ListOfRPH">
			<xs:annotation>
				<xs:documentation>ProductNumber of the Product that this product is recoveried from after PNR recovery hanppened.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductType" type="ProductTypeType" use="required">
			<xs:annotation>
				<xs:documentation>The type of product eg air, car hotel etc</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductSubType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Optional sub-type of this product. Can be used by projects to further classify a product as a Shopping Basket component, Dummy PNR to track Ground Only SuperPNR, etc...</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BrochureCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Brochure code of product if any</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SaleModel"/>
		<xs:attribute name="PayOnArrival" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Indicates whether any outstanding payment on this product is to be paid on arrival</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="PrimaryLangID_Group"/>
		<xs:attributeGroup ref="ProductIDs"/>
		<xs:attribute name="Synchronised" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Used to indicate if the product was syncronised upon last read from DB</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ChangeAllowed">
			<xs:annotation>
				<xs:documentation>Used to identify whether a product should be allowed to be modified</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BookingChannel" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>This defines the supplier channel used for this component</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EchoToken">
			<xs:annotation>
				<xs:documentation>This is a transient field, used to store session tokens on a product that has been modifed via a Terminal Emulator.  Its value is not intended to be stored in the database</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="HistoryAttribute"/>
		<xs:attribute name="ManualSegment" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Indicates if this component is a passive segment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductTypeCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Product Type Code</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ServiceCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Product Type Code from NDC.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SearchID" type="StringLength1to128" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">An ID to uniquely identify a search made by the end user.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CheapestAlternative" type="xs:float"/>
		<xs:attributeGroup ref="ActionAttributes"/>
		<xs:attribute name="PostTravel" type="xs:boolean"/>
		<xs:attribute name="Refundable" type="xs:boolean"/>
		<xs:attribute name="SplitFrom" type="xs:integer">
			<xs:annotation>
				<xs:documentation>The product number of the original product after a split in order to keep track of history pre-split</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ModularProductType">
		<xs:complexContent>
			<xs:extension base="ProductBase">
				<xs:sequence>
					<xs:group ref="ProductCommonElements"/>
				</xs:sequence>
				<xs:attribute name="SupplierCode">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="4"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="BookAfterPayment" type="xs:boolean">
					<xs:annotation>
						<xs:documentation>To only book the modular product after a successful confirmation of payment for strict invectory control products.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ComboCode" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>This refers to OJ_ComboSearchRS/ComboResult/PricedItinerary/Combo/@Code</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="CreateCoupon" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">Indicates if modular product type is required to generate a new coupon. When 'true' a new coupon will be generated.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ID" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>The ID.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ProductID" type="xs:integer" use="optional">
					<xs:annotation>
						<xs:documentation>The customised product ID for each product at the PNR level.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="TSBookingStatus" type="ModTSBookingStatusType" use="optional">
					<xs:annotation>
						<xs:documentation>The current status of the product eg booked or cancelled</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="OJ_SuperPNR">
		<xs:annotation>
			<xs:documentation>Holds all the details of a super PNR containing multiple travel related products. The super PNR data structure can be used as a data structure for a shopping basket to add and reprice new products as well a record of a completed booking </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="SuperPNRType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="PackageProductType">
		<xs:sequence>
			<xs:element name="Products">
				<xs:annotation>
					<xs:documentation>The details and pricing of the component products of the package</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Product" type="ModularProductType" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:sequence>
				<xs:group ref="PackageCommonElements"/>
			</xs:sequence>
		</xs:sequence>
		<xs:attribute name="PolicyStatus" type="xs:string">
			<xs:annotation>
				<xs:documentation>Attribute displaying the result applied corporate filter.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="CommonProductAttributes"/>
		<xs:attribute name="PackageType" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The package code used to indicate what type of package this is sold with</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OpaquelyPriced" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>Indicates if the internal price breakdown of the package should be visible to the consumer (nearly always false)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SearchID" type="StringLength1to128" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">An ID to uniquely identify a search made by the end user.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="ProductIDs"/>
		<xs:attributeGroup ref="HistoryAttribute"/>
		<xs:attribute name="CheapestAlternative" type="xs:float"/>
		<xs:attribute name="IBSAmount" type="xs:float"/>
		<xs:attribute name="Action" type="ActionCodeType">
			<xs:annotation>
				<xs:documentation>Action name, which will be performed</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="isSwitchSell" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates whether the package was generated under a switch-sell request.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PaymentDetailsType">
		<xs:sequence>
			<xs:element name="Balance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The outstanding amount due to be paid on this product, may be negative if a refund has not yet been given.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Loyalty" type="LoyaltyPaymentType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The details of any redemption payments</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attributeGroup ref="CurrencyAmountGroup"/>
					<xs:attribute name="Quantity">
						<xs:annotation>
							<xs:documentation>Used to store the outstanding redemption miles balance of the SuperPNR</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Payments" type="PaymentsType">
				<xs:annotation>
					<xs:documentation>The history of all payments on this product</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="DueDate" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation>The due date of any outstanding balance</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CollectionMethod" use="optional">
			<xs:annotation>
				<xs:documentation>The method by which any payements should be collected</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="cheque"/>
					<xs:enumeration value="confirmcard"/>
					<xs:enumeration value="autocard"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="LastModified" type="xs:dateTime">
			<xs:annotation>
				<xs:documentation>The last time the payment details were modified</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TimeLimit" type="xs:dateTime" use="optional">
			<xs:annotation>
				<xs:documentation>The payment deadline time. This was added because 'DueDate' only specifies the date, and does not include the time.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:attributeGroup name="SPNR_PriceAdjustmentAttributes">
