		<xs:attribute name="Summary" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Indicates if this is a sparsely populated summary of a SuperPNR</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LastAuditID" type="xs:int">
			<xs:annotation>
				<xs:documentation>The latest AuditID</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LastFulfillmentAuditID" type="xs:int">
			<xs:annotation>
				<xs:documentation>The last AuditID successfully given to fulfillment</xs:documentation>
			</xs:annotation>
