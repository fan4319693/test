					<xs:attribute name="SellingProposition" type="xs:string">
						<xs:annotation>
							<xs:documentation>Selling Proposition</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="BundleCode" type="xs:string">
