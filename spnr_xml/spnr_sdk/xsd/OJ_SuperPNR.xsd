			</xs:element>
			<xs:element name="AmendmentFees" type="AmendmentFeesType" minOccurs="0"/>
			<xs:element name="CancellationRefunds" type="CancellationRefundsType" minOccurs="0"/>
			<xs:element name="TotalPrice">
				<xs:annotation>
					<xs:documentation>The total price of this product, including cancellation fees, adjustments and amendment fees </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ComponentTotal" type="CurrencyAmountType" minOccurs="0"/>
						<xs:element name="AmendmentFees" type="CurrencyAmountType" minOccurs="0"/>
						<xs:element name="ExcludedAdjustments" type="CurrencyAmountType" minOccurs="0"/>
						<xs:element name="Loyalty" type="LoyaltyPricingType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">The amount of Loyalty Points that the customer can earn or spend in relation to this product</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attributeGroup ref="CurrencyAmountGroup"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
