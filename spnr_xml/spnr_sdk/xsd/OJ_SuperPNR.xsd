					<xs:documentation xml:lang="en">Point of sale object. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="xPromotion" type="xPromotionType" minOccurs="0"/>
			<xs:element name="ModularProduct" type="ModularProductType" minOccurs="0" maxOccurs="unbounded"/>
