	<xs:complexType name="AmendmentFeesType">
		<xs:sequence>
			<xs:element name="AmendmentFee" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A fee applied when the product is amended</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="AmendmentFeeType">
							<xs:attributeGroup ref="ProductIDs"/>
							<xs:attribute name="ReplacedBy" type="RPH_Type">
								<xs:annotation>
									<xs:documentation>Historic ProductNumber of the Product that replaces the original product after a date change or upgrade.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="CurrencyAmountGroup"/>
		<xs:attribute name="Quantity" type="xs:integer">
			<xs:annotation>
				<xs:documentation>Indicates the total points amendment fees</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AmendmentFeeType">
		<xs:sequence>
			<xs:element name="Agent" type="AgentDetailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The details of the agent if cancelled by an agent </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SupplierCost" type="SupplierCostType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Description" type="xs:string">
			<xs:annotation>
				<xs:documentation>A description or name of the amendment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Code" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Any code asscoiated with the type of amendment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CodeContext" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The context of the code eg the vendor name, or the supplier name, or name of some standard list of codes</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="CurrencyAmountGroup"/>
		<xs:attribute name="Manual" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>Indicates if this amendment fee is a manual one performed by an agent, rather than an automatic one applied by the rules.

					During repricing all amendment fees will be removed and recalculated, but manual ones will be retained.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Date" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation>The date this amemendment was added</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AuditID" type="xs:int"/>
		<xs:attribute name="Quantity" type="xs:integer">
			<xs:annotation>
				<xs:documentation>Indicates the points value of the amendment fee</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FlightSegmentRPH" type="RPH_Type">
			<xs:annotation>
				<xs:documentation xml:lang="en">Unique identifier to link the penalty with the appropriate Flight segment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OriginDestinationRPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Unique identifier to link the penalty with the appropriate Origin Destination.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OJ_SuperPNR_RPH" type="RPH_Type">
			<xs:annotation>
				<xs:documentation xml:lang="en">Unique identifier to link the penalty with the appropriate Traveler.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AuditType">
		<xs:sequence>
			<xs:element name="Agent" type="AgentDetailsType" minOccurs="0"/>
			<xs:element name="Events" type="AuditEventsType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="needCancel" type="xs:string">
			<xs:annotation>
				<xs:documentation>if the MP need to cancel for datechange</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ID" type="xs:int">
			<xs:annotation>
				<xs:documentation>ID of this audit</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Version">
			<xs:annotation>
				<xs:documentation>The version of the SuperPNR controller responsible for the changes at this audit</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Timestamp">
			<xs:annotation>
				<xs:documentation>The time this audit took place</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SourceAddress">
			<xs:annotation>
				<xs:documentation>IP Address of SuperPNR Controller</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Tag">
			<xs:annotation>
				<xs:documentation>Can be used by project teams to set release numbers or tags here</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Channel" type="StringLength1to32" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Identifies the channel or storefront from which the request originated.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RequestorID" type="StringLength1to32" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A unique identifying value assigned by the creating system.  The ID attribute may be used to reference a primary-key value within a database or in a particular implementation. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Action" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Record which operation this audit do.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="CorrelationIDGroup"/>
	</xs:complexType>
	<xs:complexType name="AuditsType">
		<xs:sequence>
			<xs:element name="Audit" type="AuditType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CancellationFeesType">
		<xs:sequence>
			<xs:element name="CancellationFee" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A fee applied when the product is cancelled</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="CancellationFeeType">
							<xs:attributeGroup ref="ProductIDs"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="CurrencyAmountGroup"/>
		<xs:attribute name="Quantity" type="xs:double">
			<xs:annotation>
				<xs:documentation>Indicates the total points for cancellation fees</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="WaiverCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The waiver code associated with these fees.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="CancellationFeeType">
		<xs:sequence>
			<xs:element name="Agent" type="AgentDetailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The details of the agent if cancelled by an agent </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SupplierCost" type="SupplierCostType" minOccurs="0"/>
			<xs:element name="NonMemberFee" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attributeGroup ref="CurrencyAmountGroup"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="FeeDescription" type="FeeDescType" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Percentage" type="Percentage" use="optional"/>
		<xs:attributeGroup ref="CurrencyAmountGroup"/>
		<xs:attribute name="Quantity" type="xs:double">
			<xs:annotation>
				<xs:documentation>Redemption Quantity</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Date" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation>The date this Cancellation Fee was added</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PassengerRPH" type="xs:string">
			<xs:annotation>
				<xs:documentation>Passenger RPH to indicate the passenger id</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AuditID" type="xs:int">
			<xs:annotation>
				<xs:documentation>Audit ID associated with this cancellation Fee</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FlightSegmentRPH" type="xs:string">
			<xs:annotation>
				<xs:documentation xml:lang="en">Unique identifier to link the penalty with the appropriate Flight segment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OriginDestinationRPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Unique identifier to link the penalty with the appropriate Origin Destination.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RoomIndex" type="xs:string">
			<xs:annotation>
				<xs:documentation xml:lang="en">Unique identifier room Index.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OJ_SuperPNR_RPH" type="xs:string">
			<xs:annotation>
				<xs:documentation xml:lang="en">Unique identifier to link the penalty with the appropriate Traveler.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="eTicketNumber" type="xs:string">
			<xs:annotation>
				<xs:documentation>If reservation is electronically ticketed at time of booking, this is the field for the eTicket number.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Source" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates the source used to determine the refund. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BasisAmountType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>indicates the method of the cancellation fee calculation,  Value could be: "BaseAmount" the fee is calculated from base amount;
				"BaseCabinClassAmount" the fee is calculated from the publish fare of the  booking class</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductType" type="xs:string">
			<xs:annotation>
				<xs:documentation xml:lang="en">Types of product:
air
seat
meal
baggage</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ModifyTypesType">
		<xs:sequence>
			<xs:element name="ModifyType" type="ModifyTypeType" maxOccurs="99"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ModifyTypeType">
		<xs:attribute name="Type" type="AlphaNumericStringLength1to19" use="required">
			<xs:annotation>
				<xs:documentation>Indicates the type of modification pending on this product/package/SPNR</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="oldMPNum" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>the old MP productNumber for datechange</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="splitRequired" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>split sign for datechange true:false</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="NeedCancel" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates this Split Product needs to be cancelled, this attribute will be removed once the Product is cancelled.
If ModifyType/@Type is 'SegmentTransfer', NeedCancel = 'false' indicates this ancillary product booked with old ticket was successfully transferred to the new ticket for a flight upgrade/date change.
If ModifyType/@Type is 'SegmentTransfer', NeedCancel = 'true' indicates the transfer failed and the products need to be refunded.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="oldPNR" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The old PNR for Date Change or Split</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OriginalHistoryID" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Original History ID of the Modular Product that was Date Changed, Upgraded or Split</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="freeDateChange" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>is free datechange  true:false </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AuditID" type="xs:int" use="required">
			<xs:annotation>
				<xs:documentation>record the modify number for datechange</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OpenTag" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>is split hostpnr tag for open tkt</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OfflineSplit" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates this is offline Split Product</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RPH" type="RPH_Type"/>
		<xs:attribute name="XPATH" type="xs:string"/>
		<xs:attribute name="Action"/>
		<xs:attribute name="FlightSegmentRPH" type="RPH_Type">
			<xs:annotation>
				<xs:documentation xml:lang="en">Unique identifier to link the penalty with the appropriate Flight segment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CustomerRPH" type="RPH_Type">
			<xs:annotation>
				<xs:documentation xml:lang="en">Unique identifier to link the OJ_SuperPNR_RPH of the customer being modified.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ContactPersonRPH" type="RPH_Type">
			<xs:annotation>
				<xs:documentation xml:lang="en">Unique identifier to link the OJ_SuperPNR_RPH of the customer contact person being modified.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OrigCabinClass" type="xs:string">
			<xs:annotation>
				<xs:documentation>Original cabin class.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ReshopID">
			<xs:annotation>
				<xs:documentation>The customised ID for each reshop process.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ProductBase">
		<xs:choice>
			<xs:element name="Air" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flight booking details</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="AirReservationType">
							<xs:sequence>
								<xs:element name="CancellationPenalties" type="CancelPenaltiesType" minOccurs="0"/>
								<xs:element name="AmendmentPenalties" type="OJ_AmendPenaltiesType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Defines any fees or other penallites for changing this product</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="SupplierPenalties" type="SupplierPenaltiesType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Any known supplier penalties for cancellation or amendment,  This is informational for back office systems</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Insurance" type="InsuranceProductLinkType" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>This provides a link to the Insurance Modular Product associated with the Air Product</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Agent" type="AgentDetailsType" minOccurs="0"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Vehicle" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Vehicle rental booking details</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="VehicleReservationType">
							<xs:sequence>
								<xs:element name="VehResRQInfo" type="VehicleReservationRQAdditionalInfoType" minOccurs="0"/>
								<xs:element name="CancellationPenalties" type="CancelPenaltiesType"/>
								<xs:element name="AmendmentPenalties" type="OJ_AmendPenaltiesType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Defines any fees or other penallites for changing this product</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="SupplierPenalties" type="SupplierPenaltiesType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Any known supplier penalties for cancellation or amendment,  This is informational for back office systems</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="BookingReferenceID" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="CompanyName" minOccurs="0">
												<xs:complexType>
													<xs:attribute name="Code" type="xs:string" use="optional"/>
												</xs:complexType>
											</xs:element>
											<xs:element name="VoucherURL" minOccurs="0" maxOccurs="unbounded">
												<xs:complexType>
													<xs:attribute name="RPH"/>
													<xs:attribute name="URL"/>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
										<xs:attribute name="IDContext"/>
										<xs:attribute name="ID" type="xs:string" use="optional">
											<xs:annotation>
												<xs:documentation>Booking Reference</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="BookingReferenceID" type="xs:string" use="optional">
											<xs:annotation>
												<xs:documentation>Booking Reference</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:complexType>
								</xs:element>
								<xs:element name="DriverType" minOccurs="0">
									<xs:complexType>
										<xs:attributeGroup ref="TravelerCountGroup"/>
									</xs:complexType>
								</xs:element>
								<xs:element name="VehAvailCore" type="VehicleAvailCoreType" minOccurs="0"/>
							</xs:sequence>
							<xs:attribute name="BookingReferenceID"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Hotel" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Hotel booking details</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="HotelReservationType">
							<xs:sequence>
								<xs:element name="CancellationPenalties" type="CancelPenaltiesType"/>
								<xs:element name="AmendmentPenalties" type="OJ_AmendPenaltiesType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Defines any fees or other penallites for changing this product</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="SupplierPenalties">
									<xs:annotation>
										<xs:documentation>Any known supplier penalties for cancellation or amendment,  This is informational for back office systems</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="SupplierPenaltiesType">
												<xs:sequence>
													<xs:element name="CancelPenalty" type="CancelPenaltyType" minOccurs="0" maxOccurs="unbounded"/>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Ferry" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ferry Booking details</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="FerryProductType">
							<xs:sequence>
								<xs:element name="CancellationPenalties" type="CancelPenaltiesType"/>
								<xs:element name="AmendmentPenalties" type="OJ_AmendPenaltiesType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Defines any fees or other penallites for changing this product</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="SupplierPenalties" type="SupplierPenaltiesType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Any known supplier penalties for cancellation or amendment,  This is informational for back office systems</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Activity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Events and other extra details</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="DestActivityReservationType">
							<xs:sequence>
								<xs:element name="Air" type="AirReservationType" minOccurs="0"/>
								<xs:element name="CancellationPenalties" type="CancelPenaltiesType"/>
								<xs:element name="ReservationItemReferences" type="ReservationItemReferencesType" minOccurs="0"/>
								<xs:element name="BookingReferenceID" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="CompanyName" minOccurs="0">
												<xs:complexType>
													<xs:attribute name="Code" type="xs:string" use="optional"/>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
										<xs:attribute name="ID" type="xs:string" use="optional">
											<xs:annotation>
												<xs:documentation>Booking Reference</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:complexType>
								</xs:element>
								<xs:element name="TPA_Extensions" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="DestActivityDescriptiveContent" type="DestActivityDescriptiveContentType"/>
											<xs:element name="SupplierSpecificInfos" type="SupplierSpecificInfosType" minOccurs="0"/>
											<xs:element name="Insurance" minOccurs="0">
												<xs:complexType>
													<xs:complexContent>
														<xs:extension base="InsCoverageType">
															<xs:sequence>
																<xs:element name="InsuranceCustomer" type="InsuranceCustomerType" minOccurs="0"/>
																<xs:element name="PlanCost" minOccurs="0">
																	<xs:complexType>
																		<xs:complexContent>
																			<xs:extension base="PlanCostType">
																				<xs:sequence>
																					<xs:element name="ExchangeRate" minOccurs="0" maxOccurs="unbounded">
																						<xs:complexType>
																							<xs:attributeGroup ref="ExchangeRateGroup"/>
																						</xs:complexType>
																					</xs:element>
																					<xs:element name="PriceAdjustments" type="SPNR_PriceAdjustmentsType" minOccurs="0"/>
																					<xs:element name="IncrementalPrices" minOccurs="0">
																						<xs:annotation>
																							<xs:documentation xml:lang="en">For the insurance quotes where price can increase based on number of days travelling, this element provides the information on how the price should increases.</xs:documentation>
																						</xs:annotation>
																						<xs:complexType>
																							<xs:sequence>
																								<xs:element name="IncrementalPrice" minOccurs="1" maxOccurs="unbounded">
																									<xs:complexType>
																										<xs:attributeGroup ref="CurrencyAmountGroup"/>
																										<xs:attribute name="Frequency" type="xs:positiveInteger" use="optional">
																											<xs:annotation>
																												<xs:documentation xml:lang="en">Frequency combined with FrequencyType are the timeframe used to apply the charge (e.g. every 3 days, every 5 hours). This attribute indicates the number of units, e.g. 3 or 5.</xs:documentation>
																											</xs:annotation>
																										</xs:attribute>
																										<xs:attribute name="FrequencyType" type="TimeUnitType" use="optional">
																											<xs:annotation>
																												<xs:documentation xml:lang="en">Frequency combined with FrequencyType are the timeframe used to apply the charge (e.g. every 3 days, every 5 hours). This attribute indicates the unit, e.g. days or hours.</xs:documentation>
																											</xs:annotation>
																										</xs:attribute>
																									</xs:complexType>
																								</xs:element>
																							</xs:sequence>
																						</xs:complexType>
																					</xs:element>
																				</xs:sequence>
																			</xs:extension>
																		</xs:complexContent>
																	</xs:complexType>
																</xs:element>
																<xs:element name="UniqueID" type="UniqueID_Type" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation xml:lang="en">Unique identifier associated with booking request message</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="PolicyDetail" type="PolicyDetail_Type" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation xml:lang="en">Contains such specific, policy-identifying details such as policy number, reference number, effective and expiration dates.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="QuoteDetail" type="QuoteDetail_Type" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation xml:lang="en">Contains the information about the plan, such as the name of the insurance company and restrictions, specific to the quote.</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:extension>
													</xs:complexContent>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="AmendmentPenalties" type="OJ_AmendPenaltiesType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Defines any fees or other penallites for changing this product</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="SupplierPenalties" type="SupplierPenaltiesType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Any known supplier penalties for cancellation or amendment,  This is informational for back office systems</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="ReservationReferences" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="ReservationReference" type="UniqueID_Type" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Insurance" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Contains a booking request for a single insurance plan</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="InsCoverageType">
							<xs:sequence>
								<xs:element name="InsuranceCustomer" type="InsuranceCustomerType"/>
								<xs:element name="PlanCost" type="PlanCostType" minOccurs="0">
									<xs:annotation>
										<xs:documentation xml:lang="en">Cost of insurance plan from initial quote (allows for price validation)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="UniqueID" type="UniqueID_Type" minOccurs="0">
									<xs:annotation>
										<xs:documentation xml:lang="en">Contains the supplier's insurance unique reference</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="CancellationPenalties" type="CancelPenaltiesType"/>
								<xs:element name="AmendmentPenalties" type="OJ_AmendPenaltiesType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Defines any fees or other penallites for changing this product</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="SupplierPenalties" type="SupplierPenaltiesType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Any known supplier penalties for cancellation or amendment,  This is informational for back office systems</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="PolicyDetail" type="PolicyDetail_Type" minOccurs="0">
									<xs:annotation>
										<xs:documentation xml:lang="en">Contains such specific, policy-identifying details such as policy number, reference number, effective and expiration dates.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Baggage" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains a booking request for a single baggage ssr</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Air" type="AirReservationType" minOccurs="0"/>
						<xs:element name="OriginDestination">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Baggage allowance by origin/ destination.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Service">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Optional service detail.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Description" type="xs:string">
													<xs:annotation>
														<xs:documentation source="DESCRIPTION" xml:lang="en">Commercial name (title description) of offered service.
															Example: 3RD BAG FREE</xs:documentation>
														<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, ATPCO S5 Commercial Name.</xs:documentation>
														<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
															1. UI/ display tip: You may build a one line description to display by stringing the following values together:
															"Description"+"ServiceLocation"+"ServiceDate"+"DescriptionVariable" which could be displayed as sampled here:
															'Airline Lounge Pass DFW 10May' - where 'Airline Lounge Pass' is the "Description", 'DFW' the "ServiceLocation" and '10May' the "ServiceDate"
															-or-
															Premium Seat 9D - where 'Premium Seat' is the "Description" and '9D' the "DescriptionVariable"</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element ref="DescriptionDetails" minOccurs="0"/>
												<xs:element name="Amount" type="xs:string" minOccurs="0">
													<xs:annotation>
														<xs:documentation source="DESCRIPTION" xml:lang="en">Currency display price for this option or option differential.
															Example: 25.50</xs:documentation>
														<xs:documentation source="ENCODING" xml:lang="en">=accepted values=
															A string value that includes the price as a major and minor unit, e.g. dollar amount with a decimal and cents.</xs:documentation>
														<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
															1. This is for display purposes only and not associated to a currency code.</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element ref="ServicePrice" minOccurs="0"/>
												<xs:element ref="ServiceDate" minOccurs="0"/>
												<xs:element ref="ValidatingCarrier" minOccurs="0"/>
												<xs:element name="MSCarrier" type="xs:string" minOccurs="0">
													<xs:annotation>
														<xs:documentation source="DESCRIPTION" xml:lang="en">Carrier name who governs the baggage rules for this allowance.
															Example: DELTA</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="BookingInstructions" minOccurs="0">
													<xs:annotation>
														<xs:documentation source="DESCRIPTION" xml:lang="en">Optional service booking instructions.</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="SSRCode" type="SSRCodeType" minOccurs="0" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation source="DESCRIPTION" xml:lang="en">Passenger assistance SSR code value.
																		Examples:
																		EXST (EXTRA SEAT REQUIRED)
																		INFT (INFANT ACCOMPANYING ADULT PASSENGER)
																		PETC (PASSENGER TRAVELING WITH PET IN CABIN)
																		UMNR (UNACCOMPANIED MINOR)

																		=notes=
																		1. Although optional in the schema, some SSR codes may require associated description elements.</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="OSIText" type="xs:string" minOccurs="0" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation source="DESCRIPTION" xml:lang="en">Other service information (OSI) text.
																		Example: PAX VIP MEMBER OF PARLIAMENT</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="Method" type="xs:string" minOccurs="0">
																<xs:annotation>
																	<xs:documentation source="DESCRIPTION" xml:lang="en">PNR booking method.
																		Example: AE (BY SSR - SPECIAL SERVICE REQUEST)

																		For maximum interoperability, refer to IATA PADIS Code List for data element 9750.</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="UpgradeMethod" minOccurs="0">
																<xs:annotation>
																	<xs:documentation source="DESCRIPTION" xml:lang="en">Upgrade method.
																		Example: AE (BY SSR - SPECIAL SERVICE REQUEST)

																		For maximum interoperability, refer to IATA PADIS Code List for data element 9750.</xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:simpleContent>
																		<xs:extension base="xs:string">
																			<xs:attribute name="NewClass" type="FlightCOSType" use="optional">
																				<xs:annotation>
																					<xs:documentation source="DESCRIPTION" xml:lang="en">New class of service.
																						Example: B</xs:documentation>
																				</xs:annotation>
																			</xs:attribute>
																		</xs:extension>
																	</xs:simpleContent>
																</xs:complexType>
															</xs:element>
															<xs:element name="Text" type="FreeFormTextType" minOccurs="0" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation source="DESCRIPTION" xml:lang="en">Data center instructions.</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="Equipment" type="EquipmentDetailType" minOccurs="0">
																<xs:annotation>
																	<xs:documentation source="DESCRIPTION" xml:lang="en">Aircraft equipment information.</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="Baggage" minOccurs="0">
													<xs:annotation>
														<xs:documentation source="DESCRIPTION" xml:lang="en">Baggage details.</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="MaxWeight" minOccurs="0" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation source="DESCRIPTION" xml:lang="en">Maximum weight per piece value.
																		Example: 35</xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:simpleContent>
																		<xs:extension base="xs:decimal">
																			<xs:attribute name="Unit" type="WeightUnitType" use="required">
																				<xs:annotation>
																					<xs:documentation source="DESCRIPTION" xml:lang="en">Weight unit of measure.
																						Example: KG (KILOGRAMS)</xs:documentation>
																					<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list.
																						=enumerated values=
																						KG = KILOGRAMS
																						LBS = POUNDS</xs:documentation>
																				</xs:annotation>
																			</xs:attribute>
																		</xs:extension>
																	</xs:simpleContent>
																</xs:complexType>
															</xs:element>
															<xs:element name="MinWeight" type="BaggageWeightType" minOccurs="0" maxOccurs="unbounded"/>
															<xs:element name="MaxDimensions" type="SPNRBaggageDimensionsType" minOccurs="0" maxOccurs="unbounded"/>
															<xs:element name="MinDimensions" type="SPNRBaggageDimensionsType" minOccurs="0" maxOccurs="unbounded"/>
															<xs:element name="ExcessFirst" type="xs:integer" minOccurs="0">
																<xs:annotation>
																	<xs:documentation source="DESCRIPTION" xml:lang="en">First occurrence of the excess bag.
																		Example: 3</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="ExcessLast" type="xs:integer" minOccurs="0">
																<xs:annotation>
																	<xs:documentation source="DESCRIPTION" xml:lang="en">Last occurrence of the excess bag.
																		Example: 4</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
														<xs:attribute name="SpecialItem">
															<xs:simpleType>
																<xs:restriction base="xs:string">
																	<xs:enumeration value="N"/>
																	<xs:enumeration value="Y"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:attribute>
													</xs:complexType>
												</xs:element>
												<xs:element ref="Attributes" minOccurs="0"/>
												<xs:element ref="Diagnostics" minOccurs="0"/>
												<xs:element name="S5" type="S5Type" minOccurs="0">
													<xs:annotation>
														<xs:documentation source="DESCRIPTION" xml:lang="en">ATPCO record S5 diagnostic information.</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="S7" type="S7Type" minOccurs="0">
													<xs:annotation>
														<xs:documentation source="DESCRIPTION" xml:lang="en">ATPCO record S7 diagnostic information.</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="PriceAdjustments" type="exPriceAdjustmentsType" minOccurs="0"/>
												<xs:element name="TotalAmount" type="TotalType">
													<xs:annotation>
														<xs:documentation>Element containing the total amount payable after mark-up or discount. It contains the combination of amount +/- the priceadjustments.</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ExchangeRate" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Stores the exchange rate between vendor and supplier currencies</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:attributeGroup ref="ExchangeRateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="Info" type="InfoType" minOccurs="0"/>
												<xs:element name="FlightSegmentNumber" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
												<xs:element name="SegmentIDRef" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
												<xs:element name="TravelerIDRef" type="xs:int" minOccurs="0" maxOccurs="unbounded"/>
											</xs:sequence>
											<xs:attributeGroup ref="SrvcItemAttributesGroup"/>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attributeGroup ref="DepartureArrivalCode"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="TravelerInfo" type="TravelerInfoType">
							<xs:annotation>
								<xs:documentation xml:lang="en">All traveler information relevant to a booking request.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Ticketing" type="TicketingInfoType" minOccurs="0" maxOccurs="99">
							<xs:annotation>
								<xs:documentation xml:lang="en">Information used to specify the EMD ticketing arrangement.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="EMD_Info" type="EMD_Type" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="en">Provides the electronic miscellaneous document information for a previously issued EMD.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CancellationPenalties" type="CancelPenaltiesType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>DEPRECATED - ModularProduct/CancellationPenalties should be used instead.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="AmendmentPenalties" type="OJ_AmendPenaltiesType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>DEPRECATED - ModularProduct/AmendmentPenalties should be used instead.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="Quantity" type="xs:int" use="required"/>
					<xs:attribute name="BookingOrderIndicator" type="xs:int">
						<xs:annotation>
							<xs:documentation>Order of bagage booking required for later cancellation order</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="BundleCode" type="xs:string">
						<xs:annotation>
							<xs:documentation>Code of bundle when baggage is part of the bundle </xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Refundable" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation>Indicates the product can be refund or non-refundable after it has been fully paid.
								The value can be true or false.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Cancelable" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation>Indicates if the product can be cancelled individually, before it has been paid. The value can be true or false.
								If it is true, the product can be cancelled seperately with the bookedwith flight.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="isSegBind" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation>Indicates if the product can bind with a flight segment, the value can be true or false.
								If it is false, the flight segment can be changed without modifying this binded product.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="BookingReferenceID" type="xs:int">
						<xs:annotation>
							<xs:documentation>Unique booking identifier</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="OriginDestinationRPH" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>RPH attribute from OriginDestinationOption section from this ancillary's flight segment</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="FlightSegmentRPH" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>RPH attribute from FlightSegment section from this ancillary's flight segment</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="AncillaryProduct" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Air" type="AirReservationType" minOccurs="0"/>
						<xs:element name="Content" type="ProductContentType"/>
						<xs:element name="CancellationPenalties" type="CancelPenaltiesType"/>
						<xs:element name="AmendmentPenalties" type="OJ_AmendPenaltiesType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Defines any fees or other penallites for changing this product</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TravelerInfo" type="TravelerInfoType">
							<xs:annotation>
								<xs:documentation xml:lang="en">All traveler information relevant to a booking request.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Ticketing" type="TicketingInfoType" minOccurs="0" maxOccurs="99">
							<xs:annotation>
								<xs:documentation xml:lang="en">Information used to specify the EMD ticketing arrangement.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="EMD_Info" type="EMD_Type" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="en">Provides the electronic miscellaneous document information for a previously issued EMD.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Attributes" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Contains a list of configuration codes of a bundle.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Attribute" minOccurs="0" maxOccurs="unbounded">
										<xs:complexType>
											<xs:attribute name="Code" type="xs:string" use="required">
												<xs:annotation>
													<xs:documentation>Unique code for a special configuration of the bundle</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="Name" type="xs:string" use="required">
												<xs:annotation>
													<xs:documentation>Description of this configuration</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Contact" type="ContactPersonType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">Contact information for this booking. May be either one of the travelers as specified by the RPH or a new set of contact information.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="AncillaryProducts" type="AirAncillariesBundledProductsSummaryType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>In the case of an ancillary bundle, we may need to list the contained ancillary products</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="FlightSegment" type="FlightSegmentWithPrice" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation source="Deprecation" xml:lang="en">DEPRECATED: This will be removed in a future release, and has been added so schema validation will pass for historic bookings.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Prices" type="AirAncillaryProductPricesType"/>
						<xs:element name="Voucher" type="SPNRVoucherType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Voucher data.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Warnings" type="SPNR_WarningsType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="GroundTransportService" type="GroundTransportServiceType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">The selected transport information, including type of transport service, service details, service restrictions, charges and fees and payment rules.</xs:documentation>
