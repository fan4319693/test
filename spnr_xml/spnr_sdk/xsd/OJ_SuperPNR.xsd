							<xs:documentation>Origin location of the flight</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Destination" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation>Destination location of the flight</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="SeatNumber" type="xs:string">
						<xs:annotation>
							<xs:documentation>Add a Seat Number for Seat Products.</xs:documentation>
						</xs:annotation>
