			<xs:annotation>
				<xs:documentation>The version of the SuperPNR schema.  This increments as the schema changes</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CryptoVersion" type="xs:nonNegativeInteger">
