					<xs:complexContent>
						<xs:extension base="RemarksType">
							<xs:sequence>
								<xs:element name="Queue" minOccurs="0" maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Action" type="QueueActionType" maxOccurs="unbounded"/>
										</xs:sequence>
										<xs:attributeGroup ref="QueueBase"/>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:extension>
