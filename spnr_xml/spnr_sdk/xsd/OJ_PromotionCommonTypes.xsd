<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2010 (http://www.altova.com) by Lorcan O'Toole (Openjaw Technologies Ltd.) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="OTA_CommonTypes.xsd"/>
	<xs:include schemaLocation="OTA_HotelContentDescription.xsd"/>
	<xs:include schemaLocation="OJ_DeepLink.xsd"/>
	<xs:include schemaLocation="OJ_Promotion_Search_Hotel.xsd"/>
	<xs:include schemaLocation="OJ_Promotion_Search_Flight.xsd"/>
	<xs:include schemaLocation="OJ_Promotion_Search_Vehicle.xsd"/>
	<xs:include schemaLocation="OJ_DestActivityCommonTypes.xsd"/>
	<xs:complexType name="PromotionProfilesType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Container for promotion profiles.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PromotionProfiles" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Container for promotions. Defines the criteria used to define a promotion</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="PromotionProfileType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PromotionProfileType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Container for promotions. Defines the criteria used to define a promotion</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Channels" minOccurs="0"/>
			<xs:element ref="Status">
				<xs:annotation>
					<xs:documentation xml:lang="en">The status of xPromotion. eg. deleted</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Search"/>
			<xs:element ref="Groups"/>
			<xs:element ref="Tags"/>
			<xs:element ref="SearchCriteria"/>
			<xs:element ref="TripProfiles">
				<xs:annotation>
					<xs:documentation>Trip Profile matched to flight search request</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Content"/>
			<xs:element name="FromPrices" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The set of cheapest prices that satisfies the xPromotion Promotion Profile.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="FromPrice"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Impression" type="ImpressionType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Id" use="required">
			<xs:annotation>
				<xs:documentation>The Id identifying this promotion profile uniquely.  There may be multiple Promotions that satisfy a Promotion profile.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Status" use="required">
			<xs:annotation>
				<xs:documentation>The current status of this promotion profile - see enumerations list.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="enabled"/>
					<xs:enumeration value="disabled"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Name" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Reference name for the promotion profile.  This does not have to reflect the display name which can be configured by language.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TimeStamp">
			<xs:annotation>
				<xs:documentation>The time stamp of the last update to this promotion profile.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Priority" type="xs:unsignedInt" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The priority to to be given to this promotion.  Higher priorites ranked highest.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccessLevel">
			<xs:annotation>
				<xs:documentation>The level of access retrieve this promotion profile - see enumeration.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="AlphaNumericStringLength1to14">
					<xs:enumeration value="public"/>
					<xs:enumeration value="private"/>
					<xs:enumeration value="all"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Product">
			<xs:annotation>
				<xs:documentation>Product type code reflecting the product type that this promotion applies to - see enumerations list</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="AlphaNumericStringLength1to8">
					<xs:enumeration value="H"/>
					<xs:enumeration value="F"/>
					<xs:enumeration value="V"/>
					<xs:enumeration value="E"/>
					<xs:enumeration value="HF"/>
					<xs:enumeration value="HFV"/>
					<xs:enumeration value="HV"/>
					<xs:enumeration value="VF"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Language">
			<xs:annotation>
				<xs:documentation>Defines the requesting language for this promotion </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PromotionsType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Container for promotions.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Promotion" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Promotion with minimum pricing information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="PromotionType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PromotionType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Promotion with pricing information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="a">
				<xs:complexType>
					<xs:attribute name="href" type="xs:anyURI" use="required"/>
					<xs:attribute name="onclick" type="xs:string" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="FromPrices" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The set of cheapest prices that satisfies the xPromotion Promotion Profile.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="FromPrice"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Images" type="ImagesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Set of images that represent this promotion.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Descriptions" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">List of descriptions per language.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="PromotionDescriptionType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Search" type="SearchType">
				<xs:annotation>
					<xs:documentation>The container for all of the search details.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Impression" type="ImpressionType" minOccurs="0"/>
			<xs:element ref="SearchResponse" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Status">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Enabled"/>
					<xs:enumeration value="Disabled"/>
					<xs:enumeration value="Template"/>
					<xs:enumeration value="Archived"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Name" type="xs:string" use="required"/>
		<xs:attribute name="Priority">
			<xs:annotation>
				<xs:documentation>The priority to to be given to this promotion.  Higher priorites ranked highest.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Id" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The Id identifying this specific promotion uniquely.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ImagesType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Set of images that represent this promotion.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Image" type="ImageType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">An image that represent this promotion.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ImageType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Set of images that represent this promotion.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Alt" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A text alternative to the image.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="Language" type="xs:language" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="src" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">The url and filename of the image.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PromotionDescriptionType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Description of the promotion.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Description" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Short" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="en">A short description of the promotion.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string"/>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="Long" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="en">A full long description of the promotion.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string"/>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="Language" type="xs:language" use="required"/>
					<xs:attribute name="Type" use="required">
						<xs:annotation>
							<xs:documentation>What type of content this text describes.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="promotion"/>
								<xs:enumeration value="area"/>
								<xs:enumeration value="title"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FromPrice">
		<xs:annotation>
			<xs:documentation>The cheapest price of any promotion that satisfies this Promotion Profile.  If left empty in a create promotion profile, this will be updated when the cache gets set. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FromPrice" maxOccurs="unbounded">
				<xs:complexType>
					<xs:attributeGroup ref="CurrencyAmountGroup"/>
					<xs:attribute name="Id" type="xs:string">
						<xs:annotation>
							<xs:documentation>Reference to the full Promotion details that this FromPrice is for</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ImpressionType">
		<xs:annotation>
			<xs:documentation>This is used as a temporary storage node for impression related details that are stored in the reporting DB.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Session" type="SessionType"/>
			<xs:element name="Context" type="ContextType"/>
		</xs:sequence>
		<xs:attributeGroup ref="CurrencyAmountGroup"/>
		<xs:attribute name="Position" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Where in the listing did this this impression occur.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PromotionProfileId" type="xs:string" use="required"/>
		<xs:attribute name="TripProfileId" type="xs:string" use="required"/>
		<xs:attribute name="PromotionId" type="xs:string" use="required"/>
		<xs:attribute name="Id" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The Id identifying this promotion profile uniquely.  There may be multiple Promotions that satisfy a Promotion profile.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ConversionType">
		<xs:annotation>
			<xs:documentation>This is used as a temporary storage node for booking conversion related details that are stored in the reporting DB.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CoreProduct">
				<xs:annotation>
					<xs:documentation>The cost associated with the core product of this superpnr.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attributeGroup ref="CurrencyAmountGroup"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="SPNR">
				<xs:annotation>
					<xs:documentation>The overall cost associated with this superpnr.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attributeGroup ref="CurrencyAmountGroup"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="PrimaryDestination" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The primary destination of this super pnr.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="xPromotionType">
		<xs:annotation>
			<xs:documentation>This is used to store xPromotion related information in messages and the SPNR.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Session" type="SessionType"/>
			<xs:element name="Impression" type="ImpressionType"/>
			<xs:element name="Search">
				<xs:annotation>
					<xs:documentation>The information on a particular search that came from an impression.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Top">
							<xs:annotation>
								<xs:documentation xml:lang="en">The price of the top/first option shown to the user.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attributeGroup ref="CurrencyAmountGroup"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="Cheapest">
							<xs:annotation>
								<xs:documentation xml:lang="en">The price of the cheapest option shown to the user.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attributeGroup ref="CurrencyAmountGroup"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="Type" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation>The type of search this was.  Either a visit (externally led) or a click (internally led)</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Conversion" type="ConversionType"/>
		</xs:sequence>
		<xs:attribute name="PromotionProfileId" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The Promotion Profile Id of the associated promotion.  </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TripProfileId" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The Trip Profile Id of the associated promotion.  </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PromotionId" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The Promotion Id of the associated promotion.  </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ImpressionId" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The id of the impression associated with this search or booking.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SearchId" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The id of the search associated with this booking.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PromotionProfileSessionId">
			<xs:annotation>
				<xs:documentation>The Promotion Profile Id of the associated promotion of the current session.  May not be the same as the PromotionProfileId</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TripProfileSessionId">
			<xs:annotation>
				<xs:documentation>The Trip Profile Id of the associated promotion in the current session.  May not be the same as the TripProfileId</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PromotionSessionId">
			<xs:annotation>
				<xs:documentation>The Promotion Id of the associated promotion of the current session.  May not be the same as the PromotionId</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ImpressionSessionId">
			<xs:annotation>
				<xs:documentation>The id of the impression associated with this search of booking within the current session.  May not be the same as the ImpressionId</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SearchSessionId">
			<xs:annotation>
				<xs:documentation>The id of the search associated with this booking in the session.  This may not be the same as the SearchId</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SortType" type="xs:string">
			<xs:annotation>
				<xs:documentation>Sorts the search results returned for a promotion profile</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="Channels">
		<xs:annotation>
			<xs:documentation>The distribution channels available to this xPromotion Promotion Profile.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Channel" type="ChannelType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation xml:lang="en">Promotion profile containing satisfying the search criteria.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="SessionType">
		<xs:annotation>
			<xs:documentation> </xs:documentation>
		</xs:annotation>
		<xs:attribute name="ID" use="required">
			<xs:annotation>
				<xs:documentation>The Id that identifies an individual browser session.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ConversationId">
			<xs:annotation>
				<xs:documentation>The Id that identifies an individual browser conversation.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ContextType">
		<xs:annotation>
			<xs:documentation>This is used to store information on where the promotion profile was displayed.  Used for reporting. </xs:documentation>
		</xs:annotation>
		<xs:attribute name="Display" type="xs:boolean" use="required"/>
		<xs:attribute name="ID" type="xs:string" use="required"/>
		<xs:attribute name="Level" type="xs:string" use="required"/>
		<xs:attribute name="Number" type="xs:string" use="required"/>
		<xs:attribute name="Page" type="xs:string" use="required"/>
		<xs:attribute name="Sequence" type="xs:string" use="required"/>
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="AlphaNumericStringLength1to8">
					<xs:enumeration value="all"/>
					<xs:enumeration value="email"/>
					<xs:enumeration value="web"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ChannelType">
		<xs:attribute name="index" use="required">
			<xs:annotation>
				<xs:documentation>The index that identified the Channel Type eg. B2C</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="AlphaNumericStringLength1to8">
					<xs:enumeration value="all"/>
					<xs:enumeration value="email"/>
					<xs:enumeration value="web"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="Tags">
		<xs:annotation>
			<xs:documentation>Reference tags for external system with content for this xPromotion Promotion Profile.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Tag" type="TagType"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="TagType">
		<xs:annotation>
			<xs:documentation> </xs:documentation>
		</xs:annotation>
		<xs:attribute name="index" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:byte">
					<xs:enumeration value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Id" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string"/>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Group">
		<xs:annotation>
			<xs:documentation> </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Group">
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:attribute name="Product">
		<xs:annotation>
			<xs:documentation>Product type code reflecting the product type that this promotion applies to - see enumerations list</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="AlphaNumericStringLength1to8">
				<xs:enumeration value="H"/>
				<xs:enumeration value="F"/>
				<xs:enumeration value="V"/>
				<xs:enumeration value="E"/>
				<xs:enumeration value="HF"/>
				<xs:enumeration value="HFV"/>
				<xs:enumeration value="HV"/>
				<xs:enumeration value="VF"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:element name="Status">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Comments">
					<xs:annotation>
						<xs:documentation>Describe the Status</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Reason">
					<xs:annotation>
						<xs:documentation>Describe the reason why modified the status</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Reason">
		<xs:simpleType>
			<xs:restriction base="xs:byte">
				<xs:enumeration value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Comments">
		<xs:complexType>
			<xs:sequence>
