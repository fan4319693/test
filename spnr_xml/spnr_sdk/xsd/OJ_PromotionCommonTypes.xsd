															<xs:annotation>
																<xs:documentation>Required for OJDP. The check out date that a hotel search should use with this flight</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="Reference" type="xs:positiveInteger" use="optional">
															<xs:annotation>
																<xs:documentation>Required for OJDP. Reference to a package Flight componant</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="Num24HourPeriods" type="xs:positiveInteger" use="optional">
															<xs:annotation>
																<xs:documentation>Required for OJDP. The number of 24 hour periods for car rental</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="PickUpDate" type="xs:date" use="optional">
															<xs:annotation>
																<xs:documentation>Required for OJDP. The pick up date that a car search should use with this flight</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="ReturnDate" type="xs:date" use="optional">
															<xs:annotation>
																<xs:documentation>Required for OJDP. The return date that a car search should use with this flight</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="VehicleRangeNum" type="xs:float" use="optional"/>
														<xs:attribute name="Start">
															<xs:annotation>
																<xs:documentation>Required for OJDP.  The departure datetime of the 1st flight in the itin.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="End">
															<xs:annotation>
																<xs:documentation>Required for OJDP.  The arrival datetime of the last flight in the itin.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="RedemptionChange"/>
									<xs:attribute name="EarnChange"/>
									<xs:attribute name="Reference" type="xs:string" use="required"/>
									<xs:attribute name="Amount" type="Money" use="required"/>
									<xs:attribute name="Currency" type="AlphaLength3" use="required"/>
									<xs:attribute name="Adjusted"/>
									<xs:attribute name="OriginDestinationRPH" type="xs:string" use="optional"/>
									<xs:attribute name="Selected"/>
									<xs:attribute name="AmountChange"/>
									<xs:attribute name="CheckinDate" type="xs:date" use="optional"/>
									<xs:attribute name="CheckoutDate" type="xs:date" use="optional"/>
									<xs:attribute name="PickUpDate" type="xs:date"/>
									<xs:attribute name="ReturnDate"/>
									<xs:attribute name="Start">
										<xs:annotation>
											<xs:documentation>Required for OJDP.  The departure datetime of the 1st flight in the itin.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="End">
										<xs:annotation>
											<xs:documentation>Required for OJDP.  The arrival datetime of the last flight in the itin.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="CombineAndSaveAmount" type="xs:float" use="optional"/>
									<xs:attribute name="DepartureAirport"/>
									<xs:attribute name="CabinClass"/>
									<xs:attribute name="ResBookDesigCode" type="UpperCaseAlphaLength1to2" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">Specific Booking Class for this segment.	</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="PickUpLocation"/>
									<xs:attribute name="SupplierCode" type="StringLength0to8"/>
									<xs:attribute name="Num24HourPeriods" type="StringLength0to8" use="optional"/>
									<xs:attribute name="OriginalCurrency" type="StringLength0to8" use="optional"/>
									<xs:attribute name="Rate" type="xs:float" use="optional"/>
									<xs:attribute name="VehicleRangeNum"/>
								</xs:complexType>
							</xs:element>
							<xs:element name="Extras" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Extra" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="PriceAdjustments" type="exPriceAdjustmentsType" minOccurs="0"/>
													<xs:element name="DestActivityDescriptiveContent" minOccurs="0">
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="DestActivityDescriptiveContentType">
																	<xs:sequence>
																		<xs:element name="ExchangeRate" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Stores the exchange rate between vendor and supplier currencies</xs:documentation>
																			</xs:annotation>
																			<xs:complexType>
																				<xs:attributeGroup ref="ExchangeRateGroup"/>
																			</xs:complexType>
																		</xs:element>
																		<xs:element name="ReportingExchangeRates" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>To store exchange rates for backoffice reporting tools, where the reporting tool demands a different currency from the supplier currency</xs:documentation>
																			</xs:annotation>
																			<xs:complexType>
																				<xs:attributeGroup ref="ExchangeRateGroup"/>
																			</xs:complexType>
																		</xs:element>
																	</xs:sequence>
																</xs:extension>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="Amount"/>
												<xs:attribute name="CurrencyCode"/>
												<xs:attribute name="Type"/>
												<xs:attribute name="Reference"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Price" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Modular">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="PriceAdjustments" type="exPriceAdjustmentsType" minOccurs="0"/>
												</xs:sequence>
												<xs:attribute name="Amount"/>
												<xs:attribute name="CurrencyCode"/>
												<xs:attribute name="AdjustedAmount"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="PriceAdjustments" type="exPriceAdjustmentsType" minOccurs="0"/>
							<xs:element name="Loyalty" type="LoyaltyPricingType" minOccurs="0"/>
						</xs:sequence>
						<xs:attribute name="Amount" type="Money" use="required"/>
						<xs:attribute name="CurrencyCode" type="AlphaLength3" use="required"/>
						<xs:attribute name="Reference" type="xs:string" use="required"/>
						<xs:attribute name="Type"/>
						<xs:attribute name="selected" type="xs:boolean"/>
						<xs:attribute name="RangeNum" type="xs:string" use="optional"/>
						<xs:attribute name="CombineAndSaveAmount"/>
						<xs:attribute name="AdjustedAmount"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="PricedItineraries" type="PricedItinerariesType" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="en">Successfull Low Fare priced itineraries in response to a Low Fare Search request.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RoomStays" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="RoomStay" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="RoomStayType">
											<xs:attribute name="Reference" type="xs:positiveInteger" use="optional">
												<xs:annotation>
													<xs:documentation>Required for OJDP. Reference to an Accomodation componant in a package</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="MoreIndicator" type="StringLength1to32">
							<xs:annotation>
								<xs:documentation xml:lang="en">A text field used to indicate that there are additional rates that could not be fit within the availability response.  The text returned should be meaningful in identifying where to begin the next block of data and sent in the availability request.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="VehAvails" minOccurs="0">
					<xs:annotation>
						<xs:documentation>A collection of vehicles along with their availability and associated rate information.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="VehAvail" maxOccurs="99">
								<xs:annotation>
									<xs:documentation>Availability and rate information for one specific vehicle.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="VehAvailCore">
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="VehicleAvailCoreType"/>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="Vendor" type="CompanyNameType" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Information on the vendor associated with this availability of vehicles</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="VehRentalCore" type="VehicleRentalCoreType">
											<xs:annotation>
												<xs:documentation>Information that is common,  or core, to all requests and responses associated with the reservation of a vehicle.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="VehAvailInfo" type="VehicleAvailAdditionalInfoType" minOccurs="0"/>
									</xs:sequence>
									<xs:attribute name="Reference" type="xs:string"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DestActivityDescriptiveContents" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DestActivityDescriptiveContent" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="DestActivityDescriptiveContentType">
											<xs:sequence>
												<xs:element name="ExchangeRate" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Stores the exchange rate between vendor and supplier currencies</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:attributeGroup ref="ExchangeRateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="ReportingExchangeRates" minOccurs="0">
													<xs:annotation>
														<xs:documentation>To store exchange rates for backoffice reporting tools, where the reporting tool demands a different currency from the supplier currency</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:attributeGroup ref="ExchangeRateGroup"/>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Search">
		<xs:annotation>
			<xs:documentation xml:lang="en">Search node contains defferent search information. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="SearchType">
					<xs:annotation>
						<xs:documentation xml:lang="en">Search type for promotion. eg. H stands for hotel.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence>
					<xs:element ref="Passengers">
						<xs:annotation>
							<xs:documentation xml:lang="en">Passenger information.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="OriginDestinationInformation">
						<xs:annotation>
							<xs:documentation xml:lang="en">Container for all of the search criteria related to location.  Used for all product types except vehicle only.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:sequence>
					<xs:element ref="HotelInformation"/>
					<xs:element ref="VehicleInformation"/>
					<xs:element ref="FlightInformation"/>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="SearchType">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="H"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Passengers">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Passenger" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Passenger">
		<xs:complexType>
			<xs:attribute name="index" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:byte"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Quantity" use="required">
				<xs:annotation>
					<xs:documentation>Number of passengers</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:byte"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Code" use="required">
				<xs:annotation>
					<xs:documentation>Passenger Code. This value can only be ADT, CHD or  INF. ADT stands for Adult. CHD stands for Children. INF stands for Infant.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="ADT"/>
						<xs:enumeration value="CHD"/>
						<xs:enumeration value="INF"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="OriginDestinationInformation">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="Location">
					<xs:annotation>
						<xs:documentation>The location information which include orgin and destination information.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="Origin">
		<xs:complexType mixed="true">
			<xs:attribute name="Exclusive" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>This indicates whether a search for this origin should be exclusive or not.  If true a promotion profile will only be returned if they only have trip profile origin destination options that match the origin</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="index" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Destination">
		<xs:complexType mixed="true">
			<xs:attribute name="Exclusive" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>This indicates whether a search for this destination should be exclusive or not.  If true a promotion profile will only be returned if they only have trip profile origin destination options that match the destination</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="index" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Location">
		<xs:complexType mixed="true">
			<xs:sequence minOccurs="0">
				<xs:element ref="Destination">
					<xs:annotation>
						<xs:documentation>Destination information</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Origin">
					<xs:annotation>
						<xs:documentation>Origin information</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="index">
				<xs:simpleType>
					<xs:restriction base="xs:byte">
						<xs:enumeration value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SearchCriteria">
		<xs:annotation>
			<xs:documentation>The container for the subset of search details that represent this promotion profile.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Restrictions">
					<xs:annotation>
						<xs:documentation>Search Restriction.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="PointsOfSale">
					<xs:annotation>
						<xs:documentation>The points of sale associated with this Landing page.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Dates">
					<xs:annotation>
						<xs:documentation>Date</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="OriginDestinationInformation">
					<xs:annotation>
						<xs:documentation>Container for all of the search criteria related to location.  Used for all product types except vehicle only.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Restrictions">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Search"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PointsOfSale">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PointOfSale"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PointOfSale">
		<xs:complexType>
			<xs:attribute name="Code" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Context">
		<xs:annotation>
			<xs:documentation>Information on where the promotion is being displayed, used for reporting.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="ID" use="required">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Type" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates the context where the response is displayed. e.g. web, email, banner</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="web"/>
						<xs:enumeration value="email"/>
						<xs:enumeration value="banner"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Level" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether the response display is at PromotionProfile, TripProfile or Promotion level.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="PromotionProfile"/>
						<xs:enumeration value="TripProfile"/>
						<xs:enumeration value="Promotion"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Sequence" use="optional">
				<xs:annotation>
					<xs:documentation>Sequence Id of the point on the page where the response is to be displayed.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Page" use="optional">
				<xs:annotation>
					<xs:documentation>Id of the page where the response is to be displayed.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Display" use="required">
				<xs:annotation>
					<xs:documentation>Indicates whether the response is to be displayed to customers or not.  If its not then the other attributes are not required.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:boolean"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Number" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates how many responses are to be displayed.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Session">
		<xs:annotation>
			<xs:documentation>Information on the session that the promotion is being displayed in.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="ID" use="required">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Dates">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SalesDateRange">
					<xs:annotation>
						<xs:documentation>Container for sales date associated with this Landing page.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="StayDateRange"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SalesDateRange">
		<xs:complexType>
			<xs:attribute name="Start_display" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Start" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:date"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="End_display" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="End" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:date"/>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="DestActivityDescriptiveContents" type="DestActivityDescriptiveContentsType"/>
	<xs:element name="SearchResponses">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SearchResponse" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
