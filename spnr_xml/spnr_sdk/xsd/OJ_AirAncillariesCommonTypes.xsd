										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="Category" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation>Category for this service e.g. 'baggage'</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Code" type="xs:string">
						<xs:annotation>
							<xs:documentation>Unique code for a this service</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ProductAvailability" type="xs:boolean" use="required">
						<xs:annotation>
							<xs:documentation>Indicates whether this service is available for this origin/destination</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ServiceCode" type="xs:string">
						<xs:annotation>
							<xs:documentation>Unique code for a this service</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AncillaryImage">
		<xs:annotation>
			<xs:documentation>Common type for the Ancillary Image element. This is used when storing and reading the ancillary image information from the database in Air Ancillary Manager.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ImageURL" type="xs:string">
				<xs:annotation>
					<xs:documentation>URL for the full size image for the ancillary. This may be either a external URL or a local URL.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ImageThumbnailURL" type="xs:string">
				<xs:annotation>
					<xs:documentation>URL for the thumbnail size image for the ancillary. This may be either a external URL or a local URL.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ID"/>
		<xs:attribute name="ContentData" type="ContentDataType" use="required">
			<xs:annotation>
				<xs:documentation>The ContentData attribute is necessary to indicate if the image is 'local' or 'remote'.

					local: image is uploaded and stored on our server.
					remote: image is a URL to an external site. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AncillaryMultimediaObjectType">
		<xs:annotation>
			<xs:documentation>Common type for the Ancillary Multimedia Object element. This is used when storing and reading the ancillary multimedia information from the database in Air Ancillary Manager.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="URL" type="xs:string">
				<xs:annotation>
					<xs:documentation>URL for the multimedia object for the ancillary. This may be either a external URL or a local URL.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdjustedCurrencyAmountType">
		<xs:complexContent>
			<xs:extension base="CurrencyAmountType">
				<xs:attribute name="Adjusted" type="xs:float"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DateRangeOJType">
		<xs:annotation>
			<xs:documentation>Date range as date type.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="From" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation>Defines range start date.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="To" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation>Defines range end date.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="IntegerRangeOJType">
		<xs:annotation>
			<xs:documentation>Range as integer type.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="From" type="xs:int" use="required"/>
		<xs:attribute name="To" type="xs:int" use="required"/>
	</xs:complexType>
	<xs:complexType name="DictionaryOJType">
		<xs:annotation>
			<xs:documentation>Stores element with specified code.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Code" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Value stored by dictionary.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ItineraryPassengersType">
		<xs:annotation>
			<xs:documentation>Stores types and quantity of passengers selected for flight.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Code" type="TravelerCodes" use="required">
			<xs:annotation>
				<xs:documentation>Passenger type code.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Quantity" type="xs:nonNegativeInteger" use="required">
			<xs:annotation>
				<xs:documentation>Number of passengers with given type.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="FlightSegmentWithPrice">
		<xs:annotation>
			<xs:documentation>Ancillary MetaData relating to the flight segment.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Prices" type="AirAncillaryProductPricesType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Terminal" type="xs:string">
			<xs:annotation>
				<xs:documentation>This defines the terminal used for this component.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AirportLocationCode" type="xs:string">
			<xs:annotation>
				<xs:documentation>Segment departure or arrival airport code. Example: CDG.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Airport" type="xs:string">
			<xs:annotation>
				<xs:documentation>This defines the airport used for this component.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AvailRuleID" type="xs:long" use="optional">
			<xs:annotation>
				<xs:documentation>Define unique identifier of fulfilled availability rule.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductAvailability" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>This defines the product availability on flight segment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FlightNumber" type="FlightNumberType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">The flight number of the flight. This is required for use with scheduled airline messages
					but may be omitted for requests by tour operators.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CabinClass" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Cabin class code of this segment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AvailabilityDateTime" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation>The date and time when product is available. This value determines a date for inventory operations.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Direction" type="AirportAncillaryDirection">
			<xs:annotation>
				<xs:documentation>The direction of travel. e.g. Arrive, Depart.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FlightSegmentRPH" type="RPH_Type">
			<xs:annotation>
				<xs:documentation>Optional Flight Segment RPH</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AncillaryAvailBaggageType">
		<xs:annotation>
			<xs:documentation>Baggage ancillary type definition mirrors the NDC BaggageAllowanceRS schema definition without using NDC namespace.

Please see Baggage_CommonTypes.xsd and BaggageSimpleTypes.xsl fir further information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="InfoGroup" minOccurs="0"/>
			<xs:element name="USDOT" type="USDOTType"/>
			<xs:element name="CurrencyCode" type="CurrencyCodeType" minOccurs="0"/>
			<xs:element name="OriginDestination" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Allowance" type="AllowanceType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Requested checked baggage allowance for any of the selected flights.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CarryOnAllowance" type="CarryOnAllowanceType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Requested carry-on baggage allowance for any of the selected flights.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element ref="MediaContent" minOccurs="0"/>
						<xs:element name="Service" type="AirAncillariesServiceType" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attributeGroup ref="DepartureArrivalCode"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AncillaryAvailBoundProductsType">
		<xs:annotation>
			<xs:documentation>Air Ancillary Type Product, Bound Products, Groups Products by Category, Origin/Destination and Date</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AncillaryProducts" id="AncillaryProducts" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="AncillaryProductsType">
							<xs:sequence>
								<xs:element name="Flights" minOccurs="0" maxOccurs="1">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Flight" minOccurs="1" maxOccurs="unbounded">
												<xs:complexType>
													<xs:attribute name="FlightNumber" type="xs:int" use="required"/>
													<xs:attribute name="SegmentRef"/>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="PTCs" type="PTCs" minOccurs="1" maxOccurs="1"/>
							</xs:sequence>
							<xs:attribute name="Carrier" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation>Defines the carrier</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Date" type="xs:date" use="optional">
								<xs:annotation>
									<xs:documentation>Defines availability date</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Duration" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation>Defines flight duration</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Category" type="AncillaryProductCategoryType" use="optional">
								<xs:annotation>
									<xs:documentation>Defines products category</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Flight" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation>Defines flight numbers</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Terminal" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation>Defines terminal related with products</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Direction" type="AirportAncillaryDirection" use="optional">
								<xs:annotation>
									<xs:documentation>Define direction for airport products</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="RBD" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation>Reservation Booking Designator (RBD) code. A reservation booking code for a flight segment.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Category" type="AncillaryProductCategoryType" use="optional">
			<xs:annotation>
				<xs:documentation>Defines products category</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Date" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation>Defines availability date</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Destination" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Flight destination</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Origin" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Flight origin</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="JourneySegmentType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Journey Segment information which may be a train and/or a bus segment.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BookingOffices" type="BookingOfficesType" minOccurs="0"/>
			<xs:choice>
				<xs:element name="TrainSegment" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="en">Train segment information.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="TrainSegmentType">
								<xs:sequence>
									<xs:element name="TrainOrigin" type="LocationType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="en">Represents the start location of this train service.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="TrainDestination" type="LocationType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="en">Represents the final destination location of this train service.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="RailAmenities" type="RailAmenityType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="en">A collection of preferred or offered rail amenities, including an amenity code, quantity and/or an associated preference level.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="AvailabilityDetail" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation xml:lang="en">Specific inventory-controlled service class code or detailed accommodation information, class and passenger type fares, and other service-related information including reservation class, reservation type, different class codes returned indicator, and autotrain vehicle type. This information collectively represents one or more specific classes of service, such as "First class" and/or a seat and berth  accommodation.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="ClassCode" minOccurs="0">
													<xs:annotation>
														<xs:documentation xml:lang="en">The class of inventory controlled services available.</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:complexContent>
															<xs:extension base="ClassCodeType"/>
														</xs:complexContent>
													</xs:complexType>
												</xs:element>
												<xs:element name="Reference" type="xs:string" minOccurs="0">
													<xs:annotation>
														<xs:documentation xml:lang="en">A reference code for the available service(s) returned in this message that may be used in a booking message. This is used as a reference number in the booking request so availability service details do not need to be replicated in the booking request message.</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element ref="AncillaryService" minOccurs="0"/>
												<xs:element name="Accommodation" type="AccommodationType" minOccurs="0">
													<xs:annotation>
														<xs:documentation xml:lang="en">Type, class and compartment categories of train accommodations.</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
											<xs:attribute name="ReservationClass" type="xs:string" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="en">Specifies class, such as Coach, Premium and Sleeper.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="ReservationType" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="en">Specifies if the class code is reserved or unreserved.</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="Reservable"/>
														<xs:enumeration value="NotReservable"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="VehicleType" type="xs:string" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="en">The vehicle type for autotrains, such as a Channel Tunnel, including car, van, motorcycle, etc.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="IsBicycleInd" type="xs:boolean" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="en">When true, the class code is for a bicycle which many types of trains accommodate.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="AlternativeInd" type="xs:boolean" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="en">When true, the class code returned is different from the requested class code.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
									<xs:element name="VendorMessages" type="VendorMessagesType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="en">A collection of special comments on the segment.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Remarks" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="en">A collection of remarks about the segment.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Remark" type="WarningType" maxOccurs="99">
													<xs:annotation>
														<xs:documentation xml:lang="en">A remark about the segment.</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="OnTimeRate" type="Percentage" use="optional">
									<xs:annotation>
										<xs:documentation> Specifies the Dept. of Transport on time rate of the  train.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="AlternativeInd" type="xs:boolean" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">When true, indicates that the priced segment information returned was not the same as requested segment.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="RestrictedServiceInd" type="xs:boolean" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">When true, indicates that this is a restricted service.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="CancelledInd" type="xs:boolean" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">When true, indicates that a segment has been cancelled.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="BusSegment" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="en">Bus segment information which may be hosted in the rail provider system or a code share.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="BusSegmentType">
								<xs:sequence>
									<xs:element name="ClassCodes" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation xml:lang="en">The class of inventory controlled services available.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="ClassCodeType">
													<xs:attribute name="ReservationClass" type="xs:string" use="required">
														<xs:annotation>
															<xs:documentation xml:lang="en">Specifies class, such as Coach, Premium and Sleeper.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
													<xs:attribute name="ReservationType" type="xs:string" use="required">
														<xs:annotation>
															<xs:documentation xml:lang="en">Specifies the type of the reservation.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
													<xs:attribute name="VehicleType" type="xs:string" use="optional">
														<xs:annotation>
															<xs:documentation xml:lang="en">Specifies the type of the vehicle or bus.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
													<xs:attribute name="AlternativeInd" type="xs:boolean" use="optional">
														<xs:annotation>
															<xs:documentation xml:lang="en">When true, this is an alternate form of transportation.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="Remarks" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="en">Remarks about the bus segment.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Remark" type="WarningType" maxOccurs="99"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="FareOption" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TotalFare">
							<xs:annotation>
								<xs:documentation xml:lang="en">Encapsulates the total price for all adults and all children, or all tickets (in the case of a multi-passenger ticket). If the total fare cannot be calculated, the reason is given.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="AlternateCurrencyTotal" minOccurs="0" maxOccurs="unbounded">
										<xs:complexType>
											<xs:attributeGroup ref="CurrencyAmountGroup"/>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attributeGroup ref="CurrencyAmountGroup"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="Accommodation" type="AccommodationType">
							<xs:annotation>
								<xs:documentation xml:lang="en">The class of inventory controlled services available.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PriceDetail" maxOccurs="99">
							<xs:annotation>
								<xs:documentation xml:lang="en">Pricing details calculated at the origin/destination level.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="RailPriceType"/>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="OperationScheduleRPH" type="RPH_Type">
						<xs:annotation>
							<xs:documentation xml:lang="en">The index number of an operation schedule.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="RPH" type="RPH_Type" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">The index number of a journey segment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SupplierCode" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The supplier identifier of a ground transport service journey segment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="GroundTransportServiceType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Available transport information, including type of transport service, service details, service restrictions, charges and fees and payment rules.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="JourneySegment" type="JourneySegmentType" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="RPH" type="RPH_Type" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">The index number of a ground transport service journey segment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- ==================================================================== -->
	<xs:complexType name="AncillarySearchCriteriaType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Identifies a collection of criteria for ancillary search.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="GroundTransportServiceCriteria" type="GroundTransportServiceCriteriaType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="AirCriteria" type="AirSearchCriteriaType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PriceCriteria" type="PriceCriteriaType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="LoungeCriteria" type="LoungeCriteriaType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GroundTransportServiceCriteriaType">
		<xs:annotation>
			<xs:documentation xml:lang="en">A collection of criteria for identifying the preferred ground transport service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BusCriteria" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en"> Identifies the bus info.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="FromDeadline" type="DeadlineType" minOccurs="0" maxOccurs="1"/>
						<xs:element name="ToDeadline" type="DeadlineType" minOccurs="0" maxOccurs="1"/>
					</xs:sequence>
					<xs:attribute name="MaxSchedules" type="xs:integer" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The max quantity of schedules to return.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="LocationCode" type="StringLength1to8" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en"> Location code used to identify a specific airport.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CodeContext" type="StringLength1to32" use="optional" default="IATA">
			<xs:annotation>
				<xs:documentation xml:lang="en">Identifies the context of the identifying code, such as IATA, ARC, or internal code, etc.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Direction" type="AirportAncillaryDirection" use="optional">
			<xs:annotation>
				<xs:documentation>Define direction for ancillary products</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AirSearchCriteriaType">
		<xs:annotation>
			<xs:documentation xml:lang="en">A collection of criteria for specifying flight itinerary to be searched for.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AirItinerary" type="AirItineraryType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en"> Specify specific flight information for the itinerary. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TravelerInfoSummary" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Specify the number of passengers and types for Air Low Fare Search</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="TravelerInfoSummaryType">
							<xs:attribute name="TicketingCountryCode" type="ISO3166" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">To specify the country code where ticketing of the reservation will take place.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="SpecificPTC_Indicator" type="xs:boolean" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">If true, this request is for a specific PTC and only fares applicable to that PTC will be checked and returned.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
            <xs:element name="AirItineraryPricingInfo" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en"> Pricing Information for an Air Itinerary.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="AirItineraryPricingInfoType">
                            <xs:attributeGroup ref="AirItineraryPricingInfoGroup"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
		</xs:sequence>
	</xs:complexType>

    <!-- ==================================================================== -->
    <xs:complexType name="PriceCriteriaType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used to search a particular kind of ancillary product by price.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="PriceRange" type="PriceRangeType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="PriceRangeType">
        <xs:annotation>
            <xs:documentation xml:lang="en">A range of price.</xs:documentation>
        </xs:annotation>
		<xs:attributeGroup ref="CurrencyCodeGroup"/>
		<xs:attribute name="MinAmount" type="xs:float" use="optional"/>
		<xs:attribute name="MaxAmount" type="xs:float" use="optional"/>
    </xs:complexType>
	<xs:complexType name="LoungeCriteriaType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used to search lounge service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Airport" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Airport information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attributeGroup ref="AirportLocationGroup"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="TravelDateTimeSpan" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Travel date time span information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attributeGroup ref="DateTimeSpanGroup"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
