<xs:schema xmlns="http://www.opentravel.org/OTA/2003/05" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.opentravel.org/OTA/2003/05" elementFormDefault="qualified" version="2.005" id="OTA2006B">
	<xs:include schemaLocation="OTA_AirCommonTypes.xsd"/>
	<xs:include schemaLocation="OJ_Baggage_CommonTypes.xsd"/>
	<xs:include schemaLocation="SupplierInfoCommon.xsd"/>
	<xs:include schemaLocation="OJ_Baggage_SimpleTypes.xsd"/>
	<xs:include schemaLocation="OJ_CommonTypes.xsd"/>
	<xs:include schemaLocation="OTA_RailCommonTypes.xsd"/>
	<!-- ==================================================================== -->
	<!-- Simple type for product category.                                    -->
	<!-- ==================================================================== -->
	<xs:simpleType name="AncillaryProductCategoryType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="segment"/>
			<xs:enumeration value="journey"/>
			<xs:enumeration value="airport"/>
			<xs:enumeration value="baggage"/>
			<xs:enumeration value="seat"/>
			<xs:enumeration value="bound"/>
			<xs:enumeration value="CrossSell"/>
			<xs:enumeration value="BrandedFare"/>
			<xs:enumeration value="itinerary"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ==================================================================== -->
	<!-- Simple type for airport ancillary Direction.                         -->
	<!-- ==================================================================== -->
	<xs:simpleType name="AirportAncillaryDirection">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Arrive"/>
			<xs:enumeration value="Connection"/>
			<xs:enumeration value="Depart"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ==================================================================== -->
	<!-- Simple type for ground transport route Direction.                         -->
	<!-- ==================================================================== -->
	<xs:simpleType name="GroundTransportRouteDirectionType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Arrive"/>
			<xs:enumeration value="Depart"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ==================================================================== -->
	<!-- Simple type for product status.                                      -->
	<!-- ==================================================================== -->
	<xs:simpleType name="AncillaryProductStatusType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="enabled"/>
			<xs:enumeration value="disabled"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ProductAuditType">
		<xs:attribute name="Action"/>
		<xs:attribute name="Code"/>
		<xs:attribute name="Entity"/>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- Basic summary of an air ancillary product.                           -->
	<!-- ==================================================================== -->
	<xs:complexType name="AirAncillaryProductSummaryType">
		<xs:attribute name="Name" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The name of the ancillary product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Code" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The AAM code for the ancillary product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Status" type="AncillaryProductStatusType" use="optional">
			<xs:annotation>
				<xs:documentation>The status of the product: enabled, disabled, any.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Category" type="AncillaryProductCategoryType">
			<xs:annotation>
				<xs:documentation>Product category: '&lt;b&gt;segment&lt;/b&gt;' for products linked to a single flight segment (ex.: in-flight entertainment), '&lt;b&gt;journey&lt;/b&gt;' for products that span across all/many flight segments (ex.: baggage) and '&lt;b&gt;airport&lt;/b&gt;' for products at a specific airport/terminal (ex.: lounge access).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Type" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Product code for a specific type, such as ABC for lounge access, or XYZ for in-flight entertainment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SubType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Meal subtype</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ID">
			<xs:annotation>
				<xs:documentation>Unique ID for this ancillary</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SupplierProductCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The product identification code within the supplier.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- Basic summary of an air ancillary product supplier.                  -->
	<!-- ==================================================================== -->
	<xs:complexType name="AirAncillarySupplierSummaryType">
		<xs:attribute name="Code" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The code for the supplier that provides the ancillary product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- Base definition for all ancillary product summary types.             -->
	<!-- ==================================================================== -->
	<xs:complexType name="AirAncillariesProductSummaryBase"/>
	<!-- ==================================================================== -->
	<!-- Flight-segment related product search criteria.                      -->
	<!-- ==================================================================== -->
	<xs:complexType name="AirAncillariesFlightProductSummaryType">
		<xs:complexContent>
			<xs:extension base="AirAncillariesProductSummaryBase">
				<xs:sequence>
					<xs:element name="FlightSegment">
						<xs:annotation>
							<xs:documentation>Search criteria for a specific flight segment.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:attribute name="OriginAirport" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation>IATA-code for the origin airport.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="DestinationAirport" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation>IATA-code for the destination airport.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Carrier" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation>IATA-code for the operating carrier.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="FlightNumber" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation>Flight number (excluding carrier code).</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- Journey related product search criteria.                             -->
	<!-- ==================================================================== -->
	<xs:complexType name="AirAncillariesJourneyProductSummaryType">
		<xs:complexContent>
			<xs:extension base="AirAncillariesProductSummaryBase">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- Airport/terminal related product search criteria.                    -->
	<!-- ==================================================================== -->
	<xs:complexType name="AirAncillariesAirportProductSummaryType">
		<xs:complexContent>
			<xs:extension base="AirAncillariesProductSummaryBase">
				<xs:sequence>
					<xs:element name="Airport">
						<xs:annotation>
							<xs:documentation>Search criteria for a specific airport/terminal.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:attribute name="Code" type="xs:string">
								<xs:annotation>
									<xs:documentation>IATA code for the airport the product is sold at.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Terminal" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation>Terminal number/name the product is sold at.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- Definition of an air ancillary bundle product.                              -->
	<!-- ==================================================================== -->
	<xs:complexType name="AirAncillaryBundleProductType">
		<xs:sequence>
			<xs:element name="Content" type="AirAncillaryProductContentType">
				<xs:annotation>
					<xs:documentation>Element containing detailed information about bundle</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Lounge">
				<xs:annotation>
					<xs:documentation>Element containing detailed information about lounge product</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Prices" type="AirAncillaryProductPricesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Element containing list of prices</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TravelerInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Element containing AirTraveler</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AirTraveler" type="AirTravelerType">
							<xs:annotation>
								<xs:documentation>Element containing detailed information about Air Traveler</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AncillaryProducts" type="AncillaryProductsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>List of ancillary products that are part of the bundle</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Attributes" type="BundleAttributesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A group of Bundle of Attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PassengerTypes" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>List of passenger types allowed to buy this bundle</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PassengerType" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Passenger type allowed to buy this bundle.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="Code" type="TravelerCodes" use="required">
									<xs:annotation>
										<xs:documentation>Passenger type code.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Name" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The name of the ancillary product. This name is the name used by the product manager, and is not the localised display-name of the product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Code" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The AAM code for the ancillary product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Status" type="AncillaryProductStatusType" use="required">
			<xs:annotation>
				<xs:documentation>The status of the product: enabled, disabled, any.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BookingStatus" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The booking status of a product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BookingReferenceID" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The booking reference/ID for a booked product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Category" type="AncillaryProductCategoryType" use="required">
			<xs:annotation>
				<xs:documentation>Product category: '&lt;b&gt;segment&lt;/b&gt;' for products linked to a single flight segment (ex.: in-flight entertainment), '&lt;b&gt;journey&lt;/b&gt;' for products that span across all/many flight segments (ex.: baggage) and '&lt;b&gt;airport&lt;/b&gt;' for products at a specific airport/terminal (ex.: lounge access).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Type" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Product code for a specific type, such as ABC for lounge access, or XYZ for in-flight entertainment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductSequence" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Sequence number of a specific product, such as Meal 1.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SellingProposition" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Selling Proposition</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Tag" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Tag used to group products for display purposes in the front-end.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Available" type="xs:int" use="optional">
			<xs:annotation>
				<xs:documentation>Number of products available. This attribute does not occur for products that are free-sell (ie. not inventory controlled).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SSRCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>SSR code used for the specific booking channel (mapped by xDistributor rule from product type).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- Definition of an air ancillary product.                              -->
	<!-- ==================================================================== -->
	<xs:complexType name="AirAncillaryProductType">
		<xs:sequence>
			<xs:element name="Restrictions" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Set of restrictions applied to product.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PassengerType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Defines passenger type restriction applied to ancillary.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="Code" type="TravelerCodes" use="required">
									<xs:annotation>
										<xs:documentation>Passenger type code.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="CabinClass" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Defines cabin class restriction applied to ancillary.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="Code" type="xs:string" use="required">
									<xs:annotation>
										<xs:documentation>Cabin class code.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="LoyaltyPrograms" minOccurs="0" maxOccurs="1">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="LoyaltyProgram" type="LoyaltyProgramType" minOccurs="1" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element ref="TravelerIDRef" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ProductDescriptiveContent" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Baggage" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Baggage details </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="MaxWeight" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Maximum Weight allowed per piece.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="xs:decimal">
													<xs:attribute name="Unit" type="xs:string" use="required">
														<xs:annotation>
															<xs:documentation>Measurement unit
																Values are:
																KG
																LBS</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="MaxDimensions" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Maximum Size allowed per Piece. </xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Length" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Value e.g 40 </xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:simpleContent>
															<xs:extension base="xs:decimal">
																<xs:attribute name="Type" type="xs:string">
																	<xs:annotation>
																		<xs:documentation>Provides a text description of the type of length included in the response for the bag type.                                                                Example - Linear  </xs:documentation>
																	</xs:annotation>
																</xs:attribute>
															</xs:extension>
														</xs:simpleContent>
													</xs:complexType>
												</xs:element>
												<xs:element name="Height" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Value e.g 40 </xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:simpleContent>
															<xs:extension base="xs:decimal">
																<xs:attribute name="Type" type="xs:string">
																	<xs:annotation>
																		<xs:documentation>Provides a text description of the type of height included in the response for the bag type.                                                                Example - Linear  </xs:documentation>
																	</xs:annotation>
																</xs:attribute>
															</xs:extension>
														</xs:simpleContent>
													</xs:complexType>
												</xs:element>
												<xs:element name="Width" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Value e.g 40 </xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:simpleContent>
															<xs:extension base="xs:decimal">
																<xs:attribute name="Type" type="xs:string">
																	<xs:annotation>
																		<xs:documentation>Provides a text description of the type of width included in the response for the bag type.                                                                Example - Linear  </xs:documentation>
																	</xs:annotation>
																</xs:attribute>
															</xs:extension>
														</xs:simpleContent>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
											<xs:attribute name="Units" type="xs:string" use="required">
												<xs:annotation>
													<xs:documentation>Measurement unit
														Values are:
														IN
														CM</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="LCM" type="xs:integer">
												<xs:annotation>
													<xs:documentation>Loose Cubic Meter(metric measurement)</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
									<xs:element name="MinWeight" type="BaggageWeightType" minOccurs="0" maxOccurs="unbounded"/>
									<xs:element name="MinDimensions" type="BaggageDimensionsType" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Minimum Size requirement of the Sub Code for which the baggage allowance/charge applies</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ExcessFirst" type="xs:integer" minOccurs="0">
										<xs:annotation>
											<xs:documentation>represent the first occurrence of the excess bag.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ExcessLast" type="xs:integer" minOccurs="0">
										<xs:annotation>
											<xs:documentation>represent the first occurrence of the excess bag.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="SupplierCode"/>
					<xs:attribute name="Tag"/>
					<xs:attribute name="Type"/>
					<xs:attribute name="CategoryType" use="required"/>
					<xs:attribute name="Name" use="required"/>
					<xs:attribute name="Code" use="required"/>
					<xs:attribute name="InventoryType" use="required"/>
					<xs:attribute name="Enabled" use="required"/>
					<xs:attribute name="IATACode"/>
					<xs:attribute name="Priority"/>
					<xs:attribute name="ProfileType"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Content" type="AirAncillaryProductLocalisedContentType" minOccurs="0"/>
			<xs:element name="CarbonOffsets" type="AirAncillaryOffsetProductType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="DefaultContent" type="AirAncillaryProductLocalisedContentType" minOccurs="0"/>
			<xs:element name="EligibleItem" type="AirAncillaryEligibleItemType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Lounge" type="AirAncillaryLoungeProductType" minOccurs="0"/>
			<xs:element name="AncillaryProducts" type="AncillaryProductsType" minOccurs="0"/>
			<xs:element name="Prices" type="AirAncillaryProductPricesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Won't be present in reference bundles</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaxChangeFees" type="AirAncillaryProductPaxChangeFeesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Models the fee for pricing the swap of chargeable seats.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TravelerInfo" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AirTraveler" type="AirTravelerType"/>
						<xs:element name="SpecialReqDetails" type="SpecialReqDetailsType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="en">Provides detailed information regarding any special needs, requests, or remarks associated with the traveler</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Attributes" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Attribute" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="Code" type="xs:string" use="required">
									<xs:annotation>
										<xs:documentation>Code of the attribute</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Name" type="xs:string" use="required">
									<xs:annotation>
										<xs:documentation>Description of this configuration</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ProductGroups" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ProductGroup" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Content" type="AirAncillaryProductLocalisedContentType"/>
								</xs:sequence>
								<xs:attribute name="GroupCode" type="xs:string" use="required">
									<xs:annotation>
										<xs:documentation>Group Code for the Product Group</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="GroupsCode" type="xs:string">
						<xs:annotation>
							<xs:documentation>Groups Code for the Product main category</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="ExchangeRate" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Stores the exchange rate between vendor and supplier currencies</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attributeGroup ref="ExchangeRateGroup"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="FlightSegment" type="FlightSegmentWithPrice" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="GroundTransportService" type="GroundTransportServiceType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Available transport information, including type of transport service, service details, service restrictions, charges and fees and payment rules.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ServiceRPHs" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Reference pointers to services in the flight product</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ServiceRPH" type="RPH_Type" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="en">Reference to the services for this ancillary in the flight</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Contact" type="ContactPersonType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Contact information for this booking. May be either one of the travelers as specified by the RPH or a new set of contact information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TravelerInputs" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TravelerInput" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="Code" type="xs:string" use="required">
									<xs:annotation>
										<xs:documentation xml:lang="en">Code of the Input field Info, e.g. MobileNumber, Email, Address, etc.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Name" type="xs:string" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">User friendly description of the Customer Input field.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Mandatory" type="xs:boolean" use="required">
									<xs:annotation>
										<xs:documentation xml:lang="en">Indicates if the input field is an optional or required input in order to purchase the Ancillary product.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Loyalty" type="LoyaltyDetailsPricingType" minOccurs="0"/>
			<xs:element name="SupplierBookingReferenceID" type="SupplierBookingReferenceIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">A confirmation number by which this reservation can be uniquely identified.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="QRData" type="QRDataType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The QR information provided by supplier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OriginalFlightSegment" type="BookFlightSegmentType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Original Flight Segment information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SupplierDescriptiveContent" type="SupplierDescriptiveContentType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Version">
			<xs:annotation>
				<xs:documentation>The version of the ancillary product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ID">
			<xs:annotation>
				<xs:documentation>The ID of the ancillary product. This will support same code with multiple version</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Name" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The name of the ancillary product. This name is the name used by the product manager, and is not the localised display-name of the product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Code" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The AAM code for the ancillary product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MaxItemsPerPax" use="optional">
			<xs:annotation>
				<xs:documentation>Maximum number of products that can be booked by passenger.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:minInclusive value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Status" type="AncillaryProductStatusType" use="optional">
			<xs:annotation>
				<xs:documentation>The status of the product: enabled, disabled, any.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BookingStatus" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The booking status of a product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BookingReferenceID" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The booking reference/ID for a booked product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Category" type="AncillaryProductCategoryType" use="required">
			<xs:annotation>
				<xs:documentation>Product category: '&lt;b&gt;segment&lt;/b&gt;' for products linked to a single flight segment (ex.: in-flight entertainment), '&lt;b&gt;journey&lt;/b&gt;' for products that span across all/many flight segments (ex.: baggage) and '&lt;b&gt;airport&lt;/b&gt;' for products at a specific airport/terminal (ex.: lounge access).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Type" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Product code for a specific type, such as ABC for lounge access, or XYZ for in-flight entertainment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Tag" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Tag used to group products for display purposes in the front-end.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Available" type="xs:int" use="optional">
			<xs:annotation>
				<xs:documentation>Number of products available. This attribute does not occur for products that are free-sell (ie. not inventory controlled).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SSRCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>SSR code used for the specific booking channel (mapped by xDistributor rule from product type).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ExcessLast" type="xs:int" use="optional">
			<xs:annotation>
				<xs:documentation>Last occurrence of the excess bag.
					Example: 3</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ExcessFirst" type="xs:int" use="optional">
			<xs:annotation>
				<xs:documentation>First occurrence of the excess bag.
					Example: 3</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Quantity" type="xs:int" use="optional">
			<xs:annotation>
				<xs:documentation>Selected quantity of a given ancillary product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BookingChannel" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>This defines the supplier channel used for this component</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Airport" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>This defines the airport used for this component</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FlightNumber" type="FlightNumberType" use="optional">
			<xs:annotation>
				<xs:documentation>This defines the flight number used for this component</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CabinClass" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>This defines the cabin class used for this component</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Terminal" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>This defines the terminal used for this component</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Carrier" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>This defines the carrier used for this component</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Priority" type="xs:int" use="optional">
			<xs:annotation>
				<xs:documentation>Priority code</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProfileType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Defines profile type for the component</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Date" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation>Date of the flight</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SubType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Ancillary Subtype</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SellingProposition" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Selling Proposition</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductAvailability" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Product Availability</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Direction" type="AirportAncillaryDirection" use="optional">
			<xs:annotation>
				<xs:documentation>Define direction for airport products</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductSequence" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Define sequence code</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SupplierCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Supplier code</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SupplierProductCode" type="xs:string">
			<xs:annotation>
				<xs:documentation>Code of ancillary in supplier's system.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductRPH" type="RPH_Type" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">The reference place holder to identify ancillary product in response.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BundleCode" type="xs:string">
			<xs:annotation>
				<xs:documentation>Code of bundle when ancillary is part of the bundle</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Origin" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Origin location of the flight</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Destination" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Destination location of the flight</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OriginDestinationRPH" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>RPH attribute from OriginDestinationOption section from this ancillary's flight segment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FlightSegmentRPH" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>RPH attribute from FlightSegment section from this ancillary's flight segment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="VoucherRequired" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Indicates if voucher issuance is required for this ancillary.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="TSAncillaryProductGroup"/>
		<xs:attribute name="AvailableFrom" type="xs:dateTime" use="optional">
			<xs:annotation>
				<xs:documentation>For some ancillary products, e.g. Free Change and Free Refund, this represents the start of time window during which this ancillary can be used.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AvailableTo" type="xs:dateTime" use="optional">
			<xs:annotation>
				<xs:documentation>For some ancillary products, e.g. Free Change and Free Refund, this represents the end of time window during which this ancillary can be used.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ConsumptionStartDate" type="xs:dateTime" use="optional">
			<xs:annotation>
				<xs:documentation>The consumption start date and time</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ConsumptionEndDate" type="xs:dateTime" use="optional">
			<xs:annotation>
				<xs:documentation>The consumption end date and time</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- ==================================================================== -->
	<xs:complexType name="AirAncillaryProductLocalisedContentType">
		<xs:complexContent>
			<xs:extension base="AirAncillaryProductContentType">
				<xs:attribute name="Language" type="xs:string">
					<xs:annotation>
						<xs:documentation>Language code of the content</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- ==================================================================== -->
	<xs:complexType name="AirAncillaryProductContentType">
		<xs:sequence>
			<xs:element name="ProductName" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Localised display-name for the product.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShortContent" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Localised short description of the product.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LongContent" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Localised long description of the product.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Image" type="AncillaryImage" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Images of/for the product.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductTag" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tag of/for the product.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- Ancillary product carbon offset definition.                         -->
	<!-- ==================================================================== -->
	<xs:complexType name="AirAncillaryOffsetProductType">
		<xs:sequence>
			<xs:element name="CarbonOffset" type="CarbonOffsetType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Unit" type="WeightUnitType">
			<xs:annotation>
				<xs:documentation>The weight unit of carbon offset usually measured in KG.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TotalAmount" type="xs:float">
			<xs:annotation>
				<xs:documentation>The carbon offset amount.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- Carbon offset type definition.-->
	<xs:complexType name="CarbonOffsetType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The carbon offset calculated based on journey, class and number of passengers.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Price" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="AirAncillaryProductPriceType">
							<xs:sequence>
								<xs:element name="Loyalty" type="LoyaltyProductType" minOccurs="0" maxOccurs="1">
									<xs:annotation>
										<xs:documentation xml:lang="en">The amount of Loyalty Points that the customer has earned and/or redeemed in relation to this product</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Unit" type="WeightUnitType">
			<xs:annotation>
				<xs:documentation>The weight unit of carbon offset usually measured in KG.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Amount" type="xs:float">
			<xs:annotation>
				<xs:documentation>The carbon offset amount.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FlightSegmentRPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation>RPH attribute from FlightSegment section from this ancillary's flight segment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Quantity" type="xs:positiveInteger" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The quantity for the carbon offset.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- One price two seat type definition.-->
	<xs:complexType name="AirAncillaryEligibleItemType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The eligible item of the particular ancillary product.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="Air" type="EligibleItemAirType" minOccurs="0" maxOccurs="1"/>
			</xs:choice>
			<xs:element name="EligiblePricing" type="EligiblePricingType" minOccurs="0"  maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EligibleItemAirType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The air content of the eligible item.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AirItineraryPricingInfo" type="AirItineraryPricingInfoType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="RPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A reference to eligible pricing.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- Eligible pricing type.-->
	<xs:complexType name="EligiblePricingType">
		<xs:sequence>
			<xs:element name="BaseFare" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Price of the inventory excluding taxes and fees.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attributeGroup ref="CurrencyAmountGroup"/>
					<xs:attributeGroup ref="CurrencyConversionGroup"/>
					<xs:attribute name="Adjusted" type="xs:float" use="optional"/>
					<xs:attribute name="OriginalPrice" type="Money" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">A description of the base amount original fare, a base amount fare before price discount.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="DifferFare" type="Money" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The upgrade amount of the fare.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ReissueFee" type="Money" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The daychange amount of the fare.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="RefundAmount" type="Money" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The refundable money.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="UsedAmount" type="Money" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The fare already used for the flight segment.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="NonRefundableAmount" type="Money" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">Non refundable amount for the flight segment.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="RoundTripAmount" type="Money" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The round trip fare.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="PriceAdjustments" type="exPriceAdjustmentsType" minOccurs="0"/>
			<xs:element name="TotalFare" minOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">The total price that the passenger would pay (includes fare, taxes, fees)  </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Loyalty" type="LoyaltyPricingType" minOccurs="0"/>
					</xs:sequence>
					<xs:attributeGroup ref="CurrencyAmountGroup"/>
					<xs:attribute name="Adjusted" type="xs:string" use="optional"/>
					<xs:attribute name="OriginalPrice" type="Money" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">A description of the total original fare, a total fare before price discount.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Currency" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The code specifying a monetary unit. Use ISO 4217, three alpha code. This attribute is deprecated, CurrencyCode should be used!</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ItemRPH_List" type="ListOfRPH">
			<xs:annotation>
				<xs:documentation xml:lang="en">Reference to itinerary reservation item (e.g. Car, Hotel, Air, Package).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AssociatedItemRPH" type="ListOfRPH">
			<xs:annotation>
				<xs:documentation xml:lang="en">Reference to associated itinerary item (e.g. Travel Insurance).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- ==================================================================== -->
	<!-- ==================================================================== -->
	<!-- Ancillary product lounge definition.                                 -->
	<!-- ==================================================================== -->
	<xs:complexType name="AirAncillaryLoungeProductType">
		<xs:sequence>
			<xs:element name="CheckInTime" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The check in time for the lounge product.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attributeGroup ref="DeadlineGroup"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Rooms" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The lounge room collection of the product.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Room" type="AirAncillaryLoungeRoomType" minOccurs="1" maxOccurs="unbounded">
