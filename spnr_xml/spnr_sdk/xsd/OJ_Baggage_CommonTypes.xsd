	<xs:complexType name="CurrencyCodePOSType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Currency code and associated metadata, including number of decimals and exchange rate information.</xs:documentation>
		</xs:annotation>
