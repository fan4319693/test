	<xs:complexType name="AdvanceTicketingType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Advance ticketing restrictions.</xs:documentation>
			<xs:documentation source="GUIDELINES" xml:lang="en">=notes=

				1. The type is defined by the Unit attribute. If for example this number is 3 and the unit is D, the advance purchase restriction is 3 DAYS.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AdvanceReservation" type="AdvanceReservationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Advance reservation/ ticketing value.
						Example: 14</xs:documentation>
					<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, For ATPCO data processing, this is the Category 5 Advance Ticketing restrictions.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdvanceDeparture" type="AdvanceDepartureType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Advance departure value.
						Example: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Age" type="TravelerAgeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Traveler age.
				Example: 36</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="AgencyPartyType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Agency Party Identification information.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="UserID" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">User ID.
					Examples:
					JFFNL (Agent ID)
					jsmith (User ID)</xs:documentation>
				<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
					1. For an agency-based user, this is typically the Agent's ID.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="UserRole" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">User role.
					Example: AGENT</xs:documentation>
				<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
					1. This is typically set in system provider's requestor profile.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="UserPW" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">User password.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DutyCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Agent duty code.
					Example: UZ</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SignOn" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Agent sign on (SINE).
					Example: 6146TL</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LNIATA" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Agent CRT or terminal LNIATA number.
					Example: 956301007</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AggregatorPartyType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Aggregator Party Identification.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="AggregatorID" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">ID of the aggregating entity.
					Example: AA23</xs:documentation>
				<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
					1. The aggregating entity is typically a GDS, OTA or MetaSearch.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AggregatorPW" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Aggregator password.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AggregatorType" type="xs:string">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Aggregator type.
					Example:
					A (Airline)

					For maximum interoperability, refer to IATA PADIS Code List for data element 9972 - Originator Type Code.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AggregatorRole" type="AggregatorRoleType" use="required">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Aggregator role.
					Examples:
					A (AGGREGATOR COMMUNICATING WITH AIRLINE)
					R (AGGREGATOR COMMUNICATING WITH REQUESTOR)</xs:documentation>
				<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list.
					=enumerated values=
					A = AGGREGATOR COMMUNICATING WITH AIRLINE
					R = AGGREGATOR COMMUNICATING WITH REQUESTOR</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AirlineCarrierType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Carrier Airline information.</xs:documentation>
