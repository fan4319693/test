	<xs:complexType name="POIDistanceType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Point of interest distance.
				Example: 5.5</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:decimal">
				<xs:attribute name="POIType" type="POICategoryType" use="required">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Point of Interest type.
							Examples:
							AIRPORT
							CAR RENTAL LOCATION
							CINEMA
							CITY CENTER
							HIGHWAY
							LOCATION
							PETROL/ GAS STATION
							SHOPPING
							THEATER
							TRAIN STATION</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="POIName" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Point of interest name.
							Example: FRANKFURT/MAIN</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="OfferID">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Unique Offer ID.
				Example: 0981267543AQ

				=notes=
				1. This is a unique ID provided by the airline that may include Branded Fare(s); Ancillary Services(s); and/ or Ancillary Service Bundle(s).</xs:documentation>
			<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
				1. This unique Offer ID is limited to a maximum of 39 characters, so to ensure uniqueness between airlines in a multi-supplier scenario, it is recommended that the @Source attribute be used to identify the offering airline designator.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="OfferIDType">
					<xs:attribute name="OfferAction" use="optional">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Offer action.
								Examples:
								INCLUDE
								EXCLUDE
								REPRICE</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="EXCLUDE"/>
								<xs:enumeration value="INCLUDE"/>
								<xs:enumeration value="REPRICE"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="OfferIDType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Offer ID definition.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="OfferIDBaseType">
				<xs:attributeGroup ref="SourceReq"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="PriceRefKey" type="PriceRefKeyType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Unique price reference key.
				Example: P234567</xs:documentation>
			<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
				1. This key must be included in request message if present in previous offer (response) message.</xs:documentation>
		</xs:annotation>
	</xs:element>
