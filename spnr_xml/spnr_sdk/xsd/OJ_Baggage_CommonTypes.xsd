											<xs:documentation source="ENCODING" xml:lang="en">=emumerated values=
												PIECE = MAXIMUM PIECES
												WEIGHT = MAXIMUM WEIGHT
												BLANK - NOT SPECIFIED</xs:documentation>
										</xs:annotation>
