					<xs:element ref="FlightSegmentNumber" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="MaxDimensions" type="SPNRBaggageDimensionsType" minOccurs="0"/>
				</xs:sequence>
				<xs:attributeGroup ref="AllowanceAttributes"/>
			</xs:extension>
		</xs:complexContent>
