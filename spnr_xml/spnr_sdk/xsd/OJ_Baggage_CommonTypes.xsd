									<xs:element name="FlightNumber" type="FlightNumberType">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Flight number.
												Example: 1234</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="OperatingCarrier" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Flight operating carrier information.</xs:documentation>
							</xs:annotation>
