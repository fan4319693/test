		<xs:sequence>
			<xs:element ref="AirlineCode"/>
			<xs:element name="AirlineName" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Airline name.
						Example: UNITED AIRLINES</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="FlightNumber"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="AirlineCode" type="AirlineCodeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Airline designator code.
				Example: UA</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="AirlineIDType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Airline Identification information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="AirlineCode"/>
			<xs:element name="AirlineName" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Airline name.
						Example: UNITED AIRLINES</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AirlineOperatorCoreType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Operating Airline information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="AirlineCode"/>
			<xs:element name="AirlineName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Airline name.
						Example: UNITED AIRLINES</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="FlightNumber" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AirlineOperatorDetailType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Operating Airline information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="AirlineCode"/>
			<xs:element name="AirlineName" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Airline name.
						Example: UNITED AIRLINES</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="FlightNumber" minOccurs="0"/>
			<xs:element name="ClassOfService" type="FlightCOSType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Flight class of service.
						Example: B</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Comment" type="FreeFormTextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Free form text additional flight information.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AirlineOperatorIDType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Operating Airline information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="AirlineCode"/>
			<xs:element ref="FlightNumber" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="AirportCode" type="AirportCityCodeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Airport code.
				Example: CDG</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ArrivalCode" type="AirportCityCodeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Airport or city code.
				Example: MIA</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Attributes" type="AttributesType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">ATPCO S5 record attributes.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, ATPCO Attributes on the Services Record S5 that can be used as filter criteria for a service.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="AttributesType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">ATPCO S5 record attributes definition.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, ATPCO Attributes on the Services Record S5 that can be used as filter criteria for a service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Group">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Service group information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Code" type="xs:string">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">ATPCO service group code.
									Examples:
									BD (Bundled Service)
									BG (Baggage)
									GT (Ground Transportation and Non Air Services)
									IE (In-flight Entertainment)
									LG (Lounge)
									ML (Meal/Beverage)
									SA (Pre-reserved Seat Assignment)
									ST (Store)
									TS (Travel Services)

									For maximum interoperability, ATPCO Optional Services Industry Code encoding is recommended. Select a value from the enumerated list or enter a string value.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Text" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Service group description.
									Example: BAGGAGE OPTIONS</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="SubGroup" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Service subgroup information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Code" type="xs:string">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">ATPCO service group sub group code.
									Examples:
									CY (CARRY ON HAND BAGGAGE)
									IN (INFANT BAGGAGE)
									MI (MUSICAL INSTRUMENTS)
									SI (SPECIALTY ITEMS)
									SP (SPORTING EQUIPMENT)
									BU (BUS)
									HT (HOTEL)
									LI (CAR HIRE)
									TN (TRAIN)
									HS (HEADSET)
									IT (INTERNET ACCESS)
									PB (PILLOW AND BLANKET PACK)
									DR (AL - ALCOHOLIC DRINK)
									AP (APPAREL)
									PR (PRIORITY BOARDING)
									SY (PRIORITY SECURITY)

									For maximum interoperability, it is recommended that ATPCO Optional Services Industry Sub Codes values are used. Use an ATPCO defined GROUP SUB CODE or a string value.</xs:documentation>
								<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, Standard ATPCO Group and Sub Group on the Services Record S5 that can be used as filter criteria for a service.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Text" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Service subgroup description.
									Example: CARRY ON HAND BAGGAGE</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Desc1" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Description 1 codes.</xs:documentation>
					<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, ATPCO standard Description codes (or text translation for these codes) on the Optional Services Record S5.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Code" type="xs:string">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Description code.
									Example: 05 (UP TO 5 KG)</xs:documentation>
								<xs:documentation source="ENCODING" xml:lang="en">=sample values=
									05 = up to 5 kg
									1Y = One Year
									AI = Aisle
									B1 = First Bag</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Text" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Description code text.
									Example: UP TO 5 KG</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Desc2" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Description 2 codes.</xs:documentation>
					<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, ATPCO standard Description codes (or text translation for these codes) on the Optional Services Record S5.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Code" type="xs:string">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Description code.
									Example: HT (HOT)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Text" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Description code text.
									Example: HOT</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AutoExchangeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Auto Exchange Application information.</xs:documentation>
			<xs:documentation source="GUIDELINES" xml:lang="en">=notes=

				1. Auto exchange is sophisticated airfare shopping that enables customers to change reservations online for partially flown and wholly unflown tickets.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CurrencyCode" type="CurrencyCodeType">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Currency pricing code.
						Example: USD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Penalty" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Penalty and/ or change fee amount.</xs:documentation>
					<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, For ATPCO data processing, this is the Category 31 Exchange Penalty.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="Amount" type="xs:integer" use="optional">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Total fare amount including taxes of a fare alternative.
								Example: 4298</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="CommPenalty" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Allowable penalty commission information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="Amount" type="xs:integer" use="optional">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Commission amount.
								Example: 3985</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="ValueOfOldTicket" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Old ticketed total fare.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="Amount" type="xs:integer" use="optional">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Total fare amount including taxes of the old ticketed fare.
								Example: 4468</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BaseFareType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Base fare information.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="FiledIn" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Fare filing information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="CurrencyCode" type="CurrencyCodeType">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Filing currency code.
									Example: USD</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Amount">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Originally filed base fare amount.
									Example: 2385</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:integer">
										<xs:attribute name="BulkMask" type="xs:string" use="optional">
											<xs:annotation>
												<xs:documentation source="DESCRIPTION" xml:lang="en">Ticket masked value.
													Examples: IT, BT</xs:documentation>
												<xs:documentation source="GUIDELINES" xml:lang="en">=notes=

													1. This mask is ALWAYS used for the passenger ticket image. </xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="NUCAmount">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Neutral unit of construction amount.
									Example: 4327</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:integer">
										<xs:attribute name="ROE" type="xs:string" use="optional">
											<xs:annotation>
												<xs:documentation source="DESCRIPTION" xml:lang="en">Applicable rate of exchange.
													Example:1:0.895</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="Surcharges" type="SurchargesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Surcharges information.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:attribute name="Amount" type="xs:integer" use="required">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Total base fare amount that includes surcharges (if applicable).
					Example: 200000</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="BasicEndorsementType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Endorsement information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Text" type="FreeFormTextType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="BookByDate" type="DateTimeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Offer must be booked/ purchased by date.
				Example: 2013-12-25</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BookingToolPartyType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Booking Tool or Sub-Account information.</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0">
			<xs:element name="AccountUserInfo">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Account information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="AccountUserID" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">User ID for the account.
								Example: CorpAgentID</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="AccountID" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Account ID.
					Example: J5531</xs:documentation>
				<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
					1. For an agency, this is typically the Account ID as set up between requestor and client, such as a corporate booking tool.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="BundleContent" type="ServiceBundleNoPriceRefType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Optional Service BUNDLE information.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="CabinCameraPosType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Media Content Seat Camera Position information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Row">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Row position value.
						Example: 11</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:integer">
							<xs:attribute name="Orientation" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Icon placement (in orientation to cabin row or column).
										Examples:
										FRO (Place icon before row)
										INL (Place icon inline with row or column)
										LEF (Place icon left of column)
										REA (Place icon behind row)
										RIG (Place icon right of column)

										For maximum interoperability, refer to IATA PADIS code list COT - Camera Orientation Type.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Column">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Column position value.
						Example: C</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="Orientation" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Icon placement (in orientation to cabin row or column).
										Examples:
										FRO (Place icon before row)
										INL (Place icon inline with row or column)
										LEF (Place icon left of column)
										REA (Place icon behind row)
										RIG (Place icon right of column)

										For maximum interoperability, refer to IATA PADIS code list COT - Camera Orientation Type.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CabinClassCoreType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Cabin class of service core definition.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string"/>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="CabinClassDetailType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Cabin class of service detail definition.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="SectionLabel" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Cabin class marketing name.
							Example: BUSINESS FIRST</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="CCExpiration">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Credit card expiration date.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Month">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Expiration month.
							Example: 02</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="[0-9][0-9]"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Year">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Full year or last two digits of expiration year.
							Examples: 16, 2016</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="2"/>
							<xs:maxLength value="4"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CCIssuer" type="CCIssuerType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Credit card issuer information.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="CCIssuerType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Credit card issuer definition.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CCIINNumber" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Credit card IIN/ BIN number.
						Example: 401123</xs:documentation>
					<xs:documentation source="GUIDELINES" xml:lang="en">DEFINITION: Issuer identification number (IIN)

						The first six digits of a card number (including the initial MII digit) are known as the issuer identification number (IIN) and identify the institution that issued the card to the card holder.

						The first digit of a credit card number is the Major Industry Identifier (MII), which represents the category of entity which issued the card. Examples of MII digits and represented issuer categories are:
						1/2 – Airlines
						3 – Travel and entertainment and banking/financial
						6 – Merchandising and banking/financial

						American Express, Diner's Club, Carte Blanche, and JCB are in the travel and entertainment category; VISA, MasterCard, and Discover are in the banking and financial category, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CCCode" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Payment card type code.
						Example: VI (VISA)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CCQualifierType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Credit Card Qualifier information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CCCode" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Credit card code.
						Example: VI (VISA)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CCNumber" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Credit card number.
						Example: 4011-2366-5465-2341</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="CCExpiration"/>
			<xs:element name="CardholderFirstName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Card holder first name.
						Example: JOHN</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CardholderLastName" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Card holder last name.
						Example: SMITH</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CCAddressGroup" type="AddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Credit card address.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CertificateType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Electronic Certificate information.

				=notes=
				1. Supports non-revenue and flightpass certificates.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Number" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Unique certificate number.
						Example: 12345678</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IssuingAirline" type="AirlineCodeType">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Airline designator code.
						Example: UA</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProgramName" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Associated program name.
						Example: PASSPRO</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="Type" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Program type.
										Examples:
										BUSINESS
										LEISURE</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Holder" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Certificate holder or owner information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="OwnerID" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Owner ID.
								Example: AAA2

								=notes=
								1. This will be a PCC (Pseudo City Code) if an agency has ownership.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="IATA" type="IATANumberType" use="optional">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">IATA number if agency has ownership.
								Example: 98417900</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="CorpID" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Account number if agency has ownership.
								Example: 23456</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="FQTVNumber" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Traveler frequent flyer number if personal ownership.
								Example: 0956301007</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CheckedBagAllowanceType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Checked baggage allowance information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:sequence>
					<xs:element name="MaxWeightPerPiece" minOccurs="0" maxOccurs="2">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Maximum weight per piece value.
								Example: 35</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:extension base="xs:decimal">
									<xs:attribute name="Unit" type="WeightUnitType" use="required">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Weight unit of measure.
												Example: KG (KILOGRAMS)</xs:documentation>
											<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value.
												=enumerated values=
												KG = KILOGRAMS
												LBS = POUNDS</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:extension>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="MaxLengthPerPiece" minOccurs="0" maxOccurs="2">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Maximum length per piece value.
								Example: 23.5</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:extension base="xs:decimal">
									<xs:attribute name="Unit" type="SizeUnitType" use="required">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Dimension unit of measure.
												Example: CM (CENTIMETERS)</xs:documentation>
											<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value.
												=enumerated values=
												CM = CENTIMETERS
												IN = INCHES</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:extension>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
				<xs:element name="BaggageDescription" minOccurs="0">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Baggage size or weight description.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Text" type="FreeFormTextType" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:choice>
				<xs:element name="MaxPieces" type="xs:integer" minOccurs="0">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Maximum pieces allowed.
							Example: 2</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TotalWeight" minOccurs="0">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Total weight allowed for all pieces value.
							Example: 75</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:decimal">
								<xs:attribute name="Unit" type="WeightUnitType" use="required">
									<xs:annotation>
										<xs:documentation source="DESCRIPTION" xml:lang="en">Weight unit of measure.
											Example: KG (KILOGRAMS)</xs:documentation>
										<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value.
											=enumerated values=
											KG = KILOGRAMS
											LBS = POUNDS</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="MSCarrier" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Carrier name who governs the baggage rules for this allowance.
						Example: DELTA</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="CityCode" type="AirportCityCodeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Airport or city code.
				Examples: JFK, NYC</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ClassOfService" type="FlightCOSDetailType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight class of service with price class name.
				Example: B</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Combinality" type="ServiceCombineType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Identifies how offers may be combined.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Commission" type="CommissionType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Commission currency or percentage amount value.
				Example: 4500</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Component" type="ComponentType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Non-seat cabin component information.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ComponentType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Non-seat cabin component definition.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Dimensions" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Defines the size of this item in actual measurement units or defines how much space in relation to rows and columns the space of this item actually consumes.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:sequence>
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Component size by number of rows (length) and number of columns (width).</xs:documentation>
							</xs:annotation>
							<xs:element name="RowEquivalent" type="xs:decimal">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Length in number of ROWS value.
										Example: 2.5</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ColumnEquivalent" type="xs:decimal">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Width in number of COLUMNS value.
										Example: 2.5</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
						<xs:element name="ActualSize">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Actual component size information.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="Unit" type="SeatDimensionUOMType" use="required">
									<xs:annotation>
										<xs:documentation source="DESCRIPTION" xml:lang="en">Size unit of measure.
											Example: IN (INCHES)</xs:documentation>
										<xs:documentation source="ENCODING" xml:lang="en">=enumerated values=
											IN = INCHES
											MM = MILIMETERS</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Length" type="xs:string" use="required">
									<xs:annotation>
										<xs:documentation source="DESCRIPTION" xml:lang="en">Front to back LENGTH measurement value.
											Example: 70</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Width" type="xs:string" use="required">
									<xs:annotation>
										<xs:documentation source="DESCRIPTION" xml:lang="en">Front to back WIDTH measurement value.
											Example: 70</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="Location" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Component location information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="Space">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Actual space occupied by this component.
									Example:
									GALLEY COVERS ROWS 15-17 AND COLUMNS A-C</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="RowRange" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">All rows covered on the map by this item.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:attribute name="Begin" use="optional">
												<xs:annotation>
													<xs:documentation source="DESCRIPTION" xml:lang="en">First row occupied by this item.
														Example: 13</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:integer">
														<xs:minInclusive value="01"/>
														<xs:maxInclusive value="99"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="End" use="optional">
												<xs:annotation>
													<xs:documentation source="DESCRIPTION" xml:lang="en">Last row occupied by this item.
														Example: 17</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:integer">
														<xs:minInclusive value="01"/>
														<xs:maxInclusive value="99"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
									<xs:element name="ColumnRange">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">All columns covered on the map by this item.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:attribute name="Begin" use="optional">
												<xs:annotation>
													<xs:documentation source="DESCRIPTION" xml:lang="en">Left most column occupied by this item.
														Example: A</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:pattern value="[A-Z]{1}"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="End" use="optional">
												<xs:annotation>
													<xs:documentation source="DESCRIPTION" xml:lang="en">Right most column occupied by this item.
														Example: C</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:pattern value="[A-Z]{1}"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:sequence>
							<xs:element name="Row">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Row position value.
										Example: 11</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:integer">
											<xs:attribute name="Orientation" type="xs:string" use="optional">
												<xs:annotation>
													<xs:documentation source="DESCRIPTION" xml:lang="en">Icon placement (in orientation to cabin row or column).
														Examples:
														FRO (Place icon before row)
														INL (Place icon inline with row or column)
														LEF (Place icon left of column)
														REA (Place icon behind row)
														RIG (Place icon right of column)

														For maximum interoperability, refer to IATA PADIS code list COT - Camera Orientation Type.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="Column">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Column position value.
										Example: C</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="Orientation" type="xs:string" use="optional">
												<xs:annotation>
													<xs:documentation source="DESCRIPTION" xml:lang="en">Icon placement (in orientation to cabin row or column).
														Examples:
														FRO (Place icon before row)
														INL (Place icon inline with row or column)
														LEF (Place icon left of column)
														REA (Place icon behind row)
														RIG (Place icon right of column)

														For maximum interoperability, refer to IATA PADIS code list COT - Camera Orientation Type.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="Marketing" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Images and/or links for component.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Images" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Images applicable to this section</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ImageID" type="xs:string">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Image ID defined in root node MediaContent.
												Examples:
												IMG713
												SFO_LOUNGE</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Position" type="CabinCameraPosType" minOccurs="0">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Camera position on seat map information.</xs:documentation>
											<xs:documentation source="ENCODING" xml:lang="en">=notes=
												1. This is the desired position where camera icon or equivalent needs to be positioned on a seat map.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Links" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Links applicable to this section</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="LinkID" type="xs:string">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Link ID reference defined in root node MediaContent.
												Example: SFOLOUNGEID</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Position" type="CabinCameraPosType" minOccurs="0">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Camera position on seat map information.</xs:documentation>
											<xs:documentation source="ENCODING" xml:lang="en">=notes=
												1. This is the desired position where camera icon or equivalent needs to be positioned on a seat map.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Message" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Marketing message for all seats in this section.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Text" type="FreeFormTextType" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Type" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Cabin component.
					Examples:
					701 (Movable compartment divider)
					702 (Convertible seats)
					AR (Air phone)
					BA (Bar)
					BK (Bulkhead)
					CL (Closet)
					G (Galley)
					LA (Lavatory)
					LG (Luggage storage)
					MV (Movie screen)
					SO (Storage space)
					ST (Stairs to upper deck)
					TA (Table)

					For maximum interoperability, refer to IATA PADIS Code List for data element 9978 - Cabin Facilities Characteristic.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
