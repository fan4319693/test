															<xs:element ref="Meals" minOccurs="0"/>
															<xs:element ref="FlightDuration" minOccurs="0"/>
															<xs:element name="LegMiles" type="xs:string" minOccurs="0">
																<xs:annotation>
																	<xs:documentation source="DESCRIPTION" xml:lang="en">Segment mileage quantity.
																		Example: 2143</xs:documentation>
																</xs:annotation>
