		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Contract information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="AirlineCode"/>
			<xs:element name="Contract" type="xs:string" minOccurs="0">
