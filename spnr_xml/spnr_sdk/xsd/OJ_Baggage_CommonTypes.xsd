	<xs:complexType name="ExemptTaxType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Exempt tax information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Tax" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Tax break down information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Designator" type="xs:string">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Tax or surcharge designator.
									Example: US</xs:documentation>
								<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, In ATPCO tax product, the Designator element maps to the ATPCO Tax Code element.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Nature" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Tax nature.
									Example: US2</xs:documentation>
								<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, In ATPCO tax product, the Nature element maps to the IATA/ATPCO ISO Nation Code.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Description" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Tax or surcharge description.
									Example: US National Transportation</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CurrencyCode" type="CurrencyCodeType" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Currency encoding.
									Example: USD

									For maximum interoperability, all currency codes are specified  in ISO 4217 format.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="LocalAmount" type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Local tax amount value.
									Example: 37</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CollectionPoint" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Tax data break down by collection point for XF (extra fee) and ZP (federal tax segment) taxes.</xs:documentation>
								<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
									1. The sum of individual amounts entered here must be equal to the Amount entered in the Tax element.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="AirportCode" type="AirportCityCodeType">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Segment departure or arrival airport code.
												Example: CDG</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="AirportAmount" type="xs:positiveInteger">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Amount filed by the airport in the currency applicable to the geographical location of the airport.
												Example: 2789</xs:documentation>
											<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
												1. This is the same as shown in FareCalculation line.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="CurrencyCode" type="CurrencyCodeType">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Currency encoding.
												Example: USD

												For maximum interoperability, all currency codes are specified  in ISO 4217 format.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="Amount" type="xs:integer" use="optional">
									<xs:annotation>
										<xs:documentation source="DESCRIPTION" xml:lang="en">Tax amount for collection point.
											Example: 2500</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="Amount" type="xs:integer" use="required">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Individual tax amount.
								Example: 2500</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Refundable" type="ChoiceType" use="optional" default="Y">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, tax is refundable.
								Example: Y (YES)</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Amount" type="xs:integer" use="required">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Total taxes amount.
					Example: 750</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="FareBasisCode" type="FareBasisCodeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Fare basis code.
				Example: Y26</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="FareBasisSegAttrType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Fare basis code with proce class name and flight segment qualifier.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="SegmentIDRef" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="PriceClass" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Price class or branded fare name.
					Example: SUPERSAVER</xs:documentation>
				<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, In ATPCO data processing, this is the brand name.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Code" type="FareBasisCodeDefType" use="required">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Fare basis code.
					Example: Y26</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="FareBasisCodeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Fare basis code with price class.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, This is the fare class code in ATPCO pricing.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="FareBasisCodeDefType">
				<xs:attribute name="PriceClass" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Price class or branded fare name.
							Example: SUPERSAVER</xs:documentation>
						<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, In ATPCO data processing, this is the brand name.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="FareBasisSourceAttrType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Fare basis code with price class and source attributes.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, This is the fare class code in ATPCO pricing.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="FareBasisCodeDefType">
				<xs:attributeGroup ref="Source"/>
				<xs:attribute name="PriceClass" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Price class or branded fare name.
							Example: SUPERSAVER</xs:documentation>
						<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, In ATPCO data processing, this is the brand name.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="FareCacheKey" type="FareCacheKeyType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Fare cache key value.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="FareCacheKeyType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Fare cache key definition.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="FreeFormTextType"/>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="FareFiledInType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Fare/ price filing information definition.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="CurrencyCode" type="CurrencyCodeType">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Filing currency code.
						Example: USD

						For maximum interoperability, all currency designators are specified in ISO (three character) 4217 format.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Amount">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Originally filed base fare amount.
						Example: 2385</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:integer">
							<xs:attribute name="BulkMask" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Ticket masked value.
										Examples: IT, BT</xs:documentation>
									<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
										1. This mask is ALWAYS used for the passenger ticket image. </xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="NUCAmount">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Neutral unit of construction amount.
						Example: 4327</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:float">
							<xs:attribute name="ROE" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Applicable rate of exchange.
										Example:1:0.895</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="FarePriceType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Fare pricing information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CurrencyCode" type="CurrencyCodeType">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Currency code.
						Example: USD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BaseFare" type="BaseFareType">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Base (net) price information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Taxes" type="FareTaxesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Taxes information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExemptedTaxes" type="ExemptTaxType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Exempt tax information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="Redemption" type="FullRedemptionType" minOccurs="0">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Award redemption unit and quantity information.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RedemptionCurrency" type="PartialRedemptionType" minOccurs="0">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">PARTIAL redemption payment with conversion rate information.</xs:documentation>
						<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
							1. Conversion rate is for the ServicePrice/CurrencyCode.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="Total" type="xs:integer" use="required">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Total amount.
					Example: 7500</xs:documentation>
				<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
					1. Calculation: Total = base price +  total amount of taxes
					2. This is the total amount for this service based on FQTV status and chosen Price Class.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="FareRefKey" type="FareRefKeyType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Unique fare reference key.
				Example: PQ123456</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="FareRefKeyType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Unique fare reference key definition.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="FreeFormTextType"/>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="FareTaxesType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Fare tax information definition.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Tax" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Tax break down information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Designator" type="xs:string">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Tax or surcharge designator.
									Example: US</xs:documentation>
								<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, In ATPCO tax product, the Designator element maps to the ATPCO Tax Code element.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Nature" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Tax nature.
									Example: US2</xs:documentation>
								<xs:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, In ATPCO tax product, the Nature element maps to the IATA/ATPCO ISO Nation Code.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Description" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Tax or surcharge description.  Example: US National Transportation</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CurrencyCode" type="CurrencyCodeType" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Currency encoding.
									Example: USD

									For maximum interoperability, all currency designators are specified in ISO (three character) 4217 format.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="LocalAmount" type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Local tax amount value.
									Example: 37</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CollectionPoint" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Tax data break down by collection point for XF (extra fee) and ZP (federal tax segment) taxes.</xs:documentation>
								<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
									1. The sum of individual amounts entered here must be equal to the Amount entered in the Tax element.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="CurrencyCode" type="CurrencyCodeType">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Currency encoding.
												Example: USD

												For maximum interoperability, all currency designators are specified in ISO (three character) 4217 format.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="AirportAmount" type="xs:positiveInteger">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Amount filed by the airport in the currency applicable to the geographical location of the airport.
												Example: 2789</xs:documentation>
											<xs:documentation source="GUIDELINES" xml:lang="en">=notes
												1. This is the same as shown in FareCalculation line.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="AirportCode" type="AirportCityCodeType">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Segment departure or arrival airport code.
												Example: CDG</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="Amount" type="xs:integer" use="optional">
									<xs:annotation>
										<xs:documentation source="DESCRIPTION" xml:lang="en">Tax amount for collection point.
											Example: 2500</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="Amount" type="xs:integer" use="required">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Individual tax amount.
								Example: 2500</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Refundable" type="ChoiceType" use="optional" default="Y">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, tax is refundable.
								Example: Y (YES)</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Amount" type="xs:integer" use="required">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Total taxes amount.
					Example: 750</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="FareType" type="xs:string">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Fare type.
				70E (ATPCO Negotiated Fares – Cat35)
				70G (System Negotiated Corporate Fares)
				70J (Published Fares)
				749 (Negotiated Fares)
				756 (One Way Fare)
				758 (Private Fares)

				For maximum interoperability, refer to PADIS codeset element 9910 - Fare Qualifier.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FiledCurrencyCode" type="CurrencyCodeType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Fare component pricing currency code.
				Example: HKD</xs:documentation>
			<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
				1. This should be specified if the fare component pricing currency code is DIFFERENT from the POS currency code.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="FlightCOSDetailType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight class of service with price class name definition.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="FlightCOSType">
				<xs:attribute name="PriceClassName" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Associated price class name.
							Example: SUPERSAVER</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="FlightCOSSeatType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight class of service with price class name and seat status definition.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="FlightCOSType">
				<xs:attribute name="Status" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Seat status by class of service.
							Example: R (SEATS ON REQUEST ONLY TO AIRLINE)

							For maximum interoperability, refer to IATA PADIS Code List for data element 9865 - Seat Occupation.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Quantity" type="xs:positiveInteger" use="optional">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Quantity of seats for designated status.
							Example: 8

							=notes=
							1. This may represent the actual quantity of available seats -OR- a range of available seats.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="PriceClassName" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Associated price class name.
							Example: SUPERSAVER</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="FlightDetailType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Detailed flight type preference definition.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">CONNECTIONS.

						=notes=
						1. Connections apply to DirectStop and NonStop offers.
						2. Direct stop flights include non-stop flights.
						3. Non-stop flights are true non-stop flights.</xs:documentation>
					<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
						1. Connections apply to DirectStop and NonStop offers.
						2. Direct stop flights include non-stop flights.
						3. Non-stop flights are true non-stop flights.</xs:documentation>
				</xs:annotation>
				<xs:element name="NonStop" type="xs:positiveInteger">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Acceptable quantity of stops with no change of gauge.
							Example: 2 (NO PLANE CHANGE)</xs:documentation>
						<xs:documentation source="DEFINITION" xml:lang="en">DEFINITION: A nonstop flight flies from one airport to another without stopping.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DirectStop" type="xs:positiveInteger">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Acceptable quantity of stops with no change of gauge.
							Example: 2 (NO PLANE CHANGE)</xs:documentation>
						<xs:documentation source="DEFINITION" xml:lang="en">DEFINITION: Flight connecting two cities or airports with or without enroute stops; passengers need not change aircraft (except for a change of gauge flight.)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Connections">
					<xs:annotation>
						<xs:documentation source="DESCRIPTION" xml:lang="en">Maximum number of acceptable stops.
							=notes=
							1. May be further qualified by type (online, interline, etc).</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:positiveInteger">
								<xs:attribute name="Type" type="xs:string" use="optional">
									<xs:annotation>
										<xs:documentation source="DESCRIPTION" xml:lang="en">Flight connection type.
											Example: 0 (Online no subsidiaries/same airline; subsidiaries; interline within same alliance and any interline)

											For maximum interoperability, refer to IATA PADIS Code List for data element 9885 - Online/ Interline Indicator.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="AirportChange" type="ChoiceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, EXCLUDE flights where a change of airport is required from query results.
						Example: Y (YES)</xs:documentation>
					<xs:documentation source="GUIDELINES" xml:lang="en">DEFINITION: A change of airport in a flight means that the plane will land at one airport, but the next flight segment will depart from another airport.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OvernightStop" type="ChoiceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, EXCLUDE flights where lodging is required from query results.
						Example: Y (YES)</xs:documentation>
					<xs:documentation source="GUIDELINES" xml:lang="en">DEFINITION: An overnight stop for a flight requires some form of lodging.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RedEye" type="ChoiceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, EXCLUDE red-eye flights from query results.
						Example: Y (YES)</xs:documentation>
					<xs:documentation source="GUIDELINES" xml:lang="en">DEFINITION: A red-eye flight is any flight departing late at night and arriving early the next morning.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="FlightDuration">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Segment total flying time.
				Example: 12:30</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="FlightDurationType">
				<xs:pattern value="[0-9][0-9]:[0-5][0-9]"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="FlightFrequency" type="FlightFrequencyType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight operation day(s) of the week mask.
				Examples:
				1234567 (mon through sun)
				123 (mon, tue, wed)
				467 (thu, fri, sat)</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">=accepted values=
				Individual Days of the Week
				1 = Monday
				2 = Tuesday
				3 = Wednesday
				4 = Thursday
				5 = Friday
				6 = Saturday
				7 = Sunday</xs:documentation>
			<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
				1. This flight mask contains a one digit to 7 digit combination of (unique) day values that designate the days of the week the flight operates. For example:
				- 1234567: represents Monday through Sunday (e.g. seven days a week)
				- 123: represents Monday, Tuesday and Wednesday
				- 467: represents Thursday, Saturday and Sunday</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="FlightFrequencyType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight operation day(s) of the week mask definition.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">=accepted values=
				Individual Days of the Week
				1 = Monday
				2 = Tuesday
				3 = Wednesday
				4 = Thursday
				5 = Friday
				6 = Saturday
				7 = Sunday</xs:documentation>
			<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
				1. This flight mask contains a one digit to 7 digit combination of (unique) day values that designate the days of the week the flight operates. For example:
				- 1234567: represents Monday through Sunday (e.g. seven days a week)
				- 123: represents Monday, Tuesday and Wednesday
				- 467: represents Thursday, Saturday and Sunday</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string"/>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="FlightMealsType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">In-flight mea served type code(s) definition.</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">For maximum interoperability, refer to IATA PADIS Code List for data element 7161 - Special Service, Coded.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string"/>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="FlightNumber" type="FlightNumberType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight number.
				Example: 1234</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FlightRefInfo" type="FlightRefInfoType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight information.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="FlightRefInfoType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight information definition.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="AirlineCode"/>
			<xs:element ref="FlightNumber"/>
			<xs:element name="Departure">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Departure information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="AirportCode"/>
						<xs:element name="Date" type="DateType">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Date.
									Example: 2013-01-13</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Time" type="TimeType" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Time.
									Example: 14:30</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Arrival">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Arrival information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="AirportCode"/>
						<xs:element name="Date" type="DateType">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Date.
									Example: 2013-01-13</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Time" type="TimeType" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Time.
									Example: 14:30</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ClassOfService" type="FlightCOSType">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Class of service.
						Example: B</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="AssociationID" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Unique flight/ flight segment ID.
					Example: 1 or F1: Flight Segment 1</xs:documentation>
				<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
					1. The unique flight/ flight segment ID set here may be used in other areas within this message to associate other elements to this flight information.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="FlightStopType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight Stop information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AirportCode" type="AirportCityCodeType">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Airport code.
						Example: CDG</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AirportName" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Airport name.
						Example: CHARLES DE GAULLE AIRPORT</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArrivalDate" type="DateType">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Arrival date of one or more flights related to specified fare information.
						Example: 2013-01-15</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArrivalTime" type="TimeType">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Stop arrival time.
						Example: 14:30</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartureDate" type="xs:date">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Departure date.
						Example: 2013-12-25</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartureTime" type="TimeType">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Stop departure time.
						Example: 14:30</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Equipment" type="EquipmentDetailType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Change of gauge aircraft equipment information.</xs:documentation>
					<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
						1. IMPORTANT: ONLY USE THIS in the case of a Change of Gauge flight as the equipment for the continuation of the flight will be mapped from here.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GroundTime" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Aircraft ground time minutes.
						Example: 30</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ChangeOfGauge" type="ChoiceType" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, there has been a Change of Gauge.
					Example: Y (YES)</xs:documentation>
				<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
					1. This value should be set to Y if the flight is a Change of Gauge flight.
					2. If Y, the new equipment (for the next leg of this flight) must be specified in the Equipment element.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="FlightTable" type="FlightTableType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">All flights contained in any of the returned solutions.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FlightTableRef" type="FlightTableRefType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight solutions reference information.

				=notes=
				1. This element provides a reference for the specified flight/ flight segment solution information to the flight details specified in the FlightTable element.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="FlightTableRefType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight Solutions Table Reference.

				=notes=
				1. This node replaces complete flight data sets with short listings of the referenced Segment/Flight that contains a few attributes with no elements.
				2. It provides a reference for specified flight/ flight segment information to flight details in the AirShoppingRS/FlightTable.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="FlightID" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Unique association ID to reference the respective flight in the FlightTable.
					Example: F1</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ChangeOfAirport" type="ChoiceType" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, there is a change of airport in the connection.
					Example: Y (YES)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="MarriedSegment"/>
		<xs:attributeGroup ref="CnxxIndicator"/>
		<xs:attribute name="SecureFlight" type="ChoiceType" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, secure flight data is required for this flight.
					Example: N (NO)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="FlightTableType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight Solutions Table.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Flight" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Flight details.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Departure">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Departure information.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="AirportCode"/>
									<xs:element name="AirportName" type="xs:string">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Airport name.
												Example: LaGuardia Airport</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Date" type="DateType">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Departure date.
												Example: 2013-01-11</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Time" type="TimeType">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Departure time.
												Example: 11:32</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Terminal" type="TerminalType" minOccurs="0">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Airport terminal.
												Example: 2F</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Gate" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Gate.
												Example: B14</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attributeGroup ref="ProximityGroup"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="Arrival">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Arrival information.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="AirportCode"/>
									<xs:element name="AirportName" type="xs:string">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Airport name.
												Example: Charles de Gaulle Airport</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Date" type="DateType">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Arrival date.
												Example: 2013-01-15</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Time" type="TimeType">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Arrival time.
												Example: 14:30</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ChangeOfDay" type="xs:integer" minOccurs="0">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Number of days between arrival and departure.
												Example: 1</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Terminal" type="TerminalType" minOccurs="0">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Terminal.
												Example: 2F</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Gate" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Gate.
												Example: B14</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attributeGroup ref="ProximityGroup"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="Carrier" type="AirlineCarrierType">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Flight carrier information.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="OperatingCarrier" type="AirlineOperatorCoreType" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Operating carrier information.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Equipment" type="EquipmentDetailType">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Aircraft equipment information.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element ref="NumberOfStops" minOccurs="0"/>
						<xs:element name="StopInformation" type="FlightStopType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Stop information.</xs:documentation>
								<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
									1. A Change of Gauge flight requires the @ChangeOfGauge attribute to be set to Y and new equipment specified from that stop forward.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element ref="Meals" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="Frequency" minOccurs="0"/>
						<xs:element ref="FlightDuration" minOccurs="0"/>
						<xs:element name="OnTimeInfo" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">On time flight performance.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="Low" type="xs:string" use="optional">
									<xs:annotation>
										<xs:documentation source="DESCRIPTION" xml:lang="en">Ontime percentage low value.
											Example: 70 (70 PERCENT)</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="High" type="xs:string" use="optional">
									<xs:annotation>
										<xs:documentation source="DESCRIPTION" xml:lang="en">Ontime percentage high value.
											Example: 79 (79 PERCENT)</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="NewFlightInd" type="ChoiceType" use="optional">
									<xs:annotation>
										<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, this is a new flight and no performance information is available.
											Example: Y (YES)</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="NoDataInd" type="ChoiceType" use="optional">
									<xs:annotation>
										<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, no performance information is available.
											Example: Y (YES)</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="NotRequiredInd" type="ChoiceType" use="optional">
									<xs:annotation>
										<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, this is an international flight or location where no performance reporting is required.
											Example: N (NO)</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="ElectronicTicketing" type="ChoiceType" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, an electronic ticket may be issued.
									Example: Y (YES)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Ticketless" type="ChoiceType" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, fare can be purchased without issuing a ticket.
									Example: Y (YES)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element ref="InstantPurchase" minOccurs="0"/>
						<xs:element name="TrafficRestrictions" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Traffic restriction code.
									Example: A (NO TRAFFIC PERMITTED)

									For maximum interoperability, refer to SSIM Manual Appendix G.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="LegMiles" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Segment mileage quantity.
									Example: 2143</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Classes">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Service class information.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ClassOfService" type="FlightCOSSeatType" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">Segment booking class of service.
												Example: B</xs:documentation>
											<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
												1. This booking class of service will be applied to this segment when booked as part of this FareGroup.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="FlightID" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Unique ID flight reference.
								Example: F1</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight type preferences.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AirportChange" type="ChoicePrefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Change of airport(s) preference.
						Examples:
						N = NOT ACCEPTABLE  (only search results that do not include this preference should be returned)
						P = PREFERRED (search results that do or do not satisfy this preference should be returned)
						R = REQUIRED (only search results that satisfy this preference should be returned)</xs:documentation>
					<xs:documentation source="DEFINITION" xml:lang="en">A change of airport in a flight means that the plane will land at one airport, but the next flight segment will depart from another airport.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OvernightStop" type="ChoicePrefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Overnight stop flight preference.
						Examples:
						N = NOT ACCEPTABLE  (only search results that do not include this preference should be returned)
						P = PREFERRED (search results that do or do not satisfy this preference should be returned)
						R = REQUIRED (only search results that satisfy this preference should be returned)</xs:documentation>
					<xs:documentation source="DEFINITION" xml:lang="en">An overnight stop for a flight requires some form of lodging.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RedEye" type="ChoicePrefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Red-eye flight preference.
						Examples:
						N = NOT ACCEPTABLE  (only search results that do not include this preference should be returned)
						P = PREFERRED (search results that do or do not satisfy this preference should be returned)
						R = REQUIRED (only search results that satisfy this preference should be returned)</xs:documentation>
					<xs:documentation source="DEFINITION" xml:lang="en">A red-eye flight is any flight departing late at night and arriving early the next morning.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="NonStopDirect" type="ChoicePrefType" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Non-stop direct flight preference.
					Examples:
					N = NOT ACCEPTABLE  (only search results that do not include this preference should be returned)
					P = PREFERRED (search results that do or do not satisfy this preference should be returned)
					R = REQUIRED (only search results that satisfy this preference should be returned)</xs:documentation>
				<xs:documentation source="DEFINITION" xml:lang="en">A Non-Stop Direct Flight is a flight with no intermediate stops.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="StopDirect" type="ChoicePrefType" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Stop direct flight preference.
					Examples:
					N = NOT ACCEPTABLE  (only search results that do not include this preference should be returned)
					P = PREFERRED (search results that do or do not satisfy this preference should be returned)
					R = REQUIRED (only search results that satisfy this preference should be returned)</xs:documentation>
				<xs:documentation source="DEFINITION" xml:lang="en">A Stop Direct flight is between two points with no change in flight numbers, but may include a stop over at an intermediate point to get new passengers (or allow some to disembark) or a mere technical stop over (i.e. for refuelling purposes only).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="NumberConnx" type="xs:positiveInteger" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">The MAXIMUM number of connections that may be returned in response.
					Example: 2</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AllowInterline" type="ChoiceType" use="optional" default="Y">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, interline connection(s) are acceptable.
					Example:
					Y (YES)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="ForInfo" type="ServiceProcessInfoType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Optional service-level processing information.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ForInfoType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Transaction processing information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Text" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Information text.

						=notes=
						Where possible, it is recommended that PADIS Processing Status Encoding be used.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="Source"/>
		<xs:attributeGroup ref="SourceRef"/>
	</xs:complexType>
	<xs:complexType name="FQTVInfoCoreType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Frequent traveler information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FFNumber" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Frequent flyer number.
						Example: 0956301007</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FFCompanyCode" type="AirlineCodeType">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Frequent flyer company code.
						Example: AF</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Surname" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Frequent traveler last or sur name.
						Example: SMITH</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GivenName" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Frequent traveler first or given name.
						Example: JOHN</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FQTVInfoSegType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Frequent traveler information with associated flight segment.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="SegmentIDRef" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="FFNumber" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Frequent flyer number.
						Example: 0956301007</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FFCompanyCode" type="AirlineCodeType">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Frequent flyer company code.
						Example: AF</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Surname" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Frequent traveler last or sur name.
						Example: SMITH</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GivenName" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Frequent traveler first or given name.
						Example: JOHN</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="FreqGuestNumber" type="FreqGuestNumberType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Non-air frequent guest card program information.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="FreqGuestNumberType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Non-air frequent guest card program information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CompanyCode" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Car company or hotel chain code.
						Example: ZE</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IDNumber" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Frequent renter or guest ID number.
						Example: 0956301007</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProgramName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Program name.
						Examples:
						HILTON HHONORS (hotel)
						EMERALD CLUB (car)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Association" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Associated airline frequent flyer program.</xs:documentation>
					<xs:documentation source="GUIDELINES" xml:lang="en">=notes
						1. This is the frequent flyer program a traveler specifies in order to earn additional miles/points from hotel and/or car companies.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AirlineCode" type="AirlineCodeType">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Issuing airline designator code.
									Example: UA</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IDNumber" type="xs:string">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Frequent renter or guest ID number.
									Example: 0956301007</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SurName" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Loyalty member last or sur name.
									Example: SMITH</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="GivenName" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Loyalty member first or given name.
									Example: JOHN</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Type" type="OtherSegmentType" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Non-air related category.
					Examples:
					C (CAR RENTAL)
					H (HOTEL)
					R (RAIL)
					S (CRUISE)
					T (ACTIVITY OR TOUR)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="Frequency" type="FlightFrequencyType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Flight operation day(s) of the week mask.
				Examples:
				1234567 (mon through sun)
				123 (mon, tue, wed)
				467 (thu, fri, sat)</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">=accepted values=
				Individual Days of the Week
				1 = Monday
				2 = Tuesday
				3 = Wednesday
				4 = Thursday
				5 = Friday
				6 = Saturday
				7 = Sunday</xs:documentation>
			<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
				1. This flight mask contains a one digit to 7 digit combination of (unique) day values that designate the days of the week the flight operates. For example:
				- 1234567: represents Monday through Sunday (e.g. seven days a week)
				- 123: represents Monday, Tuesday and Wednesday
				- 467: represents Thursday, Saturday and Sunday</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="FullRedemptionType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Full Award Redemption information.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Unit" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Award redemption unit type.
					Example: MIL (MILES)

					For maximum interoperability, Refer to IATA PADIS code list RUT - Redemption Unit Type.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Value" type="xs:integer" use="required">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Unit quantity.
					Example: 15000</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="Gender" type="TravelerGenderType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Traveler gender.
				Examples:
				M (Male)
				FI (Female Infant)</xs:documentation>
			<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list.
				=enumerated values=
				F = FEMALE
				M = MALE
				MI = MALE INFANT
				FI = FEMALE INFANT
				U = UNDISCLOSED/ UNKOWN GENDER</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="GeocodingType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Geographical position information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Latitude" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Latitude value.
						Example: 49.9856</xs:documentation>
					<xs:documentation source="ENCODING" xml:lang="en">=notes=

						1. If sign attribute is missing, positive values stand for NORTH, negative values for SOUTH.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="Sign" use="optional">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Latitude sign.
										Example: SOUTH (South of Meridian)</xs:documentation>
									<xs:documentation source="ENCODING" xml:lang="en">ACCEPTED VALUES

										NORTH = North of Equator
										SOUTH = South of Equator</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="SOUTH"/>
										<xs:enumeration value="NORTH"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="Minute" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Latitude degree minute value.
										Example: 45</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Second" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Latitude degree second value.
										Example: 30</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Longitude" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Longitude value.
						Example: 9.1220</xs:documentation>
					<xs:documentation source="GUIDELINES" xml:lang="en">=notes=

						1. If sign attribute is missing, positive values stand for EAST, negative values for WEST.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="Sign" use="optional">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Longitude sign.
										Example: EAST (East of Meridian)</xs:documentation>
									<xs:documentation source="ENCODING" xml:lang="en">ACCEPTED VALUES

										EAST = East of Meridian
										WEST = West of Meridian</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="EAST"/>
										<xs:enumeration value="WEST"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="Minute" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Longitude degree minute value.
										Example: 130</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Second" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Longitude degree second value.
										Example: 69</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="NAC" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Natural area geocode (NAC).
						Example: 8KDB PGFD</xs:documentation>
					<xs:documentation source="GUIDELINES" xml:lang="en">=notes=

						1. A geocoded Natural Area Code (NAC) unifies the representations of geographic coordinates, area codes, street addresses, postal codes, map grids and property identifiers of every location or area in the world. http://www.nacgeo.com/nacsite/

					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NDC_ImageType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Core image information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Height">
				<xs:annotation>
					<xs:documentation>Height of media content in specified units.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:int">
							<xs:attribute name="Unit" type="MediaDimensionUnitType" use="required">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Dimension unit.
										Examples:
										MM (MILLIMETER)
										PX (PIXEL)</xs:documentation>
									<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value.
										=enumerated values=
										MM = MILLIMETER
										PX = PIXEL</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Width">
				<xs:annotation>
					<xs:documentation>Width of media content in specified units.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:int">
							<xs:attribute name="Unit" type="MediaDimensionUnitType" use="required">
								<xs:annotation>
									<xs:documentation source="DESCRIPTION" xml:lang="en">Dimension unit.
										Examples:
										MM (MILLIMETER)
										PX (PIXEL)</xs:documentation>
									<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value.
										=enumerated values=
										MM = MILLIMETER
										PX = PIXEL</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="ImageData" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Encoded image value, typically in base64.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ImageText" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Media associated text.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Text" type="FreeFormTextType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ImageID" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Unique media ID.
					Examples:
					IMG713
					SFO_LOUNGE</xs:documentation>
				<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
					1. Provides an unique ID for any available images returned in any responsemessages.
					2. FileRetrieveRQ/ FileListRS will utilize these ID's to retrieve the full image(s) as needed.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Caption" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Media caption.
					Example: PREMIUM SEAT</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Symbology" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Barcode symbology applied.
					Example: PDF417</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Encoding" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Media encoding method.
					Example: BASE64</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Format" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">Media format.
					Example: JPEG</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="IncentiveData" type="ServiceIncentiveDataType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Incentive information.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="IncentiveProgram" type="IncentiveProgramType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Incentive Program(s) offered or supported by an airline.

				=notes=
				1. Although these programs apply to businesses, groups and individuals, there is no association with existing Frequent Traveler Loyalty programs.

				2. Incentive Program(s) may OPTIONALLY be associated to one or more defined travelers.

				3.  Incentive Program(s) apply to ALL travelers if no traveler associations are specified.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="IncentiveProgramType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Any incentive programs offered by an airline. Note that although these programs apply to businesses, groups and individual, there is no association with existing Frequent Traveler Loyalty programs.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LoyaltyMemberID" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Individual or corporate membership account or number.
						Example: IBM123</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Name" type="xs:string">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Incentive program name.
						Example: RFSB (Reward for small businesses)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AirlineCode" type="AirlineCodeType">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Designator of airline offering the incentive program.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProgramStatus" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Participant status within the program.
						Examples:
						VP
						DIRECTOR
						EXEC</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="InExchangeForTicket" type="TktInExchangeForType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Auto Exchange application information.</xs:documentation>
			<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
				1. IMPORTANT: This is MANDATORY information for AutoExchange applications, such as ReShop from ITA.
				2. If used, the attribute @AutoExchange in the PricingInfo node MUST also be set to Y.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Infant" type="TravelerInfantType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Infant on lap traveler information.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="InfantData" type="TravelerInfantType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Infant on lap traveler information.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="InfoGroup" type="InfoGroupType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Message processing errors, business warnings and/ or marketing-related information.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="InfoGroupType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Message processing errors and/ or information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ForInfo" type="ForInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Processing information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Error" type="ErrorType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Processing errors.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MarketingData" type="MarketingDataType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Marketing messages and multimedia.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="InstantPurchase" type="xs:string">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Instant purchase indicator.
				Examples:
				AP (Purchase using cerificate credit)
				CC (Ticketless purchase immediately charged to credit card)
				NO (Not an instant purchase)
				PP (Purchase is charged via digital wallet)
				TT (Simultaneous with time of ticketing)

				For maximum interoperability, Refer to IATA PADIS code list IPT - Instant Purchase Type.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="InstantPurchaseType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Instant purchase indicator.
				Example: CC (TICKETLESS PURCHASE IMMEDIATELY CHARGED TO CREDIT CARD)

				For maximum interoperability, Refer to IATA PADIS code list IPT-Instant Purchase Type.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string"/>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="JourneyControlData" type="JourneyControlType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Journey control information.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="JourneyControlType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Journey control information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="JCDFlight" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Flight information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="JCDDeparture">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Origin (departure) information.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="CityCode" type="AirportCityCodeType">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">City code.
												Example: PAR</xs:documentation>
											<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
												1. This is the location for the ticket sale and/ or issuance.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="JCDArrival">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Destination (arrival) information.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="CityCode" type="AirportCityCodeType">
										<xs:annotation>
											<xs:documentation source="DESCRIPTION" xml:lang="en">City code.
												Example: ORD</xs:documentation>
											<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
												1. This is the location for the ticket sale and/ or issuance.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="JCDDate">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Arrival or departure date.
									Example: 2013-12-25</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:date">
										<xs:attribute name="Qualifier" type="FlightDateType" use="optional" default="DEPARTURE">
											<xs:annotation>
												<xs:documentation source="DESCRIPTION" xml:lang="en">Departure or arrival date designator.
													Example: DEPARTURE</xs:documentation>
												<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list.
													=enumerated values=
													ARRIVAL = ARRIVAL DATE
													DEPARTURE = DEPARTURE DATE (default)</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="Carrier">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Flight carrier information.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="AirlineCode" type="AirlineCodeType">
