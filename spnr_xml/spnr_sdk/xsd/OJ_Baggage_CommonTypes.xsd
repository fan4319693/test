	<xs:complexType name="PenaltyCoreType">
		<xs:annotation>
			<xs:documentation source="DESCRIPTION" xml:lang="en">Core Penalty information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FeeApplies" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Applicable Fee information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="ChangeItinerary" type="ChoiceType" use="optional" default="N">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, a change of itinerary fee applies.
								Example: N (NO)</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Cancellation" type="ChoiceType" use="optional" default="N">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, cancellation fees apply.
								Example: Y (YES)</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Change" minOccurs="0" maxOccurs="3">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Change Penalty information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Amount" maxOccurs="2">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Currency or percentage amount.
									Example: 7500</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:integer">
										<xs:attribute name="Unit" type="AmountCategoryType" use="required">
											<xs:annotation>
												<xs:documentation source="DESCRIPTION" xml:lang="en">Unit of measure.
													Examples:
													A (CURRENCY AMOUNT)
													P (PERCENTAGE)</xs:documentation>
												<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list.
													=enumerated values=
													A = CURRENCY AMOUNT
													P = PERCENTAGE</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="MinMax" type="MinMaxPenaltyType" use="optional">
											<xs:annotation>
												<xs:documentation source="DESCRIPTION" xml:lang="en">Minimum or maximum penalty indicator.
													Examples:
													MAX (MAXIMUM PENALTY AMOUNT)
													MIN (MINIMUM PENALTY AMOUNT)</xs:documentation>
												<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list.
													=enumerated values=
													MAX = MAXIMUM PENALTY AMOUNT
													MIN = MINIMUM PENALTY AMOUNT</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="Text" type="FreeFormTextType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Free form text.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="Application" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Penalty application.

								Example: ADE (After Departure)

								For maximum interoperability, refer to IATA PADIS code list FAT - Fare Or Penalty Application Type.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Cancel" minOccurs="0" maxOccurs="3">
				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Cancel Penalty information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Amount" maxOccurs="2">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Currency or percentage amount.
									Example: 7500</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:integer">
										<xs:attribute name="Unit" type="AmountCategoryType" use="required">
											<xs:annotation>
												<xs:documentation source="DESCRIPTION" xml:lang="en">Unit of measure.
													Examples:
													A (CURRENCY AMOUNT)
													P (PERCENTAGE)</xs:documentation>
												<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list.
													=enumerated values=
													A = CURRENCY AMOUNT
													P = PERCENTAGE</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="MinMax" type="MinMaxPenaltyType" use="optional">
											<xs:annotation>
												<xs:documentation source="DESCRIPTION" xml:lang="en">Minimum or maximum penalty indicator.
													Examples:
													MAX (MAXIMUM PENALTY AMOUNT)
													MIN (MINIMUM PENALTY AMOUNT)</xs:documentation>
												<xs:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value.
													=enumerated values=
													MAX = MAXIMUM PENALTY AMOUNT
													MIN = MINIMUM PENALTY AMOUNT</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="Text" type="FreeFormTextType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation source="DESCRIPTION" xml:lang="en">Free form text.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="Application" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">Penalty application.

								Example: BDE (Before Departure)

								For maximum interoperability, refer to IATA PADIS code list FAT - Fare Or Penalty Application Type.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Refundable" type="ChoiceType" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, this is a refundable fare/ ticket.
					Example: Y (YES)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Reusable" type="ChoiceType" use="optional">
			<xs:annotation>
				<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, this is a reusable fare/ ticket.
					Example: Y (YES)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
