				<xs:annotation>
					<xs:documentation source="DESCRIPTION" xml:lang="en">Surcharge description.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
