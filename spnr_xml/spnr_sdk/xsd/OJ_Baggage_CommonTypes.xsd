											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="Item" type="xs:integer" use="required">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">List item number.
								Example: 1</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="InputID" type="xs:string" use="required">
