																</xs:annotation>
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="ClassOfService">
																			<xs:annotation>
																				<xs:documentation source="DESCRIPTION" xml:lang="en">Segment booking class of service.
																					Example: B</xs:documentation>
																				<xs:documentation source="GUIDELINES" xml:lang="en">=notes=
																					1. This booking class of service will be applied to this segment when booked as part of this FareGroup.</xs:documentation>
																			</xs:annotation>
																			<xs:complexType>
																				<xs:simpleContent>
																					<xs:extension base="FlightCOSType">
																						<xs:attribute name="Status" type="AvailSeatStatusType" use="optional">
																							<xs:annotation>
																								<xs:documentation source="DESCRIPTION" xml:lang="en">Status for available seats.
																									Example: SEATS ON REQUEST ONLY TO AIRLINE

																									For maximum interoperability, Refer to IATA PADIS Code List for data element 9865.</xs:documentation>
																							</xs:annotation>
																						</xs:attribute>
																						<xs:attribute name="Quantity" type="xs:positiveInteger" use="optional">
																							<xs:annotation>
																								<xs:documentation source="DESCRIPTION" xml:lang="en">Quantity of available seats.
																									Example: 8

																									=notes=
																									1. This may be the actual quantity or a range of available seats for this service class.</xs:documentation>
																							</xs:annotation>
																						</xs:attribute>
																						<xs:attributeGroup ref="PriceClassName">
																							<xs:annotation>
																								<xs:documentation source="DESCRIPTION" xml:lang="en">Associated price class name.
																									Example: SUPERSAVER</xs:documentation>
																							</xs:annotation>
																						</xs:attributeGroup>
																					</xs:extension>
																				</xs:simpleContent>
																			</xs:complexType>
																		</xs:element>
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
														</xs:sequence>
														<xs:attribute name="ChangeOfAirport" type="ChoiceType" use="optional">
															<xs:annotation>
																<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, there is a change of airport in the connection.
																	Example: Y (YES)</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attributeGroup ref="MarriedSegment"/>
														<xs:attributeGroup ref="CnxxIndicator"/>
														<xs:attribute name="SecureFlight" type="ChoiceType" use="optional">
															<xs:annotation>
																<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, secure flight data is required for this flight.
																	Example: N (NO)</xs:documentation>
															</xs:annotation>
														</xs:attribute>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attributeGroup ref="DepartureArrivalCode"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="UserInstruction" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">User instructions for required input.

								=notes=
								1. A client application should use this as a label for the user input box.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Required" type="ChoiceType" use="optional">
						<xs:annotation>
							<xs:documentation source="DESCRIPTION" xml:lang="en">If Y, user input is mandatory.
								Example: Y (YES)</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
