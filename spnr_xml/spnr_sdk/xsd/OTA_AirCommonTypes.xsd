		<xs:attribute name="ET_EMD_Linked" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">(E)lectronic (T)icket and (E)lectronic (M)iscellaneous (D)ocument linked indicator.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="SupplierRefType">
