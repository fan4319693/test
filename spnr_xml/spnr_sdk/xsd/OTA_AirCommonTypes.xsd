		<xs:attribute name="CodeContext">
			<xs:annotation>
				<xs:documentation xml:lang="en">Identifies the supplier code which Code comes from.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Name">
