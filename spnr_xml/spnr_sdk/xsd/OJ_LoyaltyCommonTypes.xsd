	<xs:complexType name="PaymentLoyaltyType">
		<xs:sequence>
			<xs:element name="Earn" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used to describe how loyalty units might be earned</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="EarnType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Redeem" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Used to describe how loyalty units might be redeemed, in terms of mixed cash and loyalty.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="RedeemType">
							<xs:sequence>
								<xs:element name="SubRedeem" minOccurs="0">
									<xs:annotation>
										<xs:documentation>The totals of the SubRedeem add up the values stored in the parent redeem element. This is in order to aid the use of the loyalty slider </xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="RedeemType">
												<xs:attribute name="AuditID" type="xs:integer">
													<xs:annotation>
														<xs:documentation>Used to record when the loyalty slider was used</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="CashRangeLowEnd" type="xs:float"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LoyaltyPricingType">
		<xs:sequence>
			<xs:choice>
				<xs:element name="Combinations">
					<xs:annotation>
						<xs:documentation>Used to store the cash and miles pricing combinations</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Combination" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Used to store the price breakdown for the cash and miles values in a pricing combination. Also contains the earn amount for a combination</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Cash" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Stores the price breakdown of the cash part of a combination.  Multiple Cash elements are possible e.g. if you charge redemption rate taxes for cash.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="PriceAdjustments" type="exPriceAdjustmentsType" minOccurs="0"/>
													<xs:element name="Formulas" type="FormulasType" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Represents a number of Formula elements that show how a cash value is generated.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Total" type="TotalType">
														<xs:annotation>
															<xs:documentation xml:lang="en">The cash price total of the cash and miles combination.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="RateReference" type="xs:string">
													<xs:annotation>
														<xs:documentation>Used to store the ID of the source rate used to calculate this breakdown</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="PTCReference_List" type="xs:string">
													<xs:annotation>
														<xs:documentation>Used to store a list of PTC reference IDs of the source used to calculate this breakdown</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="Redemption" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Stores the price breakdown of the redemption part of a combination</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="PriceAdjustments" type="exPriceAdjustmentsType" minOccurs="0"/>
													<xs:element name="Total" type="TotalType" minOccurs="0">
														<xs:annotation>
															<xs:documentation xml:lang="en">The cash price total of the cash and miles combination.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Redeem">
														<xs:annotation>
															<xs:documentation>Used to store the redemption total of the cash and miles combination. Also stores the rules that were used to generate the redemption quantity</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="RedeemType"/>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="RateReference" type="xs:string">
													<xs:annotation>
														<xs:documentation>Used to store the ID of the source rate used to calculate this breakdown</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="PTCReference_List" type="xs:string">
													<xs:annotation>
														<xs:documentation>Used to store a list of PTC reference IDs of the source used to calculate this breakdown</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="Earn" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Used to describe how loyalty units might be earned</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="EarnType"/>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="PTCs" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Stores a number of per passenger type Cash, Redemption and Earn information</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="LoyaltyPTCType"/>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="LoyaltyDetails" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Details of the Loyalty Combinations</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="LoyaltyDetailsPricingType">
														<xs:sequence>
															<xs:element name="PricingParameters" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Used to store values that are needed when repricing this itinerary.</xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="PricingParameter" maxOccurs="unbounded">
																			<xs:annotation>
																				<xs:documentation>A value required when repricing this itinerary </xs:documentation>
																			</xs:annotation>
																			<xs:complexType>
																				<xs:attribute name="Code" use="required">
																					<xs:annotation>
																						<xs:documentation>Code associated with this PricingParameter</xs:documentation>
																					</xs:annotation>
																				</xs:attribute>
																			</xs:complexType>
																		</xs:element>
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
														</xs:sequence>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="RPH" type="xs:string" use="required">
										<xs:annotation>
											<xs:documentation>Reference to uniquely identify a combination within it's parent </xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="Amount" type="xs:float" use="required">
										<xs:annotation>
											<xs:documentation>Total cash value of the cash component of the combination</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="IncludeTax" type="xs:boolean">
										<xs:annotation>
											<xs:documentation>Flag to indicate if taxes are included in the redemption conversion.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="CurrencyCode" use="required">
										<xs:annotation>
											<xs:documentation>Currency code of the cash value of the combination</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="Quantity" type="xs:nonNegativeInteger" use="required">
										<xs:annotation>
											<xs:documentation>Total miles quantity of the redemption component of the combination</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="InsufficientBalance">
										<xs:annotation>
											<xs:documentation>Flag to indicate the miles quantity is greater than the available miles balance of the logged in member</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="BookingRateReference" type="xs:string" use="required">
										<xs:annotation>
											<xs:documentation>This is used to refer to the rate that should be booked against for this combination as the cash and redemption values could be generated from two different rates but only one should be booked against</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="Type" use="required">
										<xs:annotation>
											<xs:documentation>The type of price combination: POINTS, CASH or MIXED</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="POINTS"/>
												<xs:enumeration value="CASH"/>
												<xs:enumeration value="MIXED"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="Sequence">
										<xs:annotation>
											<xs:documentation>This is the position in a sequence that this combination occurs</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="Code">
										<xs:annotation>
											<xs:documentation>Combination code/id</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="CheapestLoyalty">
										<xs:annotation>
											<xs:documentation>This is the cheapest Loyalty combination in the search results</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="CheapestCash">
										<xs:annotation>
											<xs:documentation>This is the cheapest cash combination in the search results</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="Percentage" type="xs:decimal" use="optional">
										<xs:annotation>
											<xs:documentation>This code represents the percentage of chosen cash and/or points combination that was chosen at booking time</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="PercentageOf" type="PercentageOfString" use="optional">
										<xs:annotation>
											<xs:documentation>This code represents whether the percentage detailed above is of the cash or points portion of the cash and/or points combination that was chosen at booking time</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Earn" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Used to describe how loyalty units might be earned</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="EarnType"/>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="Redeem">
				<xs:annotation>
					<xs:documentation>Used to store the redemption total of the cash and miles combination. Also stores the rules that were used to generate the redemption quantity</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="RedeemType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LoyaltyDetailsPricingType">
		<xs:sequence>
			<xs:element name="Combinations">
				<xs:annotation>
					<xs:documentation>Used to store the cash and miles pricing combinations</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Combination" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Used to store the price breakdown for the cash and miles values in a pricing combination. Also contains the earn amount for a combination</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Cash" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Stores the price breakdown of the cash part of a combination.  Multiple Cash elements are possible e.g. if you charge redemption rate taxes for cash.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="PriceAdjustments" type="exPriceAdjustmentsType" minOccurs="0"/>
												<xs:element name="Formulas" type="FormulasType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Represents a number of Formula elements that show how a cash value is generated.</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="Total" type="TotalType">
													<xs:annotation>
														<xs:documentation xml:lang="en">The cash price total of the cash and miles combination.</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
											<xs:attribute name="RateReference" type="xs:string">
												<xs:annotation>
													<xs:documentation>Used to store the ID of the source rate used to calculate this breakdown</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="PTCReference_List" type="xs:string">
