					<xs:documentation xml:lang="en">Indicates that the product is waiting for payment.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PRE_CANCEL">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates payment deadline has not returned the result of successful payment.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CANCELLING">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates product cancellation failure.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CANCELLED">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates the system automatically cancels if the payment fails due to timeout.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PENDING_TICKET">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates product payment is successful and the ticket is issued.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TICKET_FAILED">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates ticket issuing queue fails, waiting for manual guarantee.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TICKET_FAILED_ENSURING">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates ticket failure is being secured.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TICKET_ENSURE_FAILED_MANUAL">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates ticket issue failed and manual security failed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TICKET_ENSURE_FAILED_SYS">
