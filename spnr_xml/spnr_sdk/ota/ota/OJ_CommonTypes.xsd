	<xs:complexType name="FareQuoteInfoType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Pricing information for the air itinerary to be booked. Each price element contains a fare quoted and associated modifiers</xs:documentation>
		</xs:annotation>
