			<xs:annotation>
				<xs:documentation>Timestamp of upload</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Content-Location" type="xs:string">
			<xs:annotation>
				<xs:documentation>The filename</xs:documentation>
			</xs:annotation>
		</xs:attribute>
