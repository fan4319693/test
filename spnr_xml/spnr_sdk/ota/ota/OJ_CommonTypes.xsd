				<xs:attribute name="Operation" type="ActionType" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">Indicates the required modification to the element.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="NetFaresOnlyInd" type="xs:boolean">
					<xs:annotation>
						<xs:documentation>Indicates if the airline file the fares in a Net Fare format.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PTCs">
		<xs:annotation>
			<xs:documentation>A breakdown of passenger type per product</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PTC" maxOccurs="unbounded">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="TravelerCountType">
							<xs:attribute name="RPH_List" type="ListOfRPH">
								<xs:annotation>
									<xs:documentation>(Reference Place Holder) List of passenger IDs</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OJRequestType">
		<xs:annotation>
			<xs:documentation>Define base type for OJ request.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="POS" type="POS_Type"/>
		</xs:sequence>
		<xs:attributeGroup ref="OTA_PayloadStdAttributes"/>
	</xs:complexType>
	<xs:complexType name="OJResponseType">
		<xs:sequence>
			<xs:element name="POS" type="POS_Type" minOccurs="0"/>
			<xs:choice>
				<xs:sequence>
					<xs:element name="Success" type="SuccessType">
						<xs:annotation>
							<xs:documentation xml:lang="en">Success Standard way to indicate successful processing of an OTA message. Returning an empty element of this type indicates success.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Warnings" type="WarningsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en"> Standard way to indicate successful processing of an OTA message, but one in which warnings are generated.
						</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:element name="Errors" type="ErrorsType">
					<xs:annotation>
						<xs:documentation xml:lang="en"> A collection of errors that occurred during the processing of a message.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
		<xs:attributeGroup ref="OTA_PayloadStdAttributes"/>
	</xs:complexType>
	<xs:complexType name="OJ_CustomerPrimaryAdditionalType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Information on the one primary driver and, optionally, several additional drivers. This may be used to provide a frequent renter number.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Primary">
				<xs:annotation>
					<xs:documentation xml:lang="en">Information on the primary driver, possibly including frequent renter number.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="CustomerType">
							<xs:sequence>
								<xs:element name="UniqueID" type="UniqueID_Type" minOccurs="0"/>
								<xs:element name="Profile" type="ProfileType" minOccurs="0"/>
								<xs:element name="ShareReward" minOccurs="0" maxOccurs="unbounded">
									<xs:complexType>
										<xs:attributeGroup ref="ShareRewardGroup"/>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="AccountType">
								<xs:annotation>
									<xs:documentation xml:lang="en">Indicate the user account type, 1- profile user, 2 - mobile login user, etc.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="AdvMailConsent" type="AlphaLength1">
								<xs:annotation>
									<xs:documentation xml:lang="en">Indicates if the contact person agreed to receive the advertising email.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Additional" minOccurs="0" maxOccurs="9">
				<xs:annotation>
					<xs:documentation xml:lang="en">Information on additional  driver(s), possibly including frequent renter number.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="CustomerType">
							<xs:attributeGroup ref="DateTimeSpanGroup">
								<xs:annotation>
									<xs:documentation xml:lang="en">Specifies the dates for how long the additional driver should be part of the rental contract.</xs:documentation>
								</xs:annotation>
							</xs:attributeGroup>
							<xs:attribute name="CorpDiscountName" type="StringLength1to64" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">This is the name of the organization associated with the corporate discount number.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="CorpDiscountNmbr" type="AlphaNumericStringLength1to14" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">This is the code used to identify if the additional driver is eligible for benefits associated with a specific organization.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="QualificationMethod" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">Used as a qualification for an additional driver.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:NMTOKEN">
										<xs:enumeration value="RT_AirlineTicket">
											<xs:annotation>
												<xs:documentation xml:lang="en">Round trip airline ticket</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="CreditCard">
											<xs:annotation>
												<xs:documentation xml:lang="en">If needed, the credit card details should be passed in PaymentForm/PaymentCard.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="PassportAndReturnTkt">
											<xs:annotation>
												<xs:documentation xml:lang="en">Passport and return airline ticket</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="Importer" type="xs:boolean">
								<xs:annotation>
									<xs:documentation>true or false, indicates whether the traveller is the importer.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Email" type="EmailType" minOccurs="0" maxOccurs="9"/>
		</xs:sequence>
		<xs:attribute name="AuditID" type="xs:integer" use="required">
			<xs:annotation>
				<xs:documentation>The AuditID gives information of when this Customer block was created in order to track changes to the SPNR.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CustomerID" type="xs:integer" use="required">
			<xs:annotation>
				<xs:documentation>A unique identifier for a particular customer block.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Historic" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>A boolean flag of what of whether this customer block is live or not.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="HistoryID" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>A reference to the CustomerID of the direct historic version of this customer block if it exists. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SeqNo" type="xs:int"/>
	</xs:complexType>
	<xs:complexType name="PageviewType">
		<xs:sequence>
			<xs:element name="Pageview" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Pageview deatails.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TargetPage" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The page that was visited.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="Name" use="required"/>
								<xs:attribute name="URL"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="Source" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The source direction that visit request from.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="Name" use="required"/>
								<xs:attribute name="URL"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="DateRange" type="DateRangeType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The date when this page was visited</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="TrafficCode" use="required">
						<xs:annotation>
							<xs:documentation>Unique code to inditify the pageview.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Description">
						<xs:annotation>
							<xs:documentation>A description for pageview.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="NumberOfVisits" use="required">
						<xs:annotation>
							<xs:documentation>Number of total visits</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SPNR_POS_Type">
		<xs:complexContent>
			<xs:extension base="POS_Type"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="VerificationPNRPaymentType">
		<xs:attribute name="BankID" type="xs:string">
			<xs:annotation>
				<xs:documentation xml:lang="en">Search by Bank ID of payment for example: ICBC, EASYPAY.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BillNo" type="xs:string">
			<xs:annotation>
				<xs:documentation xml:lang="en">Search by transaction reference number (Bill Number) of payment in external payment system.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CurrencyCode" type="AlphaLength3" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Search by payment currency.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PaymentsType">
		<xs:sequence>
			<xs:element name="Payment" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The details of a single payment</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="PaymentType">
							<xs:sequence>
								<xs:element name="Customer" type="CustomerType" minOccurs="0"/>
								<xs:element name="Agent" type="AgentDetailsType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>The Agent who took payment this payment</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="RefundQualifiers" type="RefundQualifiersType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Used to store Refund Review information - TravelSky Specific</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="OrigPayID">
								<xs:annotation>
									<xs:documentation>Used to tie a new debit payment back to original payment</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="RemainderDueDate"/>
							<xs:attribute name="ThirdParty" type="xs:boolean"/>
							<xs:attribute name="Quantity">
								<xs:annotation>
									<xs:documentation>Used to store the redemption miles quantity for this payment</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Loyalty" type="LoyaltyPaymentType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The details of any redemption payments</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="CurrencyAmountGroup"/>
		<xs:attribute name="LastModified" type="xs:dateTime"/>
		<xs:attribute name="Quantity">
			<xs:annotation>
				<xs:documentation>Used to store the total redemption miles of all redemption payments within a SuperPNR</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ActualPayment" type="xs:float">
			<xs:annotation>
				<xs:documentation>The initial amount paid by the customer during the booking- used for display purposes only.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PaymentType">
		<xs:sequence>
			<xs:element name="ReportingExchangeRates" minOccurs="0">
				<xs:annotation>
					<xs:documentation>To store exchange rates for backoffice reporting tools, where the reporting tool demands a different currency from the vendor currency</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attributeGroup ref="ExchangeRateGroup"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="PaymentForm" type="OJ_PaymentFormType" maxOccurs="unbounded"/>
			<xs:element name="PaymentQualifiers" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PaymentQualifier" maxOccurs="99">
							<xs:complexType>
								<xs:attribute name="QualifierDescription"/>
								<xs:attribute name="QualifierValue"/>
								<xs:attribute name="OJ_SuperPNR_RPH" type="RPH_Type">
									<xs:annotation>
										<xs:documentation>Optional OJ_SuperPNR_RPH (of the Passenger) associated with this PaymentQualifier</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="FlightSegmentRPH" type="RPH_Type">
									<xs:annotation>
										<xs:documentation>Optional Flight Segment RPH associated with this PaymentQualifier</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Remarks" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Hold remarks about the Payment (added for TravelSky, used primarily for Refund for Error)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="RemarksType">
							<xs:sequence>
								<xs:element name="Remark" type="OJ_RemarkType" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Loyalty" type="PaymentLoyaltyType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="LastModifed" type="xs:date"/>
		<xs:attributeGroup ref="CurrencyAmountGroup"/>
		<xs:attribute name="PaymentDate" type="xs:dateTime">
			<xs:annotation>
				<xs:documentation>The Date the payment was made</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TransactionType" type="TransactionTypeType" use="required">
			<xs:annotation>
				<xs:documentation>Whether this is a payment or  a refund to the customer</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MerchantID">
			<xs:annotation>
				<xs:documentation>Merchant ID when validating payment for certain payment types</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="xs:string">
			<xs:annotation>
				<xs:documentation>Optional string for to describe context of the payment etc.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="PaymentID_Attributes"/>
		<xs:attribute name="Status" type="xs:string">
			<xs:annotation>
				<xs:documentation>Optional string for to describe status of payment i.e. paid, unpaid, deleted etc.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="HistoryAttribute"/>
		<xs:attribute name="SeqNo" type="xs:int"/>
		<xs:attribute name="PackageType" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="OJ_RemarkType">
		<xs:sequence>
			<xs:element name="Text" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The actual text of the remark</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Agent" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The agent who added the remark</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="AgentDetailsType">
							<xs:attribute name="InitialAgent" type="xs:string">
								<xs:annotation>
									<xs:documentation>Name or id of agent who initially created this remark</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="GroupID" type="xs:string" use="optional"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="RefundQualifiers" type="RefundQualifiersType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used to store Refund Review information - TravelSky Specific</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Restricted" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Flags if visible to another Operator</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Supplier_RPH">
			<xs:annotation>
				<xs:documentation>Reference to unique identifier of supplier representation of remark e.g. Hotel/RoomStays/RoomStay/Comments/Comment/@RPH</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Date" type="xs:dateTime" use="optional">
			<xs:annotation>
				<xs:documentation>The date and time the remark was added</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Code" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The code associated with the remark if any</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CodeContext" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The context of the code associated with the remark</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CustomerVisible" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Flags if visible to customer</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Active" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>Indicates if this is an active remark</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Deleted">
			<xs:annotation>
				<xs:documentation>Flags if the remark has been marked deleted</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TicketVisible">
			<xs:annotation>
				<xs:documentation>Flags if visible on Tickets</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="VoucherVisible">
			<xs:annotation>
				<xs:documentation>Flags if visible on Vouchers</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EmailVisible">
			<xs:annotation>
				<xs:documentation>Flags if visible on Emails</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RPH">
			<xs:annotation>
				<xs:documentation>Unique identifier for this remark</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LastModified" type="xs:dateTime">
			<xs:annotation>
				<xs:documentation>The date and time the remark was last modified</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AuditID" type="xs:int" use="required">
			<xs:annotation>
				<xs:documentation>Required for audit trail</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="HistoryRPH" type="xs:int">
			<xs:annotation>
				<xs:documentation>It is used to group together all the remark elements over it's history, it will be set to RPH when the remarks are first created and will not change thereafter</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LiveRPH" type="xs:int">
			<xs:annotation>
				<xs:documentation>Indicates the next remark in the chain of it's history, it will only exist if this is a historical remark</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SeqNo" type="xs:int">
			<xs:annotation>
				<xs:documentation>Number identifying position of spnr item in DB</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="OJ_PaymentFormType">
		<xs:annotation>
			<xs:documentation>This is a generic element to describe "other" payment types.  This should be cross referenced with a datalist to describe what the ref fields represent</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="PaymentCard" type="PaymentCardType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Details of a debit or credit card.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankAcct" type="BankAcctType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Details of a bank account. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DirectBill" type="DirectBillType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Details of a direct billing arrangement.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Voucher">
				<xs:annotation>
					<xs:documentation xml:lang="en">Details of a document indicating prepayment.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="RefundRules" type="CouponRefundRulesType" minOccurs="0"/>
						<xs:element name="ChangeRules" type="CouponChangeRulesType" minOccurs="0"/>
					</xs:sequence>
					<xs:attributeGroup ref="VoucherGroup"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="LoyaltyRedemption">
				<xs:annotation>
					<xs:documentation xml:lang="en">Details of a loyalty redemption arrangement. This is normally miles or points. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attributeGroup ref="LoyaltyCertificateNumberGroup"/>
					<xs:attributeGroup ref="PromotionCodeGroup"/>
					<xs:attribute name="RedemptionQuantity" type="xs:positiveInteger" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The quantity of loyalty units being redeemed.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="MiscChargeOrder">
				<xs:annotation>
					<xs:documentation xml:lang="en">Details of a miscellaneous charge order (MCO).</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="TicketNumber" type="StringLength1to32">
						<xs:annotation>
							<xs:documentation xml:lang="en">The ticket number of the miscellaneous charge order (MCO).</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Cash">
				<xs:annotation>
					<xs:documentation xml:lang="en">Used to indicate payment in cash.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="CashIndicator" type="xs:boolean" use="optional" default="true">
						<xs:annotation>
							<xs:documentation xml:lang="en">If true, this indicates cash is being used.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Other" type="OtherPaymentForm"/>
		</xs:choice>
		<xs:attributeGroup ref="PrivacyGroup"/>
		<xs:attribute name="CostCenterID" type="StringLength1to32" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A reference to identify the billing department for allocating cost of travel to company account.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Provides a reference to a specific form of payment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PaymentTransactionTypeCode" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">This is used to indicate either a charge or reserve (deposit).</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="charge">
						<xs:annotation>
							<xs:documentation xml:lang="en">This indicates that an actual payment has been made.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="reserve">
						<xs:annotation>
							<xs:documentation xml:lang="en">This indicates that a hold for the indicated amount has been placed on a credit card or that a cash amount has been taken from the customer to guarantee final payment.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="PackageType" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="OtherPaymentForm">
		<xs:annotation>
			<xs:documentation>This is a generic element to describe "other" payment types.  This should be cross referenced with a datalist to describe what the ref fields represent</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Ref" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="Code" type="xs:string">
								<xs:annotation>
									<xs:documentation>Code to deifine the reference</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Type" type="xs:int">
			<xs:annotation>
				<xs:documentation>Type of payment this is describing</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PaymentRef" type="xs:string">
			<xs:annotation>
				<xs:documentation>Ref of payment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="RefundQualifiersType">
		<xs:sequence>
			<xs:element name="RefundQualifier" type="RefundQualifierType" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RefundQualifierType">
		<xs:sequence>
			<xs:element name="QualifierItem" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Breakdown of RefundQualifier value attribution.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="Name" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Name of the RefundQualifier item, e.g. tax name.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Value" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Value of the RefundQualifier item.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="QualifierDescription" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Required Description of the RefundQualifier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="QualifierValue" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Required Value of the RefundQualifier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FlightSegmentRPH" type="RPH_Type">
			<xs:annotation>
				<xs:documentation>Optional Flight Segment RPH associated with this RefundQualifier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OJ_SuperPNR_RPH" type="RPH_Type">
			<xs:annotation>
				<xs:documentation>Optional OJ_SuperPNR_RPH (of the Passenger) associated with this RefundQualifier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="eTicketNumber" type="xs:string">
			<xs:annotation>
				<xs:documentation>Optional eTicketNumber associated with this RefundQualifier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PassengerTypeCode" type="AlphaLength3">
			<xs:annotation>
				<xs:documentation>Optional Pax Type code (ADT, CHD) associated with this RefundQualifier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TaxCode" type="StringLength1to16">
			<xs:annotation>
				<xs:documentation>Optional Tax Code associated with this RefundQualifier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="InsuranceHistoryID" type="StringLength1to16">
			<xs:annotation>
				<xs:documentation>Optional InsuranceHistoryID (of the related Insurance Product)associated with this RefundQualifier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="HistoryID" type="xs:integer">
			<xs:annotation>
				<xs:documentation xml:lang="en">History ID of the product associated with this RefundQualifier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductNumber" type="xs:integer">
			<xs:annotation>
				<xs:documentation>The unique number of this product within the booking</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="ProductTypeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="air"/>
			<xs:enumeration value="activity"/>
			<xs:enumeration value="ancillary"/>
			<xs:enumeration value="baggage"/>
			<xs:enumeration value="bundle"/>
			<xs:enumeration value="hotel"/>
			<xs:enumeration value="insurance"/>
			<xs:enumeration value="Seat">
				<xs:annotation>
					<xs:documentation xml:lang="en">This Product Type value is deprecated. It is currently used to request seat amend rules. For example in: OJ_CalcModifyCostRQ/Product/@ProductType</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="seat">
				<xs:annotation>
					<xs:documentation xml:lang="en">This Product Type value is the correct way to indicate a seat product (rather than the deprecated "Seat" value). For example in: OJ_SuperPNR/ModularProduct/@ProductType</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="vehicle"/>
			<xs:enumeration value="car"/>
			<xs:enumeration value="coupon"/>
			<xs:enumeration value="transfer"/>
			<xs:enumeration value="membership"/>
			<xs:enumeration value="XEVENT"/>
			<xs:enumeration value="itineraryReceipt"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PaymentStateType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="PaymentDue"/>
			<xs:enumeration value="FullyPaid"/>
			<xs:enumeration value="Paid"/>
			<xs:enumeration value="Deferred"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:attributeGroup name="PaymentID_Attributes">
		<xs:attribute name="ID" type="xs:integer">
			<xs:annotation>
				<xs:documentation>Unique ID to identiify the payment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AuditID" type="xs:int">
			<xs:annotation>
				<xs:documentation>The SuperPNR Audit ID this  payment refers to</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductList" type="ListOfRPH" use="optional">
			<xs:annotation>
				<xs:documentation>RPH List of products this payment refers to</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PAX_List" type="ListOfRPH" use="optional">
			<xs:annotation>
				<xs:documentation>RPH List of customers this payment is for</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:simpleType name="TransactionTypeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Debit"/>
			<xs:enumeration value="Credit"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AssociatedContentType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Associated content is content (items) associated with the itinerary, but not being part of an itinerary.  Examples of itinerary associated items are, travel insurance, maps, and city information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AssocItems">
				<xs:annotation>
					<xs:documentation xml:lang="en">Collection of Itinerary associated items.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AssocItem" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="en">Itinerary associated item (e.g. Travel Insurance).</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="AssocItemRef" type="UniqueID_Type">
										<xs:annotation>
											<xs:documentation xml:lang="en">Reference to the associated item content.  This could be a URL to map or city information.  This can also be used as a vendor's reference to a reservation (i.e. record locator).</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="RPH" type="RPH_Type" use="required">
									<xs:annotation>
										<xs:documentation xml:lang="en">Reference to this associated item.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="DateTime" type="DateOrDateTimeType">
									<xs:annotation>
										<xs:documentation xml:lang="en">Date and time of start and end of the associated item.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Type" type="OTA_CodeType" use="required">
									<xs:annotation>
										<xs:documentation xml:lang="en">Associated item type (e.g. Convention).</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="Pricing" type="ItinPricingType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">Cost of a single associated item.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SpecialRequestDetails">
							<xs:annotation>
								<xs:documentation xml:lang="en">Remarks, OSIs, Seat Requests etc. </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="SpecialReqDetailsType">
										<xs:sequence>
											<xs:element ref="TPA_Extensions" minOccurs="0"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Pricing" type="ItinPricingType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Cost of ALL associated items.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SpecialRequestDetails">
				<xs:annotation>
					<xs:documentation xml:lang="en">Remarks, OSIs, Seat Requests etc. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="SpecialReqDetailsType">
							<xs:sequence>
								<xs:element ref="TPA_Extensions" minOccurs="0"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element ref="TPA_Extensions" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomerInfoType">
		<xs:sequence>
			<xs:element name="Customer" type="CustomerType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Detailed customer information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProfileRef" type="UniqueID_Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Reference to customer's profile.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remarks" type="StringLength1to64" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Free text remarks associated with this customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AgencyAcctNumber" type="UniqueID_Type">
				<xs:annotation>
					<xs:documentation xml:lang="en">Agency's reference to this customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="RPH" type="RPH_Type">
			<xs:annotation>
				<xs:documentation xml:lang="en">Reference to this customer.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ItinPricingType">
		<xs:sequence>
			<xs:element name="Cost">
				<xs:annotation>
					<xs:documentation xml:lang="en">Cost of this pricing.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="TotalType">
							<xs:attribute name="FormOfPaymentRPH" type="RPH_Type">
								<xs:annotation>
									<xs:documentation xml:lang="en">Reference to the Form of Payment used in association to this cost.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element ref="TPA_Extensions" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="ItemRPH_List" type="ListOfRPH">
			<xs:annotation>
				<xs:documentation xml:lang="en">Reference to itinerary reservation item (e.g. Car, Hotel, Air, Package).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AssociatedItemRPH" type="ListOfRPH">
			<xs:annotation>
				<xs:documentation xml:lang="en">Reference to associated itinerary item (e.g. Travel Insurance).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ItineraryInfoType">
		<xs:sequence>
			<xs:element name="ContactInfo" type="ContactPersonType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Additional contact information eg. travel agency.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReservationItems" type="ReservationItemsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Itinerary reservation items (e.g. Car, Hotel, Air, Package).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Ticketing" type="TicketingFullInfo_Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Air reservation ticket information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ItineraryPricing" type="ItinPricingType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Cost of ALL itinerary reservation items.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SpecialRequestDetails">
				<xs:annotation>
					<xs:documentation xml:lang="en">Remarks, OSIs, Seat Requests etc. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="SpecialReqDetailsType">
							<xs:sequence>
								<xs:element ref="TPA_Extensions" minOccurs="0"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element ref="TPA_Extensions" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ReservationItemsType">
		<xs:sequence>
			<xs:element name="Item" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A single reservation item (e.g. Hotel, Car, Air, Package).</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="TravelSegmentType">
							<xs:attribute name="ItinSeqNumber" type="xs:nonNegativeInteger">
								<xs:annotation>
									<xs:documentation xml:lang="en">Number identifying position of reservation item in itinerary.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="IsPassive" type="xs:boolean" default="false">
								<xs:annotation>
									<xs:documentation xml:lang="en">When true, indicates that the reservation was completed externally and the information is for reference only.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="IssueBoardingPass" type="xs:boolean">
								<xs:annotation>
									<xs:documentation xml:lang="en">Can a boarding pass(es) be issued for this Itinerary?  True=yes, False=no.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="BoardingPassIssued" type="xs:boolean">
								<xs:annotation>
									<xs:documentation xml:lang="en">Has a boarding pass(es) been issued for this Itinerary?  True=yes, False=no.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="ItemPricing" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Cost of a single reservation item.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="ItinPricingType">
							<xs:sequence>
								<xs:element name="AirFareInfo" type="BookingPriceInfoType">
									<xs:annotation>
										<xs:documentation xml:lang="en">Information that is related to an invoice of this pricing like ticketing.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="SpecialRequestDetails" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Remarks, OSIs, Seat Requests etc. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="SpecialReqDetailsType">
							<xs:sequence>
								<xs:element ref="TPA_Extensions" minOccurs="0"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ChronoOrdered" type="xs:boolean">
			<xs:annotation>
				<xs:documentation xml:lang="en">If TRUE, indicates items are in chronological order.  If FALSE, indicates items are not in chronological order.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TravelCostType">
		<xs:sequence>
			<xs:element name="FormOfPayment" type="PaymentFormType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Form of payment information (e.g. VISA, Debit Card, Check).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CostTotals" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Total cost associated with the ENTIRE itinerary.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="TotalType">
							<xs:attribute name="FormOfPaymentRPH" type="RPH_Type">
								<xs:annotation>
									<xs:documentation xml:lang="en">Reference to a form of payment.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element ref="TPA_Extensions" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TravelSegmentType">
		<xs:choice>
			<xs:element name="Air">
				<xs:annotation>
					<xs:documentation xml:lang="en">Flight reservation information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="Reservation">
							<xs:annotation>
								<xs:documentation xml:lang="en">Flight reservation information.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="BookFlightSegmentType">
										<xs:sequence>
											<xs:element name="Seats" minOccurs="0">
												<xs:annotation>
													<xs:documentation xml:lang="en">All seat information.</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="Seat" maxOccurs="unbounded">
															<xs:annotation>
																<xs:documentation xml:lang="en">Seat information associated with a single reservation item and customer.</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:attribute name="CustomerRPH" type="RPH_Type">
																	<xs:annotation>
																		<xs:documentation xml:lang="en">Reference to customer assigned to this seat.</xs:documentation>
																	</xs:annotation>
																</xs:attribute>
																<xs:attribute name="Number" type="AlphaNumericStringLength1to8">
																	<xs:annotation>
																		<xs:documentation xml:lang="en">Seat number (e.g. 20A, 17C, etc.).</xs:documentation>
																	</xs:annotation>
																</xs:attribute>
																<xs:attribute name="Characteristic" type="StringLength1to64">
																	<xs:annotation>
																		<xs:documentation xml:lang="en">Seat characteristic.  Suggested values include: Window, Aisle, EconomyPlus, etc.</xs:documentation>
																	</xs:annotation>
																</xs:attribute>
																<xs:attribute name="SeatLocation" type="StringLength1to16">
																	<xs:annotation>
																		<xs:documentation xml:lang="en">Location of seat in cabin of conveyance.  Suggested values include: Forward, Middle, Aft, ExitRow, Bulkhead, Right or Left Side, etc.</xs:documentation>
																	</xs:annotation>
																</xs:attribute>
															</xs:complexType>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element ref="TPA_Extensions" minOccurs="0"/>
										</xs:sequence>
										<xs:attribute name="OrginDestType">
											<xs:annotation>
												<xs:documentation xml:lang="en">Intermediate and last segments contain the RPH of the first segment for an Origin and Destination set.</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKEN">
													<xs:enumeration value="First"/>
