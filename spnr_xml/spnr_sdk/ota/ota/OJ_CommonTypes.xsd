	<xs:complexType name="BiasingConfig">
		<xs:annotation>
			<xs:documentation>Details of the bias configuration e.g Module Type, Module Weight</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ModuleType" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The module type is type of bias module  e.g  bookings, views, default</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BiasWeight" use="required">
			<xs:annotation>
				<xs:documentation>Each bias module is assigned a weight which is relative to other bias modules</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SupplierCode" use="optional">
			<xs:annotation>
				<xs:documentation>Each bias module could be associated to a supplier - code of the supplier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SupplierName" use="optional">
			<xs:annotation>
				<xs:documentation>Each bias module could be associated to a supplier - name of the supplier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="BiasingConfigSet">
		<xs:annotation>
			<xs:documentation>Defines biasing configuration set</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="BiasingConfig" type="BiasingConfig" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="TestID" use="required">
			<xs:annotation>
				<xs:documentation>The test identifier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TestName" use="optional">
			<xs:annotation>
				<xs:documentation>The test name</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TestToken" use="required">
			<xs:annotation>
				<xs:documentation>The test token</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TestLocation" use="optional">
			<xs:annotation>
				<xs:documentation>Each test could be associated to a location - identifier of the location</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ProductBiasList">
		<xs:annotation>
			<xs:documentation>Product details of a particular product e.g Hotel, Event</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="Product" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Product item e.g Hotel item, Car Depot item</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence maxOccurs="unbounded">
						<xs:element name="Module" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Module item e.g bookings bias module, trip advisor module .</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="Value" use="required">
									<xs:annotation>
										<xs:documentation>The Bias Value of this Product for this module .</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="ModuleType" use="required">
									<xs:annotation>
										<xs:documentation>Module Type Name .</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="Code" use="required">
						<xs:annotation>
							<xs:documentation>Unique code of the product item. e.g For Hotel items it would be OJHotelCode</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="OverallBias" use="required">
						<xs:annotation>
							<xs:documentation>The Overall Bias value for this Product </xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="SupplierCode" use="optional">
						<xs:annotation>
							<xs:documentation>The supplier code that provides this Product</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="RequestDate" type="xs:date" use="required"/>
		<xs:attribute name="TestID" use="required"/>
	</xs:complexType>
	<xs:complexType name="Module">
		<xs:annotation>
			<xs:documentation>Details relating to a biasing module e.g bookings bias module, trip advisor module .</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Products" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Product details of a particular product e.g Hotel, Event</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence maxOccurs="unbounded">
						<xs:element name="Product" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Product item e.g Hotel item, Car Depot item</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="Code" type="xs:string" use="required">
									<xs:annotation>
										<xs:documentation>Unique code of the product item. e.g For Hotel items it would be OJHotelCode</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Unit" type="xs:string" use="required">
									<xs:annotation>
										<xs:documentation>The product item unit value is used to calculate the product item metric value. e.g no of product bookings, no of product views, star rating </xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="LocationCode">
						<xs:annotation>
							<xs:documentation>A location code for some modules is required. e.g hotel bookings bias module returns a list of hotels per location</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Destination" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Destination of the module details e.g xHotel Supplier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Source" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Source of the module details e.g TripAdvisor Supplier, XReport Supplier for booking and view details</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Duration" type="xs:int" use="required">
			<xs:annotation>
				<xs:documentation>The no of days between present date and a past date.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Type" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Type of bias module e.g Booking Bias Module, TripAdvisor Module</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The type of Product the bias type has been applied to</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="CabinDetails">
		<xs:attribute name="CategoryLocation" type="CategoryLocationType" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Category location in the ship.
I  : Inside
O : Outside</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BerthedCategoryCode" type="StringLength1to8" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Contains the cruise line's category code for the general cabin location. BerthedCategoryCode will differ from the PricedCategoryCode if the customer receives an upgrade.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MaxCabinOccupancy" type="Numeric1to99" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Maximum cabin occupancy available on this specific sailing (ship).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ItineraryRPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Used to indicate on which leg(s) of the booking the passengers party is travelling.  If not supplied then 'all legs' is assumed.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="CabinDetail">
		<xs:sequence>
			<xs:element name="CabinDetails" type="CabinDetails" minOccurs="0" maxOccurs="99"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FerryProductType">
		<xs:sequence>
			<xs:element name="Originator" type="FerryPOS_Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains details identifying the party or connection channel making the request.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ItineraryLeg" maxOccurs="2">
				<xs:annotation>
					<xs:documentation>Details of a ferry sailing.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="BaseItineraryType">
							<xs:sequence>
								<xs:element name="AltItinerary" type="BaseItineraryType" minOccurs="0" maxOccurs="20">
									<xs:annotation>
										<xs:documentation>AltItinerary elements are returned as response when either : a) the relevant requested Itinerary does not match any existing crossing, or  b) theTimetableInd attribute is "true", or c) the TimetableInd is "false" or not present and the related Itinerary cannot accommodate the requested passengers and/or vehicle.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="AltAccommodation" type="AccomType" minOccurs="0" maxOccurs="30">
									<xs:annotation>
										<xs:documentation>AltAccomodation elements are returned regardless of the TimetableInd whenever the requested Itinerary matches an existing crossing and that crossing can accommodate the requested passengers and/vehicle.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="PassengerCount" type="FerryTravellerCountType" minOccurs="0" maxOccurs="12">
				<xs:annotation>
					<xs:documentation xml:lang="en">Describes the numbers of passengers by category, e.g. Adults, Children.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinDetails" type="CabinDetails" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Details of cabins and occupancy</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VehicleDetails" type="VehicleDetailsType" minOccurs="0" maxOccurs="2">
				<xs:annotation>
					<xs:documentation xml:lang="en">Details of the vehicle and trailer (if any) on one or both itinerary legs. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PetCount" type="PetCountType" minOccurs="0" maxOccurs="4">
				<xs:annotation>
					<xs:documentation xml:lang="en">A count of the number of pets by type on each itinerary leg.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PassengerDetails" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A collection of individual passenger details.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PassengerDetail" type="PassengerDetailType" maxOccurs="19"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="BookingReferenceID">
			<xs:annotation>
				<xs:documentation>Confirmed booking reference for this product</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:attributeGroup name="CommonProductAttributes">
		<xs:attribute name="BookingStatus" type="xs:string" use="required"/>
		<xs:attribute name="CancellationDate" type="xs:dateTime" use="optional">
			<xs:annotation>
				<xs:documentation>The date the product was cancelled </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BookingDate" type="xs:dateTime">
			<xs:annotation>
				<xs:documentation>The date the product was originally made</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="StartDate" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation>The date of commencement of the product</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EndDate" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation>The date of commencement of the product</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PromotionType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The code of any promotion or offer the sale was made through</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LastModified" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation>The last time this item was modified</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Destination" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The xLocation id of the primary destination of this SPNR.  Calculated as follows...The way destination should be calculated:-
* Sort all transport products (currently only air) into travel date order
* Take first arrival point (that isn't a connection) as first destination
* Take arrival date as destination arrival date
* Take departure of next flight as destination departure date
  (if only one way flights assume departure is in +7 days)

IF there are no air/transport segments, then do first accommodation location
(and stay duration) as the destination.

If there are no air or hotel, then use first car location.

If there is only an extra then use extra location and date as destination and
arrival and +7 days as departure.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>

	<xs:attributeGroup name="CommonAncillaryProductCancelAttributes">
		<xs:attribute name="Code" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Code of product in booking to be cancelled.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BookingReferenceID" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Booking reference of booking to be cancelled.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BookingStatus" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Status to set on existing booking.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ID" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The ID of the product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SupplierCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Supplier code</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SupplierProductCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Supplier code of product in booking to be cancelled.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Type" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Product code for a specific type, such as ABC for lounge access, or XYZ for in-flight entertainment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>

	<xs:attributeGroup name="MarkupPromotionsAttributes">
		<xs:attribute name="Code2" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Any code associated with this item</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Stage">
			<xs:annotation>
				<xs:documentation>Stage where the adjustement has been added</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The type of Product the price adjustment has been applied to</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Exclusivity" use="optional">
			<xs:annotation>
				<xs:documentation>The Product Type the price adjustment has been applied to</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="ANY"/>
					<xs:enumeration value="ONCE"/>
					<xs:enumeration value="EXCLUSIVE"/>
					<xs:enumeration value="PARTIALLY_EXCLUSIVE"/>
					<xs:enumeration value="ONCE_GROUP"/>
					<xs:enumeration value="EXCLUSIVE_GROUP"/>
					<xs:enumeration value="EXCLUSIVE_GROUP_ITINERARY"/>
					<xs:enumeration value="ALWAYS_APPLY"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Group" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The Product Group the price adjustment has been applied to</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Modular" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates whether this price adjustment was applied to the product at a modular level</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="true"/>
					<xs:enumeration value="false"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Percent" type="Percentage" use="optional">
			<xs:annotation>
				<xs:documentation>The percentage value of the applied price adjustment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:complexType name="PriceAdjustmentsType">
		<xs:sequence>
			<xs:element name="PriceAdjustment" type="PriceAdjustmentType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A charge or discount applied to the product</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="CurrencyAmountGroup"/>
	</xs:complexType>
	<xs:complexType name="PriceAdjustmentType">
		<xs:attributeGroup ref="PriceAdjustmentBaseAttributes"/>
		<xs:attributeGroup ref="PriceAdjustmentAttributes"/>
		<xs:attributeGroup ref="CurrencyAmountGroup"/>
	</xs:complexType>
	<xs:complexType name="BasePriceType">
		<xs:attribute name="Amount" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation>The amount of the base price</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PaymentCardHolderType">
		<xs:attribute name="Name" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The name on the card</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EmailAddress" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The email address of the card holder</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PhoneNumber" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The contact phone number of the card holder</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PostalAddress" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The billing address of the card holder</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PaymentCardDetailsType">
		<xs:sequence>
			<xs:element name="CardHolder" type="PaymentCardHolderType">
				<xs:annotation>
					<xs:documentation>The details of the account owner of the card</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Number" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The car number</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Type" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The card type eg AMEX, VISA, Switch etc</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ExpiryDate" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The date on which the card expires</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PointOfSaleType">
		<xs:attribute name="Id" type="xs:integer" use="required">
			<xs:annotation>
				<xs:documentation>The Id of the POS (a reference to the ID in the Location Hierarchy)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SaleCurrency" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The currency in which the overall product has been sold</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ReportingCurrency" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The currency for which rates must be recorded for transmission to the financial system</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:attributeGroup name="CommonProductsAttributes">
		<xs:attribute name="Total" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation>The sum total all component products with booking status of booked excluding amendment and cancellation fees</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TotalAmendmentFees" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation>The sum of amendment fees  from all component products</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TotalCancellationFees" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation>The sum of cancellation fees  from all component products</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="ProductIDs">
		<xs:attribute name="ProductNumber" type="xs:integer">
			<xs:annotation>
				<xs:documentation>The unique number of this product within the booking</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MasterProductNumber" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>If this product is an add-on to another product, this holds the number of that master product. It can used when compositing a booking message to group the products together</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="ShareRewardGroup">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines information related to sharing rewards with other related travelers.</xs:documentation>
		</xs:annotation>
