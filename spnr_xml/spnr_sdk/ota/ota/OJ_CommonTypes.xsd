		<xs:attribute name="Source" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates the source used to determine the refund. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProductType" type="xs:string">
			<xs:annotation>
				<xs:documentation xml:lang="en">Types of product:
air
seat
meal
baggage</xs:documentation>
			</xs:annotation>
