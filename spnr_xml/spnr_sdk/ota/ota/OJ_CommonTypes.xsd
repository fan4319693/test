			<xs:annotation>
				<xs:documentation>The MIME type of the file.  Default to application/octet-stream</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Content-Description" type="xs:string">
			<xs:annotation>
