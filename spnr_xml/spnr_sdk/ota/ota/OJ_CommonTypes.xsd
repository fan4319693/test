					<xs:documentation xml:lang="en">Indicates failed to issue ticket and system guarantee time out.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TICKETED">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates the product has been ticketed successfully.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BOOKED">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates product book successfull.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="USED">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates product has been used.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="APPLIED">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates product has already applied.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BOOK_FAILED">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates product book failed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="COMPLETED">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates the product has been completed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EXPIRED">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates product has been expired</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="STATION_INBOUND">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates product has been inbound.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="STATION_OUTBOUND">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates product has been outbound.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UPGRADE_FAILED">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates upgrade product failed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UPGRADED">
				<xs:annotation>
					<xs:documentation xml:lang="en">Indicates upgrade product successfull</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FORCE_REFUND_REVIEW">
				<xs:annotation>
					<xs:documentation xml:lang="en">This type of refund is not restrict by refund rules, it usually initiated by the administrator.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CancellationRefundsType">
		<xs:sequence>
			<xs:element name="CancellationRefund" type="CancellationRefundType" minOccurs="0" maxOccurs="unbounded">
