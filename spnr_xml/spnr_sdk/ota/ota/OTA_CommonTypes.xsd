				<xs:documentation xml:lang="en">State or Province where the document was issued.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DocIssueDate" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Indicates the date of the document was issued, in ISO 8601 prescribed format.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DocIssueCountry" type="ISO3166" use="optional">
