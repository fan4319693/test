				<xs:documentation xml:lang="en">Specifies the birth country of the document holder.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BirthPlace" type="StringLength1to64" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Specifies the birth place of the document holder (e.g., city, state, county, province), when designating a country of birth, use BirthCountry.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Remark" type="StringLength1to128" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">A remark associated with the document.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DocHolderNationality" type="ISO3166">
			<xs:annotation>
				<xs:documentation xml:lang="en">The country code of the nationality of the document holder.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
