				<xs:documentation xml:lang="en">Indicates the location where the document was issued.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DocID" type="StringLength1to64" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The unique identification number assigned by the authorities to the document.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EncryptedDocID" type="StringLength1to64" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The encrypted text for DocID.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DocType" type="OTA_CodeType" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Indicates the type of document (for example, Passport, Military ID, Drivers License, national ID, Vaccination Certificate). Refer to OTA Code List Document Type (DOC).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="GenderGroup"/>
		<xs:attributeGroup ref="BirthDateGroup"/>
		<xs:attributeGroup ref="EffectiveExpireOptionalDateGroup"/>
		<xs:attribute name="DocIssueStateProv" type="StateProvCodeType" use="optional">
