	<xs:simpleType name="AccomPositionType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The area in which the on-board accommodation: is located: I=Inner O=Outer</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="I">
				<xs:annotation>
					<xs:documentation xml:lang="en">Inner</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="O">
				<xs:annotation>
					<xs:documentation xml:lang="en">Outer</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FerryLocationType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The section in which the on-board accommodation: is located: P=Port S=Starboard F=Forward A=Aft</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="P">
				<xs:annotation>
					<xs:documentation xml:lang="en">Port</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="S">
				<xs:annotation>
					<xs:documentation xml:lang="en">Starboard</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="F">
				<xs:annotation>
					<xs:documentation xml:lang="en">Forward</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation xml:lang="en">Aft</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FerryClassType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Class code for travel or service: 1= 1st class 2=2nd class 3=3rd Class C=Business/Club class U=Shuttle service.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">1st class</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation xml:lang="en">2nd class</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation xml:lang="en">3rd class</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation xml:lang="en">Business/Club class</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="U">
				<xs:annotation>
					<xs:documentation xml:lang="en">Shuttle service</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="FerryInvoiceDetail">
		<xs:annotation>
			<xs:documentation xml:lang="en">A list of all the items making up a costed booking</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CostingItems" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">A collection of invoice item lines.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CostingItem" maxOccurs="99">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="CostingItemType">
										<xs:attribute name="Tag" type="xs:string" use="optional">
											<xs:annotation>
												<xs:documentation xml:lang="en">If present, this attribute may identify the booking item being priiced. It is recommended that the Tag attribute use XPath notation to identify the location of a tag in the event that more than one tag of the same name is present in the document.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="GrossAmount" type="CurrencyAmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The total booking cost to the customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepositAmount" type="CurrencyAmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The amount payable at the time of booking to secure the booking.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AgentCommission" type="AgentCommissionType" minOccurs="0" maxOccurs="8">
				<xs:annotation>
					<xs:documentation xml:lang="en">The total fee earned by the agent for the booking.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NetAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The amount payable by the agent, i.e. GrossAmount less Commission less Tax.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="CurrencyAmountType">
							<xs:attribute name="PaymentDueDate" type="xs:date" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">Date on which payment must be received.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="TaxItems" type="TaxesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">A collection of tax item lines.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceDueAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The amount remaining to be paid by the customer i.e. GrossAmount less the greater of DepositAmount and Amount Received.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="CurrencyAmountType">
							<xs:attribute name="PaymentDueDate" type="xs:date" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">Date on which payment must be received.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="AmountReceived" type="CurrencyAmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The payment amount received against the reservation.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
