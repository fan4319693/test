	<!--  Complex Types ============================================================================================================================-->
	<xs:complexType name="AccomType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Details of a type of onboard accommodation, or of a specific unit of onboard accommodation.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Places">
				<xs:annotation>
					<xs:documentation xml:lang="en">Number and characteristics of available or requested places (berths, seats) in the accommodation</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="AvailableQty" type="Numeric1to99" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">Number of cabins, seats or berths available. Normally only present in a response.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="RequiredQty" type="Numeric1to99" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">Number of cabins, seats or berths required</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="OccupancyMode" type="OccupancyModeType" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The form of accommodation occupancy: B = by berth; C = complete unit or seat </xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attributeGroup ref="GenderGroup">
						<xs:annotation>
							<xs:documentation xml:lang="en">Identifies the sex of the occupants of a cabin i.e. M = all male, F = all female; U = mixed or unknown. Not required for Seats. </xs:documentation>
						</xs:annotation>
					</xs:attributeGroup>
					<xs:attribute name="Characteristic" type="AlphaLength1" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">Normally used with a value of 'D' to request accommodation suitable for disabled passengers. Name changed from 'Type' for version 2 - tw 4Nov05</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="AccomRef" minOccurs="0" maxOccurs="999">
				<xs:annotation>
					<xs:documentation xml:lang="en">References to specific accommodation units requested or offered  e.g. seat numbers </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="StartAccomID" type="StringLength1to8" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">Identity of the first accommodation unit requested or offered. If only a single accommodation reference is being suppplied, EndAccomID will be omitted. </xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="EndAccomID" type="StringLength1to8" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">Identity of the last accommodation unit requested or offered. </xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="BerthID" type="AlphaLength1" use="optional"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="RPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A place holder to enable this occurrence to be referenced from another element. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Type" type="FerryAccomType" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The type of accommodation: B = bedded cabin; C = couchette; S = Seat.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccomCode" type="StringLength1to8" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The identity of a specific accommodation unit.  If it is supplied, then the other attributes are not normally required.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="StringLength1to32" use="optional"/>
		<xs:attribute name="Occupancy" type="Numeric0to99" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Defines the number of berths in a cabin; it is normally set to 1 for seats. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Quality" type="AccomQualityType" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Indicates the quality of the accommodation:  L=Luxe N=Normal P=Pullman Seat R=Reclining Seat S=Special T=Tourist V=De Luxe X=Super de Luxe Y=Economy</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Sanitary" type="AccomSanitaryType" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The type of sanitary arrangements provided:	B=Bath and WC H=Shower and WC I=Bath Shower and WC V=Washbasin and WC W=Washbasin Y=WC</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Position" type="AccomPositionType" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The area in which the on-board accommodation: is located: I=Inner O=Outer</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Deck" type="AlphaLength1" use="optional"/>
		<xs:attribute name="Location" type="FerryLocationType" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The section in which the on-board accommodation: is located: P=Port S=Starboard F=Forward A=Aft</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Class" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Class code for travel or service: 1= 1st class 2=2nd class 3=3rd Class C=Business/Club class U=Shuttle service.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="1"/>
					<xs:enumeration value="2"/>
					<xs:enumeration value="3"/>
					<xs:enumeration value="C"/>
					<xs:enumeration value="U"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ItineraryRQ_Type">
		<xs:annotation>
			<xs:documentation xml:lang="en">Itinerary information provided in a booking or amendment request</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Depart" type="LocationTimeType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Departure port details</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Arrive" type="PortLocationType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Arrival port details</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Accommodation" type="AccomType" minOccurs="0" maxOccurs="25"/>
		</xs:sequence>
		<xs:attribute name="RPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A place holder to enable this occurrence to be referenced from another element. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="DepartType">
		<xs:simpleContent>
			<xs:extension base="LocationTimeType">
				<xs:attribute name="NonStandardDateTime" type="xs:dateTime" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">The actual operation date and time for the sailing, e.g for tidal changes. </xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ReportingDateTime" type="xs:dateTime" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">The date and/or time at which the travelers should check in for a sailing.  Only applicable to a departure location. </xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ArriveType">
		<xs:simpleContent>
			<xs:extension base="LocationTimeType">
				<xs:attribute name="NonStandardDateTime" type="xs:dateTime" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">The actual operation date and time for the sailing, e.g for tidal changes. </xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="BaseItineraryType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Itinerary information provided in a response</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Depart" type="DepartType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Departure port details</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Arrive" type="ArriveType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Arrival port details</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="RPH" type="RPH_Type" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">A place holder to enable this occurrence to be referenced from another element. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ShipName" type="StringLength1to32" use="required">
			<xs:annotation>
				<xs:documentation>Name of ship for this leg</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PaxInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">When 'true', passenger space is available; when 'false', passenger space is not available.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="StandardUnitsInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">When 'true', space for standard vehicles is available; when 'false', space for standard vehicles is not available.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="HighUnitsInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">When 'true', space for high vehicles is available; when 'false', space for high vehicles is not available.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CabinsInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">When 'true', cabins are available; when 'false', cabins are not available.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SeatsInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">When 'true', seating space is available; when 'false', seating space is not available.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccomMandatoryInd" type="xs:boolean" use="optional" default="false">
			<xs:annotation>
				<xs:documentation xml:lang="en">When 'true', accommodation must be booked on the sailing. When 'false', accommodation is optional on the sailing. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="StandbyInd" type="xs:boolean" use="optional" default="false">
			<xs:annotation>
				<xs:documentation xml:lang="en">When 'true', this leg has been created using standby availability.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="StandbyRPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">If this is a standby leg being created as a replacement for an existing leg in the booking then this attribute holds the RPH of the leg being replaced.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="BookingSummaryType">
		<xs:sequence>
			<xs:element name="BookingRef" type="FerryUniqueID_Type"/>
		</xs:sequence>
		<xs:attribute name="BookedDateTime" type="DateOrDateTimeType" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The date and/or time the booking was made.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BookingStatus" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Coded status of requested or actual booking: C Cancelled; F Firm; H On Hold; O On Request; P Provisional (Option); T Firm and Ticketed</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="C"/>
					<xs:enumeration value="F"/>
					<xs:enumeration value="H"/>
					<xs:enumeration value="O"/>
					<xs:enumeration value="P"/>
					<xs:enumeration value="T"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="ExpiryDateTime" type="DateOrDateTimeType" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The date and/or time at which a provisional (option) booking will expire or lapse.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AgentRef" type="StringLength1to20" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The reference given to the booking by the agent..</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ItineraryType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Details of a ferry sailing provided in a response message</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseItineraryType">
				<xs:sequence>
					<xs:element name="Accommodation" type="AccomType" minOccurs="0" maxOccurs="25"/>
				</xs:sequence>
			</xs:extension>
			<!--<xs:extension base="ItineraryRS_Type"> -->
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="LocationTimeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Provides the location and time(s) for a sailing departure or arrival.  temporarily? added tw 05Dec05 </xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="PortLocationType">
				<xs:attribute name="ScheduledDateTime" type="xs:dateTime" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="en">The scheduled date and time for the sailing as published in brochure etc.. </xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="OnboardAccomType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Details of onboard accommodation (cabins or seats) available of requested on a vessel or sailing.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="RPH" type="RPH_Type" use="optional"/>
		<xs:attribute name="Type" type="StringLength1to16" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Describes the type of cabin or seat requested or available. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="CodeListGroup">
			<xs:annotation>
				<xs:documentation xml:lang="en">Provides attributes for the code value and source and the quantity for the cabin or seat type.</xs:documentation>
			</xs:annotation>
		</xs:attributeGroup>
		<xs:attribute name="ID" type="StringLength1to16" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A string value representing the unique identification of a cabin or seat.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="StringLength1to64" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A string value representing the description of a cabin or seat.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Deck" type="Numeric1to4" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Indicates the deck on which the accommodation is placed. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PassengerDetailType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Details of a travelling passenger.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="FerryPersonNameType"/>
			<xs:element name="SpecialNeed" minOccurs="0" maxOccurs="5">
				<xs:annotation>
					<xs:documentation xml:lang="en">A special requirement e.g. wheelchair or vegetarian meal. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="StringLength0to64">
							<xs:attribute name="Code" type="StringLength1to8" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Age" type="Numeric0to999" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">This is used to specify age in years.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Code" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Identifies a passenger age band.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="A">
						<xs:annotation>
							<xs:documentation xml:lang="en">Adult</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="C">
						<xs:annotation>
							<xs:documentation xml:lang="en">Child</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="I">
						<xs:annotation>
							<xs:documentation xml:lang="en">Infant</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="S">
						<xs:annotation>
							<xs:documentation xml:lang="en">Senior</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="O">
						<xs:annotation>
							<xs:documentation xml:lang="en">Other</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="RPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">(Reference Place Holder) - an index code to identify an instance in a collection of like items.. For example,  used to assign individual passengers or clients to particular itinerary items.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="InsuranceRPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Used to associate this passenger with their insurance cover provided by the supplier.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="GenderGroup"/>
		<xs:attribute name="Nationality" type="ISO3166" use="optional"/>
		<xs:attributeGroup ref="BirthDateGroup"/>
		<xs:attribute name="LeadCustomerInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">When 'true', indicates that this is the 'lead' passenger (i.e., the primary contact making the booking).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ItineraryRPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Used to indicate on which leg(s) of the booking the passenger is travelling.  If not supplied then 'all legs' is assumed.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="CabinDetailsType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Information on the vehicle and optionally trailer to be carried.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OccupancyCount" type="FerryTravellerCountType" maxOccurs="5"/>
		</xs:sequence>
		<xs:attribute name="RPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A place holder to enable this occurrence to be referenced from another element. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ItineraryRPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Used to indicate on which leg(s) of the booking the vehicle is travelling.  If not supplied then 'all legs' is assumed.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="QuotationType">
		<xs:sequence>
			<xs:element name="Tariff" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">This may be specified to request the supplier system to apply a charging basis other than the default for the account number being used</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="Promotion" type="StringLength1to10" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">The promotional code used to price a booking.  'Use' changed to required for vn 2.0 tw 12Dec05</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<!-- remaining attributes removed for Vn 2.0 tw 12Dec05
					<xs:attribute name="Type" type="StringLength1to8" use="optional"/>
					<xs:attribute name="TypeDesc" type="StringLength1to35" use="optional"/>
					<xs:attribute name="Band" type="StringLength1to4" use="optional"/>
					<xs:attribute name="BandDesc" type="StringLength1to35" use="optional"/>
					<xs:attribute name="PromotionDesc" type="StringLength1to35" use="optional"/>
					-->
				</xs:complexType>
			</xs:element>
			<xs:element name="Scheme" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The Scheme element allows details of promotional schemes such as frequent traveller clubs to be specified. At least one of Identifiaction and Authorisation elements should be present.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="Authorisation" type="StringLength1to25" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en">This is used typically to supply the membership number.  Use changed to required for Vn 2.0 tw 12Dec05</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Category" use="optional" default="Member">
						<xs:annotation>
							<xs:documentation xml:lang="en">The category of scheme membership.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="StringLength1to35">
								<xs:enumeration value="Member"/>
								<xs:enumeration value="Guest"/>
								<xs:enumeration value="Associate"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<!--Remaining attributes rremoved for Vn2.0 tw 12Dec05
					<xs:attribute name="Identification" type="StringLength1to17" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">This is used to identify the promotional scheme</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="TransactionUnits" type="xs:integer" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">This can be used to specify a number of scheme points to be use in payment or part payment of the booking</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="BandDesc" type="StringLength1to35" use="optional"/>
					<xs:attribute name="Description" type="StringLength1to35" use="optional"/>
					<xs:attribute name="Level" type="StringLength1to35" use="optional"/>
					<xs:attribute name="CarriedUnits" type="xs:integer" use="optional"/>
					-->
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OccupancyCountType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Information on the vehicle and optionally trailer to be carried.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OccupancyCount" type="FerryTravellerCountType" maxOccurs="5"/>
		</xs:sequence>
		<xs:attribute name="RPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A place holder to enable this occurrence to be referenced from another element. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ItineraryRPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Used to indicate on which leg(s) of the booking the vehicle is travelling.  If not supplied then 'all legs' is assumed.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="VehicleDetailsType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Information on the vehicle and optionally trailer to be carried.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Vehicle">
				<xs:annotation>
					<xs:documentation xml:lang="en">Identifies a towing vehicle, including attributes for dimensions</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="VehicleBaseType">
							<xs:sequence>
								<xs:element name="VehiclePricing" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="OriginalUnitPrice" minOccurs="0">
												<xs:annotation>
													<xs:documentation xml:lang="en">Original default price</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:attributeGroup ref="CurrencyAmountGroup"/>
												</xs:complexType>
											</xs:element>
											<xs:element name="UnitPrice" minOccurs="0">
												<xs:annotation>
													<xs:documentation xml:lang="en">Price of each Vehicle of this type</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:attributeGroup ref="CurrencyAmountGroup"/>
												</xs:complexType>
											</xs:element>
											<xs:element name="TotalPrice">
												<xs:annotation>
													<xs:documentation xml:lang="en">Total price of all vehicles of this type</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:attributeGroup ref="CurrencyAmountGroup"/>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="Code" type="StringLength1to4" use="required">
								<xs:annotation>
									<xs:documentation xml:lang="en">The vehicle class (see Unicorn dataset U294)</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="BikesOnBackIndicator" type="xs:boolean" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">When 'true', indicates that the vehicle is longer than standard because it is carrying bicycles on the rear.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Trailer" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Identifies a towed vehicle, including attributes for dimensions</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="VehicleBaseType">
							<xs:sequence>
								<xs:element name="TrailerPricing" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="OriginalUnitPrice" minOccurs="0">
												<xs:annotation>
													<xs:documentation xml:lang="en">Original default price</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:attributeGroup ref="CurrencyAmountGroup"/>
												</xs:complexType>
											</xs:element>
											<xs:element name="UnitPrice" minOccurs="0">
												<xs:annotation>
													<xs:documentation xml:lang="en">Price of each Trailer of this type</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:attributeGroup ref="CurrencyAmountGroup"/>
												</xs:complexType>
											</xs:element>
											<xs:element name="TotalPrice">
												<xs:annotation>
													<xs:documentation xml:lang="en">Total price of all Trailers of this type</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:attributeGroup ref="CurrencyAmountGroup"/>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="Code" type="StringLength1to4" use="required">
								<xs:annotation>
									<xs:documentation xml:lang="en">The vehicle class (see Unicorn dataset U286). </xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="BulkVehicleInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">When 'true', no individual vehicle information is present; the vehicle class and number of vehicles are contained in the Code and Quantity attributes of this element.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Quantity" type="Numeric1to999" use="optional"/>
		<xs:attribute name="RPH" type="RPH_Type" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">A place holder to enable this occurrence to be referenced from another element. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ItineraryRPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Used to indicate on which leg(s) of the booking the vehicle is travelling.  If not supplied then 'all legs' is assumed.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="VehicleBaseType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Identifies a vehicle, including  attributes for dimensions</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="Dimensions"/>
		<xs:attribute name="Registration" type="StringLength0to32" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The registration number of a vehicle. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MakeModel" type="StringLength1to16" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Added by Pharos Datacom pending adoption by TTI.  The make/model of a vehicle. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Weight" type="xs:integer" use="optional"/>
	</xs:complexType>
	<xs:complexType name="FerryPOS_Type">
		<xs:annotation>
			<xs:documentation xml:lang="en">Point of Sale (POS) is the details identifying the party or connection channel making the request.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Source" type="FerrySourceType" maxOccurs="5">
				<xs:annotation>
					<xs:documentation xml:lang="en">This holds details regarding the requestor. It may be repeated to also accommodate the delivery systems.</xs:documentation>
				</xs:annotation>
				<!--			<xs:element name="Source" type="FerrySourceType" maxOccurs="5"> -->
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FerryTravellerCountBaseType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Restricts OTA TravelerCountType to Ferry requirements</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:restriction base="TravelerCountType">
				<xs:attribute name="CodeContext" use="prohibited">
					<xs:annotation>
						<xs:documentation xml:lang="en">Identifies the source authority for the code. </xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="StringLength1to32"/>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="URI" use="prohibited">
					<xs:annotation>
						<xs:documentation xml:lang="en">Identifies the location of the code table</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:anyURI"/>
					</xs:simpleType>
				</xs:attribute>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FerryTravellerCountType">
		<xs:annotation>
			<xs:documentation>Provides the number of travelers of a specific type (e.g.  Adult, Child).</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="FerryTravellerCountBaseType">
				<xs:attribute name="RPH" type="RPH_Type" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="en">A place holder to enable this occurrence to be referenced from another element. </xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ItineraryRPH" type="RPH_Type" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">Used to indicate on which leg(s) of the booking the passengers party is travelling.  If not supplied then 'all legs' is assumed.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="TravelStatus">
					<xs:annotation>
						<xs:documentation>Used to indicate the status of a travellers trip. It indicates if a passenger has completed a trip.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Berthed" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation>Used to indicate if the infant is in a Berth or not</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FerryErrorType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Standard way to indicate that an error occurred during the processing of an OTA message</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="ErrorType">
				<xs:attribute name="RPH" type="RPH_Type" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">In conjunction with the RPH_Context attribute, allows the source of the error to be identified.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="RPH_Context" type="xs:string" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="FerryPersonNameType">
		<xs:complexContent>
			<xs:restriction base="PersonNameType">
				<xs:sequence>
					<xs:element name="NamePrefix" type="StringLength1to16" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">Salutation of honorific. (e.g., Mr. Mrs., Ms., Miss, Dr.) </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GivenName" type="SimplifiedStringType">
						<xs:annotation>
							<xs:documentation xml:lang="en">Given name, first name or names</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MiddleName" type="SimplifiedStringType" minOccurs="0" maxOccurs="3">
						<xs:annotation>
							<xs:documentation xml:lang="en">The middle name of the person name</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Surname" type="SimplifiedStringType">
						<xs:annotation>
							<xs:documentation xml:lang="en">Family name, last name.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FerryPaymentCardBaseType">
		<xs:complexContent>
			<xs:restriction base="PaymentCardType">
				<xs:sequence>
					<xs:element name="CardHolderName" type="StringLength1to128" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="MaskedCardNumber" type="StringLength1to32" use="prohibited">
					<xs:annotation>
						<xs:documentation xml:lang="en">May be used to send a concealed credit card number (e.g., xxxxxxxxxxxx9922).</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:restriction>
