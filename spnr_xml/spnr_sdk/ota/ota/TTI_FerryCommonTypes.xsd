						<xs:sequence>
							<xs:element name="OwnInsuranceChoice" type="OwnInsuranceChoiceType" maxOccurs="9"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Insurance" type="TPA_Extensions_Type" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Details of the insurance provided by the booking supplier.</xs:documentation>
					</xs:annotation>
