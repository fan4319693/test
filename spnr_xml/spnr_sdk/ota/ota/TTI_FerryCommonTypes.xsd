			<xs:enumeration value="COM">
				<xs:annotation>
					<xs:documentation xml:lang="en">Motorcycle and sidecar combination </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CRB">
				<xs:annotation>
					<xs:documentation xml:lang="en">Car with rear-mounted bicycle carrier </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MBB">
				<xs:annotation>
					<xs:documentation xml:lang="en">Minibus with rear-mounted bicycle carrier </xs:documentation>
