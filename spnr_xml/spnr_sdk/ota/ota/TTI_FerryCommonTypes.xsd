					<xs:documentation xml:lang="en">Details of the vehicle and trailer (if any) on the itinerary. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="VehicleDetailsType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="PetCount" type="PetCountType" minOccurs="0" maxOccurs="4">
