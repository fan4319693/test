					<xs:documentation xml:lang="en">Camper with rear-mounted bicycle carrier </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CMP">
				<xs:annotation>
					<xs:documentation xml:lang="en">Camper </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="COA">
				<xs:annotation>
					<xs:documentation xml:lang="en">Coach </xs:documentation>
