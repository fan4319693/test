					<xs:documentation xml:lang="en">Caravan </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FVT">
				<xs:annotation>
					<xs:documentation xml:lang="en"> Freight vehicle trailer </xs:documentation>
